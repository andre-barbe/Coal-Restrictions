   1022    
   1023   Coefficient (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)(all,c,COL)(all,d,DIREC)
   1024       TRADE(i,r,s,c,d);
   1025   Formula (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   1026       TRADE(i,r,s,"welcnt","import") = TTAX(i,r,s,"mtax");
   1027   Formula (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   1028       TRADE(i,r,s,"welcnt","export") = TTAX(i,r,s,"xtax");
   1029   Formula (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   1030       TRADE(i,r,s,"dvol","import") = BVXMD(i,r,s) * qxs(i,r,s) / 100;
   1031   Formula (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   1032       TRADE(i,r,s,"dvol","export") = BVXMD(i,r,s) * qxs(i,r,s) / 100;
   1033   Formula (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   1034       TRADE(i,r,s,"taxrateb","import") = BIMTAX(i,r,s);
   1035   Formula (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   1036       TRADE(i,r,s,"taxrateu","import") = UIMTAX(i,r,s);
