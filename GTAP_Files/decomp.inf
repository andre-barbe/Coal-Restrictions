   1260   Coefficient (all,i,TRAD_COMM)(all,r,REG)(all,t,PRICES)(all,s,FORM)
   1261       TOT(i,r,t,s)
   1262       # terms trade effects decomposed by effects of different prices #;
   1263   Formula (all,i,TRAD_COMM)(all,r,REG)
   1264       TOT(i,r,"pworld","percent") = c1_ir(i,r);
   1265   Formula (all,i,TRAD_COMM)(all,r,REG)
   1266       TOT(i,r,"pexport","percent") = c2_ir(i,r);
   1267   Formula (all,i,TRAD_COMM)(all,r,REG)
   1268       TOT(i,r,"pimport","percent") = -c3_ir(i,r);
   1269   Formula (all,i,TRAD_COMM)(all,r,REG)
   1270       TOT(i,r,"pworld","value")
   1271           = CNTtotr(r) * c1_ir(i,r)
   1272           / sum(j,TRAD_COMM, sum(t,PRICES, TOT(j,r,t,"percent")));
   1273   Formula (all,i,TRAD_COMM)(all,r,REG)
   1274       TOT(i,r,"pexport","value")
   1275           = CNTtotr(r) * c2_ir(i,r)
   1276           / sum(t,PRICES, sum(j,TRAD_COMM, TOT(j,r,t,"percent")));
   1277   Formula (all,i,TRAD_COMM)(all,r,REG)
   1278       TOT(i,r,"pimport","value")
   1279           = -c3_ir(i,r) * CNTtotr(r)
   1280           / sum(t,PRICES, sum(j,TRAD_COMM, TOT(j,r,t,"percent")));
