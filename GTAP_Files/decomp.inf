   1038       TRADE(i,r,s,"taxrateb","export") = BEXTAX(i,r,s);
   1039   Formula (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   1040       TRADE(i,r,s,"taxrateu","export") = UEXTAX(i,r,s);
