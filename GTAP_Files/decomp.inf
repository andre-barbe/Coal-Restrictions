    501       A211: Output Tax Effect: Explanatory Factors
    502       ............................................
    503   >!
    504    
    505   Coefficient (all,i,ENDW_COMM)(all,j,PROD_COMM)(all,r,REG)
    506       BVFM(i,j,r) # producer expenditure on i by j in r at market prices #;
    507   Read
    508       BVFM from file GTAPBASE header "VFM";
    509    
    510   Coefficient (all,i,TRAD_COMM)(all,r,REG)
    511       BVDM(i,r) # domestic sales of i in r at market prices (tradeables only) #;
    512   Formula (all,i,TRAD_COMM)(all,r,REG)
    513       BVDM(i,r) = BVDPM(i,r) + BVDGM(i,r) + sum(j,PROD_COMM, BVDFM(i,j,r));
    514    
    515   Coefficient (all,m,MARG_COMM)(all,r,REG)
    516       BVST(m,r)
    517       # exports of m from r for int'l transport at mkt prices (tradeables only) #;
    518   Read
    519       BVST from file GTAPBASE header "VST";
    520    
    521   Coefficient (all,i,NSAV_COMM)(all,r,REG)
    522       BVOM(i,r) # value of commodity i output in region r at mkt prices #;
    523   Formula (all,i,ENDW_COMM)(all,r,REG)
    524       BVOM(i,r) = sum(j,PROD_COMM, BVFM(i,j,r));
    525   Formula (all,m,MARG_COMM)(all,r,REG)
    526       BVOM(m,r) = BVDM(m,r) + sum(s,REG, BVXMD(m,r,s)) + BVST(m,r);
    527   Formula (all,i,NMRG_COMM)(all,r,REG)
    528       BVOM(i,r) = BVDM(i,r) + sum(s,REG, BVXMD(i,r,s));
    529   Formula (all,h,CGDS_COMM)(all,r,REG)
    530       BVOM(h,r) = BVOA(h,r);
    531    
    532   Coefficient (all,i,NSAV_COMM)(all,r,REG)
    533       qo(i,r) # industry output of commodity i in region r #;
    534   Read
    535       qo from file GTAPSOL header "qo";
    536    
    537   Zerodivide (nonzero_by_zero) default 0;
    538    
    539   Coefficient (all,i,NSAV_COMM)(all,r,REG)
    540       BOUTAX(i,r);
    541   Formula (all,i,NSAV_COMM)(all,r,REG)
    542       BOUTAX(i,r) = [BVOM(i,r) - BVOA(i,r)] / BVOM(i,r) * 100;
    543    
    544   Zerodivide (nonzero_by_zero) off;
    545    
    546   Coefficient (all,i,ENDW_COMM)(all,j,PROD_COMM)(all,r,REG)
    547       UVFM(i,j,r) # producer expenditure on i by j in r at market prices #;
    548   Read
    549       UVFM from file GTAPUPDATE header "VFM";
    550    
    551   Coefficient (all,i,TRAD_COMM)(all,r,REG)
    552       UVDM(i,r) # domestic sales of i in r at market prices (tradeables only) #;
    553   Formula (all,i,TRAD_COMM)(all,r,REG)
    554       UVDM(i,r) = UVDPM(i,r) + UVDGM(i,r) + sum(j,PROD_COMM, UVDFM(i,j,r));
    555    
    556   Coefficient (all,m,MARG_COMM)(all,r,REG)
    557       UVST(m,r)
    558       # exports of m from r for int'l transport at mkt prices (tradeables only) #;
    559   Read
    560       UVST from file GTAPUPDATE header "VST";
    561    
    562   Coefficient (all,i,ENDW_COMM)(all,r,REG)
    563       UEVOA(i,r) # value of output at agent's prices #;
    564   Read
    565       UEVOA from file GTAPUPDATE header "EVOA";
    566    
    567   Coefficient (all,i,ENDW_COMM)(all,j,PROD_COMM)(all,r,REG)
    568       UEVFA(i,j,r) # producer expenditure on i by industry j #;
    569   Read
    570       UEVFA from file GTAPUPDATE header "EVFA";
    571    
    572   Coefficient (all,i,NSAV_COMM)(all,r,REG)
    573       UVOA(i,r) # value of commodity i output in region r #;
    574   Formula (all,i,ENDW_COMM)(all,r,REG)
    575       UVOA(i,r) = UEVOA(i,r);
    576   Formula (all,i,PROD_COMM)(all,r,REG)
    577       UVOA(i,r)
    578           = sum(j,ENDW_COMM, UEVFA(j,i,r))
    579           + sum(j,TRAD_COMM, UVIFA(j,i,r) + UVDFA(j,i,r));
    580    
    581   Coefficient (all,i,NSAV_COMM)(all,r,REG)
    582       UVOM(i,r) # value of commodity i output in region r at mkt prices #;
    583   Formula (all,i,ENDW_COMM)(all,r,REG)
    584       UVOM(i,r) = sum(j,PROD_COMM, UVFM(i,j,r));
    585   Formula (all,m,MARG_COMM)(all,r,REG)
    586       UVOM(m,r) = UVDM(m,r) + sum(s,REG, UVXMD(m,r,s)) + UVST(m,r);
    587   Formula (all,i,NMRG_COMM)(all,r,REG)
    588       UVOM(i,r) = UVDM(i,r) + sum(s,REG, UVXMD(i,r,s));
    589   Formula (all,h,CGDS_COMM)(all,r,REG)
    590       UVOM(h,r) = UVOA(h,r);
    591    
    592   Zerodivide (nonzero_by_zero) default 0;
    593    
    594   Coefficient (all,i,NSAV_COMM)(all,r,REG)
    595       UOUTAX(i,r);
    596   Formula (all,i,NSAV_COMM)(all,r,REG)
    597       UOUTAX(i,r) = [UVOM(i,r) - UVOA(i,r)] / UVOM(i,r) * 100;
    598    
    599   Zerodivide (nonzero_by_zero) off;
    600    
    601   Coefficient (all,i,NSAV_COMM)(all,r,REG)(all,c,COL)
    602       OUTPUT(i,r,c);
    603   Formula (all,i,NSAV_COMM)(all,r,REG)
    604       OUTPUT(i,r,"welcnt") = OTAX(i,r);
    605   Formula (all,i,NSAV_COMM)(all,r,REG)
    606       OUTPUT(i,r,"dvol") = BVOM(i,r) * qo(i,r) / 100;
    607   Formula (all,i,NSAV_COMM)(all,r,REG)
    608       OUTPUT(i,r,"taxrateb") = BOUTAX(i,r);
    609   Formula (all,i,NSAV_COMM)(all,r,REG)
    610       OUTPUT(i,r,"taxrateu") = UOUTAX(i,r);
    611    
    612   Write
    613       OUTPUT to file WELVIEW header "A211"
    614       longname "Output Tax Effect: Explanatory Factors";
