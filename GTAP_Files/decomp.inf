   1014       UEXTAX(i,r,s);
   1015   Formula (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   1016       UEXTAX(i,r,s) = [UVXWD(i,r,s) - UVXMD(i,r,s)] / UVXWD(i,r,s) * 100;
   1017    
   1018   Coefficient (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   1019       qxs(i,r,s) # export sales of commodity i from r to region s #;
   1020   Read
