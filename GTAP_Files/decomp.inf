    163       Note: A common coefficient is the one used across several modules.
    164   >!
    165    
    166   Coefficient (all,i,ENDW_COMM)(all,r,REG)
    167       BEVOA(i,r) # value of output at agent's prices #;
    168   Read
    169       BEVOA from file GTAPBASE header "EVOA";
    170   Coefficient (all,i,ENDW_COMM)(all,j,PROD_COMM)(all,r,REG)
    171       BEVFA(i,j,r) # producer expenditure on i by industry j #;
    172   Read
    173       BEVFA from file GTAPBASE header "EVFA";
    174   Coefficient (all,i,TRAD_COMM)(all,j,PROD_COMM)(all,r,REG)
    175       BVIFA(i,j,r) # purchases of imported i r for use in j in region r #;
    176   Read
    177       BVIFA from file GTAPBASE header "VIFA";
    178   Coefficient (all,i,TRAD_COMM)(all,j,PROD_COMM)(all,r,REG)
    179       BVDFA(i,j,r) # purchases of domestic i r for use in j in region r #;
    180   Read
    181       BVDFA from file GTAPBASE header "VDFA";
    182    
    183   Coefficient (all,i,NSAV_COMM)(all,r,REG)
    184       BVOA(i,r) # value of commodity i output in region r #;
    185   Formula (all,i,ENDW_COMM)(all,r,REG)
    186       BVOA(i,r) = BEVOA(i,r);
    187   Formula (all,i,PROD_COMM)(all,r,REG)
    188       BVOA(i,r)
    189           = sum(j,ENDW_COMM, BEVFA(j,i,r))
    190           + sum(j,TRAD_COMM, BVIFA(j,i,r) + BVDFA(j,i,r));
