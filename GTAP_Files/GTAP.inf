   1071   Coefficient (all,i,ENDW_COMM)(all,j,PROD_COMM)(all,r,REG)
   1072       ETAX(i,j,r) # tax on use of endowment good i by industry j in region r #;
   1073   Formula (all,i,ENDW_COMM)(all,j,PROD_COMM)(all,r,REG)
