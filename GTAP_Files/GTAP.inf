   2272    
   2273   !Commented out by Andre Barbe!
   2274   !Equation VAENFDEMAND
   2275   # demand for inputs into value-added-energy subproduction #
   2276   (all,i,VAEN_FCOMM)(all,j,PROD_COMM)(all,r,REG)
   2277       qf(i,j,r) = -af(i,j,r) + qf("vaen",j,r)
   2278           - ELFVAEN(j,r) * [pf(i,j,r) - af(i,j,r) - pf("vaen",j,r)];!
   2279    
   2280    
   2281   !<
   2282       3-10. Top of the Production Structure
   2283       -------------------------------------
   2284   >!
   2285    
   2286   Set NENY_FCOMM # inputs into non-energy subproduction # = TRAD_COMM - EGY_COMM;
   2287    
   2288   Set VAEN # value-added-energy subproduction # (vaen);
   2289    
   2290   Set
   2291       OUT_COMM # inputs into top of production structure # = NENY_FCOMM + VAEN;
   2292    
   2293   Subset
   2294       OUT_COMM is subset of FIRM_COMM;
   2295    
   2296   Variable (all,j,PROD_COMM)(all,r,REG)
   2297       ao(j,r) # output augmenting technical change in sector j of r #;
   2298   Variable (all,j,PROD_COMM)
   2299       aosec(j) # output augmenting tech change of sector j, worldwide #;
   2300   Variable (all,r,REG)
   2301       aoreg(r) # output augmetning tech change in region r #;
   2302   Variable (all,j,PROD_COMM)(all,r,REG)
   2303       aoall(j,r) # output augmenting technical change in sector j of r #;
   2304   Equation AOWORLD
   2305   # sector/region specific average rate of output augmenting tech change #
   2306   (all,j,PROD_COMM)(all,r,REG)
   2307       ao(j,r) = aosec(j) + aoreg(r) + aoall(j,r);
   2308    
   2309   Coefficient (Parameter)(all,j,PROD_COMM)
   2310       ESUBT(j) # elasticity of substitution at top of production structure #;
   2311   Read
   2312       ESUBT from file GTAPPARM header "ESBT";
   2313    
   2314   Variable (all,j,PROD_COMM)(all,r,REG)
   2315       profitslack(j,r) # slack variable in the zero profit equation #;
   2316   !<
   2317       This is exogenous, unless the user wishes to specify output in a given
   2318       region exogenously.
   2319   >!
   2320    
   2321   Coefficient (all,j,PROD_COMM)(all,r,REG)
   2322       VFATOT(j,r) # total cost of inputs into j in r #;
   2323   Formula (all,j,PROD_COMM)(all,r,REG)
   2324       VFATOT(j,r) = sum(i,OUT_COMM, VFA(i,j,r));
   2325    
   2326   Coefficient
   2327       FSIZE_OUT # size of OUT_COMM set #;
   2328   Formula
   2329       FSIZE_OUT = sum(i,OUT_COMM, 1);
   2330    
   2331   Coefficient (all,i,OUT_COMM)(all,j,PROD_COMM)(all,r,REG)
   2332       FSHOUT(i,j,r) # share of i in total costs of j in r #;
   2333   Formula (all,i,OUT_COMM)(all,j,PROD_COMM)(all,r,REG: VFATOT(j,r) = 0.0)
   2334       FSHOUT(i,j,r) = 1.0 / FSIZE_OUT;
   2335   Formula (all,i,OUT_COMM)(all,j,PROD_COMM)(all,r,REG: VFATOT(j,r) > 0.0)
   2336       FSHOUT(i,j,r) = VFA(i,j,r) / VFATOT(j,r);
   2337    
   2338   Equation ZEROPROFITS # industry zero pure profits condition #
   2339   (all,j,PROD_COMM)(all,r,REG)
   2340       ps(j,r) + ao(j,r)
   2341           = sum(i,OUT_COMM, FSHOUT(i,j,r) * [pf(i,j,r) - af(i,j,r)])
   2342               + profitslack(j,r);
   2343   !<
   2344       Industry zero pure profits condition (HT 6).  This condition permits us to
   2345       determine the endogenous output level for each of the non-endowment
   2346       sectors, excepting when profitslack is itself endogenous.  The level of
   2347       activity in the endowment sectors is exogenously determined.
   2348   >!
   2349    
   2350   Equation OUTFDEMAND
   2351   # sector demands for factor-energy subproduct and intermediate inputs #
   2352   (all,i,OUT_COMM)(all,j,PROD_COMM)(all,r,REG)
   2353       qf(i,j,r)= -af(i,j,r) + qo(j,r) - ao(j,r)
   2354           - ESUBT(j) * [pf(i,j,r) - af(i,j,r) - ps(j,r) - ao(j,r)];
   2355    
   2356    
   2357   !<
   2358       3-11. Output Taxation
   2359       ---------------------
   2360   >!
   2361    
   2362   Equation OUTPUTPRICES
   2363   # Eq'n links pre- and post-tax supply prices for all industries.  (HT 15) #
   2364   (all,i,PROD_COMM)(all,r,REG)
   2365       ps(i,r) = to(i,r) + pm(i,r);
   2366   !<
   2367       This equation links pre- and post-tax supply prices for all industries.
   2368       This captures the effect of output taxes.  TO(i,r) < 1 in the case of a
   2369       tax.  (HT 15)
   2370   >!
   2371    
   2372   Equation TOUTRATIO
   2373   # change in ratio of output tax payments to regional income #
   2374   (all,r,REG)
   2375       100.0 * INCOME(r) * del_taxrout(r) + TOUT(r) * y(r)
   2376           = sum(i,PROD_COMM,
   2377               VOA(i,r) * [-to(i,r)] + PTAX(i,r) * [pm(i,r) + qo(i,r)]);
   2378    
   2379    
   2380   !<
   2381       ---------------------------------------
   2382       4. Investment, Global Bank, and Savings
   2383       ---------------------------------------
   2384    
   2385       Capital stock and rate of return equations follow.  They correspond to the
   2386       Investment Equations of Table 8 in Hertel and Tsigas.
   2387    
   2388       4-0. Module-Specific Variables
   2389       4-1. Equations of Notational Convenience
   2390       4-2. Rate of Return Equations
   2391       4-3. Capital Accumulation
   2392       4-4. Global Bank
   2393       4-5. Price Index of Aggregate Global Composite Capital Goods
   2394       4-6. Price of Saving
   2395   >!
   2396    
   2397    
   2398   !<
   2399       4-0. Module-Specific Variables
   2400       ------------------------------
   2401    
   2402       only used in this Investment, Global Bank and Savings module
   2403   >!
   2404    
   2405   Variable (all,r,REG)
   2406       rental(r) # rental rate on capital = ps("capital",r) #;
   2407   Variable (all,r,REG)
   2408       ke(r) # end-of-period capital stock in r #;
   2409   Variable (all,r,REG)
   2410       rore(r) # expected net rate of return on capital stock in r #;
   2411   Variable (all,r,REG)
   2412       rorc(r) # current net rate of return on capital stock in r #;
   2413   Variable (all,r,REG)
   2414       qcgds(r) # output of capital goods sector = qo("cgds",r) #;
   2415    
   2416    
   2417   !<
   2418       4-1. Equations of Notational Convenience
   2419       ----------------------------------------
   2420   >!
   2421    
   2422   Variable (all,r,REG)
   2423       ksvces(r) # capital services = qo("capital",r) #;
   2424   Equation KAPSVCES
   2425   # eq'n defines a variable for capital services (HT 52) #
   2426   (all,r,REG)
   2427       ksvces(r)
   2428           = sum(h,ENDWC_COMM, [VOA(h,r) / sum(k,ENDWC_COMM, VOA(k,r))] * qo(h,r));
   2429   !<
   2430       This equation defines a variable for capital services, for convenience.
   2431       (There is really only one capital services item.) (HT 52)
   2432   >!
   2433    
   2434   Equation KAPRENTAL
   2435   # eq'n defines a variable for capital rental rate (HT 53) #
   2436   (all,r,REG)
   2437       rental(r)
   2438           = sum(h,ENDWC_COMM, [VOA(h,r) / sum(k,ENDWC_COMM, VOA(k,r))] * ps(h,r));
   2439    
   2440   Equation CAPGOODS
   2441   # eq'n defines a variable for gross investment (HT 54) #
   2442   (all,r,REG)
   2443       qcgds(r)
   2444           = sum(h,CGDS_COMM, [VOA(h,r) / REGINV(r)] * qo(h,r));
   2445   !<
   2446       This equation defines a variable for gross investment, for convenience.
   2447       There is really only one capital goods item.
   2448   >!
   2449    
   2450   Equation PRCGOODS
   2451   # eq'n defines the price of cgds (HT 55) #
   2452   (all,r,REG)
   2453       pcgds(r) = sum(h,CGDS_COMM, [VOA(h,r) / REGINV(r)] * ps(h,r));
   2454    
   2455   Equation KBEGINNING
   2456   # associates change in cap. services w/ change in cap. stock (HT 56) #
   2457   (all,r,REG)
   2458       kb(r) = ksvces(r);
   2459   !<
   2460       This equation associates any change in capital services during the period
   2461       with a change in capital stock.  Full capacity utilization is assumed.
   2462   >!
   2463    
   2464   Coefficient (ge 0)(all,r,REG)
   2465       VKB(r) # value of beginning-of-period capital stock in region r #;
   2466   Update (all,r,REG)
   2467       VKB(r) = pcgds(r) * kb(r);
   2468   Read
   2469       VKB from file GTAPDATA header "VKB";
   2470   !<
   2471       VKB(r) is used only in this sub-module, so its data are read here.  The
   2472       associated variables, pcgds(r) and kb(r), are used across modules, so they
   2473       have been defined as common variables.
   2474   >!
   2475    
   2476   Coefficient (all,r,REG)
   2477       INVKERATIO(r)
   2478       # ratio of gross investment to end-of-period capital stock in r #;
   2479   Formula (all,r,REG)
   2480       INVKERATIO(r) = REGINV(r) / [VKB(r) + NETINV(r)];
   2481    
   2482   Equation KEND
   2483   # Ending capital stock equals beginning stock plus net investment.  (HT 10) #
   2484   (all,r,REG)
   2485       ke(r) = INVKERATIO(r) * qcgds(r) + [1.0 - INVKERATIO(r)] * kb(r);
   2486    
   2487    
   2488   !<
   2489       4-2. Rate of Return Equations
   2490       -----------------------------
   2491   >!
   2492    
   2493   Coefficient (all,r,REG)
   2494       GRNETRATIO(r) # ratio of GROSS/NET rates of return on capital in r #;
   2495   Formula (all,r,REG)
   2496       GRNETRATIO(r)
   2497           = sum(h,ENDWC_COMM, VOA(h,r))
   2498           / [sum(h,ENDWC_COMM, VOA(h,r)) - VDEP(r)];
   2499   !<
   2500       NOTE: VOA("capital",r) is GROSS returns to capital.
   2501   >!
   2502   Equation RORCURRENT
   2503   # current rate of return on capital in region r (HT 57) #
   2504   (all,r,REG)
   2505       rorc(r) = GRNETRATIO(r) * [rental(r) - pcgds(r)];
   2506    
   2507   Coefficient (parameter)(all,r,REG)
   2508       RORFLEX(r)
   2509       # flexibility of expected net ROR on capital stock in r wrt investment #;
   2510   Read
   2511       RORFLEX from file GTAPPARM header "RFLX";
   2512   !<
   2513       RORFLEX is the flexibility of the expected net rate of return on the
   2514       capital stock, in region r, with respect to investment.  If a region's
   2515       capital stock increases by 1%, then it is expected that the net rate of
   2516       return on capital will decline by RORFLEX %.  A larger absolute value for
   2517       this parameter will reduce the tendency of international investment flows
   2518       to respond to changes in expected rates of return.
   2519   >!
   2520    
   2521   Equation ROREXPECTED
   2522   # expected rate of return depends on the current return and investment (HT 58) #
   2523   (all,r,REG)
   2524       rore(r) = rorc(r) - RORFLEX(r) * [ke(r) - kb(r)];
   2525    
   2526    
   2527   !<
   2528       4-3. Capital Accumulation
   2529       -------------------------
   2530    
   2531       based on the theory laid out in GTAP Technical Paper #7
   2532   >!
   2533    
   2534   Variable (all,i,ENDWC_COMM)(all,r,REG)
   2535       EXPAND(i,r) # change in investment levels relative to endowment stock #;
   2536   Equation BALDWIN
   2537   # change in investment levels relative to endowment stock #
   2538   (all,i,ENDWC_COMM)(all,r,REG)
   2539       EXPAND(i,r) = qcgds(r) - qo(i,r);
   2540    
   2541    
   2542   !<
   2543       4-4. Global Bank
   2544       ----------------
   2545   >!
   2546    
   2547   Coefficient (integer,parameter)
   2548       RORDELTA
   2549       # binary coefficient to switch mechanism of allocating investment funds #;
   2550   Read
   2551       RORDELTA from file GTAPPARM header "RDLT";
   2552   !<
   2553       RORDELTA is a binary coefficient which determines the mechanism of
   2554       allocating investment funds across regions.  When RORDELTA = 1, investment
   2555       funds are allocated across regions to equate the change in the expected
   2556       rates of return (i.e., rore(r)).  When RORDELTA = 0, investment funds are
   2557       allocated across regions to maintain the existing composition of capital
   2558       stocks.
   2559   >!
   2560    
   2561   Variable
   2562       rorg # global net rate of return on capital stock #;
   2563   Variable (all,r,REG)
   2564       cgdslack(r) # slack variable for qcgds(r) #;
   2565   !<
   2566       This is exogenous, unless the user wishes to exogenously fix the level of
   2567       gross investment in a region, in which case it can be swapped with
   2568       cgdslack.
   2569   >!
   2570   Equation RORGLOBAL
   2571   # either gross investment or expected rate of return in region r (HT 59) #
   2572   (all,r,REG)
   2573       RORDELTA * rore(r)
   2574       + [1 - RORDELTA]
   2575       * [[REGINV(r) / NETINV(r)] * qcgds(r) - [VDEP(r) / NETINV(r)] * kb(r)]
   2576           = RORDELTA * rorg + [1 - RORDELTA] * globalcgds + cgdslack(r);
   2577   !<
   2578       This equation determines either gross investment or the expected rate of
   2579       return in each region, depending on the setting for the binary RORDELTA
   2580       parameter.
   2581   >!
   2582    
   2583   Equation GLOBALINV
   2584   # either expected global rate of return or global net investment (HT 11) #
   2585       RORDELTA * globalcgds + [1 - RORDELTA] * rorg
   2586           = RORDELTA
   2587           * sum(r,REG,
   2588               [REGINV(r) / GLOBINV] * qcgds(r) - [VDEP(r) / GLOBINV] * kb(r))
   2589           + [1 - RORDELTA] * sum(r,REG, [NETINV(r) / GLOBINV] * rore(r));
   2590   !<
   2591       This equation computes either the change in global net investment (when
   2592       RORDELTA = 1), or the change in the expected global rate of return on
   2593       capital (when RORDELTA = 0).
   2594   >!
   2595    
   2596    
   2597   !<
   2598       4-5. Price Index of Aggregate Global Composite Capital Goods
   2599       ------------------------------------------------------------
   2600   >!
   2601    
   2602   Equation PRICGDS
   2603   # eq'n generates a price index for the aggregate global cgds composite (HT 60) #
   2604       pcgdswld = sum(r,REG, [NETINV(r) / GLOBINV] * pcgds(r));
   2605    
   2606    
   2607   !<
   2608       4-6. Price of Saving
   2609       --------------------
   2610   >!
   2611    
   2612   Variable (all,r,REG)
   2613       psaveslack(r) # slack variable for the savings price equation #;
   2614   !<
   2615       This is exogenous under the normal closure.  However, in order to
   2616       replicate simulations from the GTAP book, where there was a single savings
   2617       price, psaveslack may be swapped with psave in order to fix the latter.
   2618       In this case, pcgdswld must be the numeraire so that each region faces the
   2619       same price of savings.
   2620   >!
   2621    
   2622   Equation SAVEPRICE
   2623   # savings price #
   2624   (all,r,REG)
   2625       psave(r)
   2626           = pcgds(r)
   2627           + sum(s,REG, [[NETINV(s) - SAVE(s)] / GLOBINV] * pcgds(s))
   2628           + psaveslack(r);
   2629   !<
   2630       In contrast to the GTAP book, the price of savings is now region-specific
   2631       and it changes at the same rate as the price of regional investment, plus
   2632       an adjustment factor which accounts for the fact that savings and
   2633       investment are not equal at the regional level.
   2634   >!
   2635    
   2636    
   2637   !<
   2638       ----------------------
   2639       5. International Trade
   2640       ----------------------
   2641    
   2642       5-1. Export Prices
   2643       5-2. Demand for Imports
   2644   >!
   2645    
   2646    
   2647   !<
   2648       5-1. Export Prices
   2649       ------------------
   2650   >!
   2651    
   2652   Variable (all,i,TRAD_COMM)(all,r,REG)
   2653       tx(i,r) # dest.-gen. change in subsidy on exports of i from r #;
   2654   Variable (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   2655       txs(i,r,s) # dest.-spec. change in subsidy on exports of i from r to s #;
   2656   !<
   2657       The variable txs captures changes in the power of bilateral export taxes.
   2658       However, the presence of a destination-generic export subsidy shift (tx)
   2659       also permits the user to swap a single export tax shock with another
   2660       target variable.  It is most naturally swapped with the variable qo to
   2661       insulate domestic producers from the world market.
   2662   >!
   2663    
   2664   Equation EXPRICES
   2665   # eq'n links agent's and world prices (HT 27) #
   2666   (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   2667       pfob(i,r,s) = pm(i,r) - tx(i,r) - txs(i,r,s);
   2668    
   2669   Equation TEXPRATIO
   2670   # change in ratio of export tax payments to regional income #
   2671   (all,r,REG)
   2672       100.0 * INCOME(r) * del_taxrexp(r) + TEX(r) * y(r)
   2673           = sum(i,TRAD_COMM, sum(s,REG,
   2674               VXMD(i,r,s) * [-tx(i,r) - txs(i,r,s)]
   2675               + XTAXD(i,r,s) * [pfob(i,r,s) + qxs(i,r,s)]));
   2676    
   2677    
   2678   !<
   2679       5-2. Demand for Imports
   2680       -----------------------
   2681    
   2682       Composite Imports Nest: Table 3 of Hertel and Tsigas
   2683   >!
   2684    
   2685   Variable (all,i,TRAD_COMM)(all,s,REG)
   2686       tm(i,s) # source-gen. change in tax on imports of i into s #;
   2687   Variable (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   2688       tms(i,r,s) # source-spec. change in tax on imports of i from r into s #;
   2689   !<
   2690       The variable tms captures changes in the power of bilateral import taxes.
   2691       However, the presence of a source-generic import tariff shift (tm) also
   2692       permits the user to swap a single import tariff shock with another target
   2693       variable.  In particular, to insulate domestic producers from import price
   2694       changes, it may be swapped with the relative price variable pr -- see
   2695       below.
   2696   >!
   2697    
   2698   Equation MKTPRICES
   2699   # eq'n links domestic and world prices (HT 24) #
   2700   (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   2701       pms(i,r,s) = tm(i,s) + tms(i,r,s) + pcif(i,r,s);
   2702    
   2703   Coefficient (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   2704       MSHRS(i,r,s) # share of imports from r in import bill of s at mkt prices #;
   2705   Formula (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   2706       MSHRS(i,r,s) = VIMS(i,r,s) / sum(k,REG, VIMS(i,k,s));
   2707   Equation DPRICEIMP
   2708   # price for aggregate imports (HT 28) #
   2709   (all,i,TRAD_COMM)(all,s,REG)
   2710       pim(i,s) = sum(k,REG, MSHRS(i,k,s) * [pms(i,k,s) - ams(i,k,s)]);
   2711    
   2712   Variable (orig_level=1.0)(all,i,TRAD_COMM)(all,r,REG)
   2713       pr(i,r) # ratio of domestic to imported prices in r #;
   2714   Equation PRICETGT
   2715   # eq'n defines target price ratio to be attained via the variable levy (HT 25) #
   2716   (all,i,TRAD_COMM)(all,s,REG)
   2717       pr(i,s) = pm(i,s) - pim(i,s);
   2718   !<
   2719       This equation defines the target price ratio to be attained via the
   2720       variable levy.  This price ratio is the ratio of domestic to average
   2721       imported goods' price.  Note that the way this price ratio is defined, it
   2722       includes intraregional imports as well.  In most applications, regions
   2723       will represent groups of individual countries.  However, in the case of
   2724       the EU, this is problematic, since recent versions of the database have
   2725       incorporated intra-EU trade flows.  Therefore, when aggregated to the EU
   2726       level, the composite import price includes both intra-EU and outside
   2727       imports.  So some modification is needed to handle the EU case.
   2728   >!
   2729    
   2730   Coefficient (parameter)(all,i,TRAD_COMM)
   2731       ESUBM(i)
   2732       # region-generic el. of sub. among imports of i in Armington structure #;
   2733   Read
   2734       ESUBM from file GTAPPARM header "ESBM";
   2735    
   2736   Equation IMPORTDEMAND
   2737   # regional demand for disaggregated imported commodities by source (HT 29)  #
   2738   (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   2739       qxs(i,r,s)
   2740           = -ams(i,r,s) + qim(i,s)
   2741           - ESUBM(i) * [pms(i,r,s) - ams(i,r,s) - pim(i,s)];
   2742    
   2743   Equation TIMPRATIO
   2744   # change in ratio of import tax payments to regional income #
   2745   (all,r,REG)
   2746       100.0 * INCOME(r) * del_taxrimp(r) + TIM(r) * y(r)
   2747           = sum(i,TRAD_COMM, sum(s,REG,
   2748               VIMS(i,s,r) * [tm(i,r) + tms(i,s,r)]
   2749               + MTAX(i,s,r) * [pcif(i,s,r) + qxs(i,s,r)]));
   2750    
   2751    
   2752   !<
   2753       -----------------------------------
   2754       6. International Transport Services
   2755       -----------------------------------
   2756    
   2757       6-0. Module-Specific Variables and Coefficients
   2758       6-1. Demand for Global Transport Services
   2759       6-2. Supply of Transport Services
   2760   >!
   2761    
   2762    
   2763   !<
   2764       6-0. Module-Specific Variables and Coefficients
   2765       -----------------------------------------------
   2766    
   2767       only used in this International Transport Services module
   2768   >!
   2769    
   2770   Variable (all,m,MARG_COMM)(all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   2771       qtmfsd(m,i,r,s) # international usage margin m on i from r to s #;
   2772   !<
   2773       International margin usage, by Margin, Freight, Source, and Destination,
   2774       i.e., the percent change in usage of m in transport of i from r to s.
   2775   >!
   2776   Variable (all,m,MARG_COMM)(all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   2777       atmfsd(m,i,r,s) # tech change in m's shipping of i from region r to s #;
   2778   !<
   2779       Technical progress in shipping by Margin, Freight, Source, and
   2780       Destination.  This is endogenous and driven by the following mode-,
   2781       product-, source-, and destination-specific determinants.
   2782   >!
   2783   Variable (all,m,TRAD_COMM)
   2784       atm(m) # tech change in mode m, worldwide #;
   2785   Variable (all,i,TRAD_COMM)
   2786       atf(i) # tech change shipping of i, worldwide #;
   2787   Variable (all,r,REG)
   2788       ats(r) # tech change shipping from region r #;
   2789   Variable (all,s,REG)
   2790       atd(s) # tech change shipping to s #;
   2791   Variable (all,m,MARG_COMM)(all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   2792       atall(m,i,r,s) # tech change in m's shipping of i from region r to s #;
   2793   Variable (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   2794       ptrans(i,r,s) # cost index for international transport of i from r to s #;
   2795   !<
   2796       average cost index for margin services used in getting i from r to s
   2797   >!
   2798   Variable (all,m,MARG_COMM)
   2799       qtm(m) # global margin usage #;
   2800   Variable (all,m,MARG_COMM)
   2801       pt(m) # price of composite margins services, type #;
   2802   !<
   2803       price index for commodity m in margin services usage
   2804   >!
   2805    
   2806   Coefficient (ge 0) (all,m,MARG_COMM)(all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   2807       VTMFSD(m,i,r,s)
   2808       # int'l margin usage, by margin, freight, source, and destination #;
   2809   Update (all,m,MARG_COMM)(all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   2810       VTMFSD(m,i,r,s) = pt(m) * qtmfsd(m,i,r,s);
   2811   Read
   2812       VTMFSD from file GTAPDATA header "VTWR";
   2813    
   2814   Coefficient (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   2815       VTFSD(i,r,s) # aggregate value of svces in the shipment of i from r to s #;
   2816   Formula (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   2817       VTFSD(i,r,s) = sum(m,MARG_COMM, VTMFSD(m,i,r,s));
   2818   !<
   2819       In a balanced data base, (all,i,TRAD_COMM)(all,r,REG)(all,s,REG),
   2820       VIWS(i,r,s) = VXWD(i,r,s) + VTFSD(i,r,s).
   2821   >!
   2822    
   2823   Coefficient (all,m,MARG_COMM)
   2824       VTMUSE(m) # international margin services usage, by type #;
   2825   Formula (all,m,MARG_COMM)
   2826       VTMUSE(m) = sum(i,TRAD_COMM, sum(r,REG, sum(s,REG, VTMFSD(m,i,r,s))));
   2827    
   2828   Coefficient (all,m,MARG_COMM)
   2829       VTMPROV(m) # international margin services provision #;
   2830   Formula (all,m,MARG_COMM)
   2831       VTMPROV(m) = sum(r,REG, VST(m,r));
   2832   !<
   2833       In a balanced data base, VTMPROV = VTMUSE.
   2834   >!
   2835    
   2836   Coefficient (all,r,REG)
   2837       VTRPROV(r) # international margin supply, by region #;
   2838   Formula (all,r,REG)
   2839       VTRPROV(r) = sum(m,MARG_COMM, VST(m,r));
   2840    
   2841   Coefficient
   2842       VT # international margin supply #;
   2843   Formula
   2844       VT = sum(m,MARG_COMM, sum(r,REG, VST(m,r)));
   2845    
   2846    
   2847   !<
   2848       6-1. Demand for Global Transport Services
   2849       -----------------------------------------
   2850   >!
   2851    
   2852   Equation QTRANS_MFSD
   2853   # bilateral demand for transport services  #
   2854   (all,m,MARG_COMM)(all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   2855       qtmfsd(m,i,r,s) = qxs(i,r,s) - atmfsd(m,i,r,s);
   2856   !<
   2857       This equation computes the bilateral demand for international
   2858       transportation services.  It reflects the fact that the demand for
   2859       services along any particular route is proportional to the quantity of
   2860       merchandise shipped [i.e., QXS(i,r,s)].  It is here that we introduce the
   2861       potential for input-augmenting tech change, atmfsd(m,i,r,s), which is
   2862       commodity- and route-specific.  Thus, in the levels:
   2863               ATMFSD(m,i,r,s) * QTMFSD(m,i,r,s) = QXS(i,r,s)
   2864       where QTMFSD is the amount of composite margins services m used along this
   2865       route.  Technological improvements are reflected by atmfsd(i,r,s) > 0, and
   2866       these reduce the margins services required for this i,r,s triplet.  Tech.
   2867       change also dampens the cost of shipping, thereby lowering the CIF price
   2868       implied by a given FOB value (see 6-2).
   2869   >!
   2870    
   2871   Coefficient (all,m,MARG_COMM)(all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   2872       VTMUSESHR(m,i,r,s) # share of i,r,s usage in global demand for m #;
   2873   Formula (all,m,MARG_COMM)(all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   2874       VTMUSESHR(m,i,r,s) = VTFSD(i,r,s) / VT;
   2875   Formula
   2876       (all,m,MARG_COMM: VTMUSE(m) <> 0.0)(all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   2877       VTMUSESHR(m,i,r,s) = VTMFSD(m,i,r,s) / VTMUSE(m);
   2878    
   2879   Equation TRANS_DEMAND
   2880   # global demand for margin m #
   2881   (all,m,MARG_COMM)
   2882       qtm(m)
   2883           = sum(i,TRAD_COMM, sum(r,REG, sum(s,REG,
   2884             VTMUSESHR(m,i,r,s) * qtmfsd(m,i,r,s))));
   2885    
   2886    
   2887   !<
   2888       6-2. Supply of Transport Services
   2889       ---------------------------------
   2890   >!
   2891    
   2892   Coefficient (all,m,MARG_COMM)(all,r,REG)
   2893       VTSUPPSHR(m,r) # share of region r in global supply of margin m #;
   2894   Formula (all,m,MARG_COMM)(all,r,REG)
   2895       VTSUPPSHR(m,r) = VTRPROV(r) / VT;
   2896   Formula (all,m,MARG_COMM: VTMPROV(m) <> 0.0)(all,r,REG)
   2897       VTSUPPSHR(m,r) = VST(m,r) / VTMPROV(m);
   2898    
   2899   Equation PTRANSPORT
   2900   # generate price index for composite transportation services #
   2901   (all,m,MARG_COMM)
   2902       pt(m) = sum(r,REG, VTSUPPSHR(m,r) * pm(m,r));
   2903   !<
   2904       This equation generates a price index for transportation services based on
   2905       zero profits.  NOTE:
   2906       (1) Sales to international transportation are not subject to export tax.
   2907           This is why we base the costs to the transport sector on market prices
   2908           of the goods sold to international transportation.
   2909       (2) We assume that the supply shares for margin services are uniform
   2910           across freight, source of freight, and destination.
   2911       (cf. HT#7)
   2912   >!
   2913    
   2914   Coefficient
   2915       VTUSE # international margin services usage #;
   2916   Formula
   2917       VTUSE
   2918           = sum(m,MARG_COMM, sum(i,TRAD_COMM, sum(r,REG, sum(s,REG,
   2919               VTMFSD(m,i,r,s)))));
   2920    
   2921   Coefficient (all,m,MARG_COMM)(all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   2922       VTFSD_MSH(m,i,r,s) # share of margin m in cost of getting i from r to s #;
   2923   Formula (all,m,MARG_COMM)(all,i,TRAD_COMM)(all,r,REG)
   2924           (all,s,REG: VTFSD(i,r,s) > 0.0)
   2925       VTFSD_MSH(m,i,r,s) = VTMFSD(m,i,r,s) / VTFSD(i,r,s);
   2926   Formula (all,m,MARG_COMM)(all,i,TRAD_COMM)(all,r,REG)
   2927           (all,s,REG: VTFSD(i,r,s) = 0.0)
   2928       VTFSD_MSH(m,i,r,s) = VTMUSE(m) / VTUSE;
   2929    
   2930   Equation TRANSCOSTINDEX
   2931   #  generates flow-specific modal average cost of transport index (cf. HT7) #
   2932   (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   2933       ptrans(i,r,s)
   2934           = sum(m,MARG_COMM, VTFSD_MSH(m,i,r,s) * [pt(m) - atmfsd(m,i,r,s)]);
   2935    
   2936   Equation TRANSTECHANGE
   2937   #  generates flow-specific average rate of technical change #
   2938   (all,m,MARG_COMM)(all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   2939       atmfsd(m,i,r,s) = atm(m) + atf(i) + ats(r) + atd(s) + atall(m,i,r,s);
   2940    
   2941   Equation TRANSVCES
   2942   # generate demand for regional supply of global transportation service (HT 61) #
   2943   (all,m,MARG_COMM)(all,r,REG)
   2944       qst(m,r) = qtm(m) + [pt(m) - pm(m,r)];
   2945   !<
   2946       This equation generates the international transport sector's derived
   2947       demand for regional supplies of transportation services.  It reflects a
   2948       unitary elasticity of substitution between transportation services inputs
   2949       from different regions.
   2950   >!
   2951    
   2952   Coefficient (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   2953       VIWSCOST(i,r,s) # value of imports calculated as total cost of imports #;
   2954   Formula (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   2955       VIWSCOST(i,r,s) = VXWD(i,r,s) + VTFSD(i,r,s);
   2956    
   2957   Coefficient (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   2958       FOBSHR(i,r,s) # FOB share in VIW #;
   2959   Formula (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   2960       FOBSHR(i,r,s) = VXWD(i,r,s) / VIWSCOST(i,r,s);
   2961    
   2962   Coefficient (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   2963       TRNSHR(i,r,s) # transport share in VIW #;
   2964   Formula (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   2965       TRNSHR(i,r,s) = VTFSD(i,r,s) / VIWSCOST(i,r,s);
   2966    
   2967   Equation FOBCIF
   2968   # eq'n links FOB and CIF prices for good i shipped from region r to s (HT 26') #
   2969   (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   2970       pcif(i,r,s)
   2971          = FOBSHR(i,r,s) * pfob(i,r,s)
   2972          + TRNSHR(i,r,s) * ptrans(i,r,s);
   2973   !<
   2974       This equation links export and import prices for each commodity/route
   2975       triplet.  Note that technical change is embodied in ptrans(i,r,s) which is
   2976       now a cost index, as opposed to (HT 26') where it represented the price of
   2977       margins services.
   2978   >!
   2979    
   2980    
   2981   !<
   2982       --------------------------------
   2983       7. International Emissions Trade
   2984       --------------------------------
   2985   >!
   2986    
   2987   !Andre Barbe on April 1, 2015 Added 1 line and commented out 1 line so that
   2988   absolute CO2 emissions by region can be viewed!
   2989   !Variable (all,r,REG)! !This line was commented out!
   2990   Variable(orig_level=CO2Q) (all,r,REG) !this line was added!
   2991       gco2q(r) # emissions quota #;
   2992   Coefficient (ge 0) (all,r,REG)
   2993       CO2Q(r) # emissions quota (millions of tonnes of CO2) #;
   2994   Update (all,r,REG)
   2995       CO2Q(r) = gco2q(r);
   2996   Read
   2997       CO2Q from file GTAPDATA header "CO2Q";
   2998    
   2999   Variable (all,r,REG)
   3000       pemp(r) # power of emissions purchases#;
   3001   Equation PEMPURCH(all,r,REG)
   3002       pemp(r) = gco2t(r) - gco2q(r);
   3003    
   3004   Equation EMTRADING
   3005   # net emissions trading revenue #
   3006   (all,r,REG)
   3007       DVCO2TRA(r)
   3008           = CO2Q(r) * NCTAXLEV(r) * 0.01 * gco2q(r)
   3009           - CO2T(r) * NCTAXLEV(r) * 0.01 * gco2t(r)
   3010           + [CO2Q(r) - CO2T(r)] * NCTAXB(REGTOBLOC(r));
   3011    
   3012   Variable (all,b,BLOC)
   3013       gco2tb(b) # carbon dioxide emissions #;
   3014   Variable (all,b,BLOC)
   3015       gco2qb(b) # emissions quota#;
   3016    
   3017   Equation TOTALEMISSIONSB # CO2 emissions #
   3018   (all,b,BLOC)
   3019       CO2TB(b) * gco2tb(b) = sum(r,REG: REGTOBLOC(r) = b, CO2T(r) * gco2t(r));
   3020    
   3021   Coefficient (all,b,BLOC)
   3022       CO2QB(b) # emissions quota (millions of tonnes of CO2)#;
   3023   Formula (all,b,BLOC)
   3024       CO2QB(b) = sum(r, REG: REGTOBLOC(r)=b, CO2Q(r));
   3025   Equation EMISSQUOTA # emissions quota #
   3026   (all,b,BLOC)
   3027       CO2QB(b) * gco2qb(b) = sum(r, REG: REGTOBLOC(r) = b, CO2Q(r) * gco2q(r));
   3028    
   3029   Variable (all,b,BLOC)
   3030       pempb(b) # power of emissions purchases#;
   3031   Equation PEMPURCHB(all,b,BLOC)
   3032       pempb(b) = gco2tb(b) - gco2qb(b);
   3033    
   3034    
