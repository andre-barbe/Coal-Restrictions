   2041   !<
   2042       3-5. Non-Coal Energy Nest
   2043       -------------------------
   2044   >!
   2045    
   2046   Set
   2047       NCOAL_FCOMM # inputs into non-coal energy subproduction #
   2048       (oil, gas, oil_pcts);
   2049   Subset
   2050       NCOAL_FCOMM is subset of FIRM_COMM;
   2051    
   2052   Formula (all,j,PROD_COMM)(all,r,REG)
   2053       VFAS("ncoal",j,r) = sum(i,NCOAL_FCOMM, VFA(i,j,r));
   2054   Formula (all,j,PROD_COMM)(all,r,REG)
   2055       VFA("ncoal",j,r) = VFAS("ncoal",j,r);
   2056    
   2057   Coefficient
   2058       FSIZE_NCOAL # size of NCOAL_FCOMM set #;
   2059   Formula
   2060       FSIZE_NCOAL = sum(i,NCOAL_FCOMM, 1);
   2061    
   2062   Coefficient (all,i,NCOAL_FCOMM)(all,j,PROD_COMM)(all,r,REG)
   2063       FSHNCOAL(i,j,r)
   2064       # share of i in cost to j of non-coal energy subproduct #;
   2065   Formula (all,i,NCOAL_FCOMM)(all,j,PROD_COMM)(all,r,REG: VFAS("ncoal",j,r) = 0)
   2066       FSHNCOAL(i,j,r) = 1.0 / FSIZE_NCOAL;
   2067   Formula (all,i,NCOAL_FCOMM)(all,j,PROD_COMM)(all,r,REG: VFAS("ncoal",j,r) > 0)
   2068       FSHNCOAL(i,j,r) = VFA(i,j,r) / VFAS("ncoal",j,r);
   2069    
   2070   !Commented out by Andre Barbe!
   2071   !Equation NCOALFPRICE # price of non-coal energy #
   2072   (all,j,PROD_COMM)(all,r,REG)
   2073       pf("ncoal",j,r)
   2074           = sum(k,NCOAL_FCOMM, FSHNCOAL(k,j,r) * [pf(k,j,r) - af(k,j,r)]);!
   2075    
   2076   Coefficient (parameter) (all,j,PROD_COMM)(all,r,REG)
   2077       ELFNCOAL(j,r)
   2078       # elasticity of substitution in non-coal energy subproduction #;
   2079   Read
   2080       ELFNCOAL from file GTAPPARM header "EFNC";
   2081    
   2082   !Commented out by Andre Barbe!
   2083   !Equation NCOALFDEMAND
   2084   # demand for inputs into non-coal energy subproduction #
   2085   (all,i,NCOAL_FCOMM)(all,j,PROD_COMM)(all,r,REG)
   2086       qf(i,j,r) = -af(i,j,r) + qf("ncoal",j,r)
   2087           - ELFNCOAL(j,r) * [pf(i,j,r) - af(i,j,r) - pf("ncoal",j,r)];!
