   4086   !<
   4087       Quantity Indices for Aggregate Trade
   4088       ....................................
   4089   >!
   4090   Variable (orig_level=VXW)(all,i,TRAD_COMM)(all,r,REG)
   4091       qxw(i,r) # aggregate exports of i from region r, FOB weights #;
   4092   Equation QREGEX_ir_MARG
   4093   # change in volume of exports of margin commodity m from r #
   4094   (all,m,MARG_COMM)(all,r,REG)
   4095       VXW(m,r) * qxw(m,r)
   4096           = sum(s,REG, VXWD(m,r,s) * qxs(m,r,s)) + VST(m,r) * qst(m,r);
   4097   Equation QREGEX_ir_NMRG
   4098   # change in volume of exports of non-margin commodity i from r #
   4099   (all,i,NMRG_COMM)(all,r,REG)
   4100       VXW(i,r) * qxw(i,r) = sum(s,REG, VXWD(i,r,s) * qxs(i,r,s));
   4101   !<
   4102       modified from HT 89 for use with AnalyzeGE
   4103   >!
   4104    
   4105   Variable (orig_level=VXWREGION)(all,r,REG)
   4106       qxwreg(r) # volume of merchandise exports, by region #;
   4107   Equation QREGEX_r
   4108   # computes % change in quantity index of exports, by region #
   4109   (all,r,REG)
   4110       VXWREGION(r) * qxwreg(r) = sum(i,TRAD_COMM, VXW(i,r) * qxw(i,r));
   4111   !<
   4112       modified from HT 91 for use with AnalyzeGE
   4113   >!
   4114    
   4115   Variable (orig_level=VXWCOMMOD)(all,i,TRAD_COMM)
   4116       qxwcom(i) # volume of global merchandise exports by commodity #;
   4117   Equation QWLDEX_i
   4118   # computes % change in quantity index of exports, by commodity #
   4119   (all,i,TRAD_COMM)
   4120       VXWCOMMOD(i) * qxwcom(i) = sum(r,REG, VXW(i,r) * qxw(i,r));
   4121   !<
   4122       modified from HT 93 for use with AnalyzeGE
   4123   >!
   4124    
   4125   Variable (orig_level=VXWLD)
   4126       qxwwld # volume of world trade #;
   4127   Equation QWLDEX
   4128   # computes % change in quantity index of global exports #
   4129       VXWLD * qxwwld = sum(r,REG, VXWREGION(r) * qxwreg(r));
   4130   !<
   4131       modified from HT 95 for use with AnalyzeGE
   4132   >!
   4133    
   4134   Variable (all,i,TRAD_COMM)(all,s,REG)
   4135       qiw(i,s) # aggregate imports of i into region s, CIF weights #;
   4136   Equation QREGIM_is
   4137   # change in volume of imports of commodity i into s #
   4138   (all,i,TRAD_COMM)(all,s,REG)
   4139       VIW(i,s) * qiw(i,s) = sum(r,REG, VIWS(i,r,s) * qxs(i,r,s));
   4140   !<
   4141       modified from HT 90 for use with AnalyzeGE
   4142   >!
   4143    
   4144   Variable (orig_level=VIWREGION)(all,r,REG)
   4145       qiwreg(r) # volume of merchandise imports, by region #;
   4146   Equation QREGIM_s
   4147   # computes % change in quantity index of imports, by region #
   4148   (all,s,REG)
   4149       VIWREGION(s) * qiwreg(s) = sum(i,TRAD_COMM, VIW(i,s) * qiw(i,s));
   4150   !<
   4151       modified from HT 92 for use with AnalyzeGE
   4152   >!
   4153    
   4154   Variable (orig_level=VIWCOMMOD)(all,i,TRAD_COMM)
   4155       qiwcom(i) # volume of global merchandise imports by commodity #;
   4156   Equation QWLDIM_i
   4157   # computes % change in quantity index of imports, by commodity #
   4158   (all,i,TRAD_COMM)
   4159       VIWCOMMOD(i) * qiwcom(i) = sum(s,REG, VIW(i,s) * qiw(i,s));
   4160   !<
   4161       modified from HT 94 for use with AnalyzeGE
   4162   >!
   4163    
   4164   Variable (all,i,TRAD_COMM)
   4165       qow(i) # quantity index for world supply of good i #;
   4166   Equation QWLDOUT
   4167   # change in index of world production of i #
   4168   (all,i,TRAD_COMM)
   4169       VWOW(i) * qow(i) = sum(r,REG, VOW(i,r) * qo(i,r));
   4170   !<
   4171       modified from HT 96 for use with AnalyzeGE
   4172   >!
   4173    
   4174   Variable (orig_level=VWOU)(all,i,TRAD_COMM)
   4175       qowu(i) # quantity index for world supply of good i at user prices #;
   4176   Equation QWLDOUTU
   4177   # change in index of world production of i evaluated at user prices #
   4178   (all,i,TRAD_COMM)
   4179       VWOU(i) * qowu(i)
   4180           = sum(s,REG, VPAT(i,s) * qpt(i,s)
   4181           + VGAT(i,s) * qgt(i,s)
   4182           + sum(j,PROD_COMM, VFAD(i,j,s) * qft(i,j,s)));
   4183    
   4184    
   4185   !<
   4186       A-5. Trade Balance Indices
   4187       --------------------------
   4188   >!
   4189    
   4190   Variable (change)(all,i,TRAD_COMM)(all,r,REG)
   4191       DTBALi(i,r) # change in trade balance by i and by r, $ US million #;
   4192   !<
   4193       A positive value indicates that the change in exports exceeds the change
   4194       in imports.
   4195   >!
   4196   Equation TRADEBAL_i
   4197   # computes change in trade balance by commodity and by region (HT 97) #
   4198   (all,i,TRAD_COMM)(all,r,REG)
   4199       DTBALi(i,r)
   4200           = [VXW(i,r) / 100] * vxwfob(i,r) - [VIW(i,r) / 100] * viwcif(i,r);
   4201    
   4202   Variable (change)(all,r,REG)
   4203       DTBAL(r) # change in trade balance X - M, $ US million #;
   4204   Equation TRADEBALANCE
   4205   # computes change in trade balance (X - M), by region (HT 98) #
   4206   (all,r,REG)
   4207       DTBAL(r)
   4208           = [VXWREGION(r) / 100] * vxwreg(r) - [VIWREGION(r) / 100] * viwreg(r);
   4209    
   4210   !<
   4211       The balance on current account is equal to the sum of the ordinary trade
   4212       balance and net emissions trading revenue.
   4213   >!
   4214   Variable (change) (all,r,REG)
   4215       DTBALCTRA(r) # balance on current account #;
   4216   Equation TRAEMTRADBAL # balance on current account #
   4217   (all,r,REG)
   4218       DTBALCTRA(r) = DTBAL(r) + DVCO2TRA(r);
   4219    
   4220   Coefficient (all,r,REG)
   4221       TBALCTRA(r) # balance on current account #;
   4222   Formula (all,r,REG)
   4223       TBALCTRA(r) = VXWREGION(r) - VIWREGION(r)
   4224           + [CO2Q(r) - CO2T(r)] * NCTAXLEV(r);
   4225    
   4226   Variable (change)(all,r,REG)
   4227       DBALCAR(r)
   4228       # change in ratio of current account balance to regional income #;
   4229   Equation DBALCARATIO
   4230   # change in ratio of trade balance to regional income #
   4231   (all,r,REG)
   4232       100 * INCOME(r) * DBALCAR(r) = 100 * DTBALCTRA(r) - TBALCTRA(r) * y(r);
   4233    
   4234   !<
   4235       In order to maintain homogeneity in the presence of a fixed trade balance,
   4236       it is useful to have a nominal variable which this is measured against.
   4237       The next equation provides this, and we recommend users fix DTBALR instead
   4238       of fixing DTBAL in future simulations.  The strategy is the same one used
   4239       above for taxes.
   4240   >!
   4241    
   4242   Coefficient (all,r,REG)
   4243       TBAL(r) # trade balance for region r #;
   4244   Formula (all,r,REG)
   4245       TBAL(r) = VXWREGION(r) - VIWREGION(r);
   4246    
   4247   Variable (change)(all,r,REG)
   4248       DTBALR(r) # change in ratio of trade balance to regional income #;
   4249   Equation DTBALRATIO
   4250   # change in ratio of trade balance to regional income #
   4251   (all,r,REG)
   4252       100 * INCOME(r) * DTBALR(r) = 100 * DTBAL(r) - TBAL(r) * y(r);
   4253    
   4254    
   4255   !<
   4256       A-6. Real Energy Price Index
   4257       ----------------------------
   4258    
   4259       The real energy price index is a Divisia index of user prices of energy,
   4260       deflated by the GDP deflator.
   4261   >!
   4262    
   4263   Coefficient (all,i,EGY_COMM)(all,r,REG)
   4264       TOTDEM(i,r) # energy usage at agents' prices #;
   4265   Formula (all,i,EGY_COMM)(all,r,REG)
   4266       TOTDEM(i,r) = sum{j,PROD_COMM, VDFA(i,j,r)}
   4267           + sum{j,PROD_COMM, VIFA(i,j,r)} + VDPA(i,r) + VIPA(i,r) + VDGA(i,r)
   4268           + VIGA(i,r);
   4269    
   4270   Variable (all,i,EGY_COMM)(all,r,REG)
   4271       vdem(i,r) # money value of energy usage at agents' prices #;
   4272   Equation TOTVALEN
   4273   # money value of energy usage at agents' prices #
   4274   (all,i,EGY_COMM)(all,r,REG)
   4275       TOTDEM(i,r) * vdem(i,r) =
   4276           sum{j,PROD_COMM, VDFA(i,j,r)*[pfd(i,j,r) + qfd(i,j,r)]}
   4277           + sum{j,PROD_COMM, VIFA(i,j,r)*[pfm(i,j,r) + qfm(i,j,r)]}
   4278           + VDPA(i,r)*[ppd(i,r) + qpd(i,r)] + VIPA(i,r)*[ppm(i,r) + qpm(i,r)]
   4279           + VDGA(i,r)*[pgd(i,r) + qgd(i,r)] + VIGA(i,r)*[pgm(i,r) + qgm(i,r)];
   4280    
   4281   Variable (all,i,EGY_COMM)(all,r,REG)
   4282       qdem(i,r) # agents'-prices-weighted energy usage quantity index #;
   4283   Equation TOTQUAEN
   4284   # agents'-prices-weighted energy usage quantity index #
   4285   (all,i,EGY_COMM)(all,r,REG)
   4286       TOTDEM(i,r) * qdem(i,r) =
   4287           sum{j,PROD_COMM, VDFA(i,j,r)*qfd(i,j,r)}
   4288           + sum{j,PROD_COMM, VIFA(i,j,r)*qfm(i,j,r)}
   4289           + VDPA(i,r)*qpd(i,r) + VIPA(i,r)*qpm(i,r)
   4290           + VDGA(i,r)*qgd(i,r) + VIGA(i,r)*qgm(i,r);
   4291    
   4292   Variable (all,i,EGY_COMM)(all,r,REG)
   4293       apen(i,r) # real energy price index #;
   4294   Equation AVPRICE
   4295   # real energy price index #
   4296   (all,i,EGY_COMM)(all,r,REG)
   4297       apen(i,r) = vdem(i,r) - qdem(i,r) - pgdp(r);
   4298    
   4299    
   4300   !<
   4301       -----------------------
   4302       B. Equivalent Variation
   4303       -----------------------
   4304    
   4305       B-0. Appendix-Specific Variables and Coefficients
   4306       B-1. Government Consumption Shadow Demand System
   4307       B-2. Private Consumption Shadow Demand System
   4308       B-3. Regional Household Shadow Demand System
   4309       B-4. Equivalent Variation
   4310    
   4311       This appendix calculates equivalent variation "EV" and world equivalent
   4312       variation, "WEV", by determining the income "yev" that would be required
   4313       to achieve the current actual utility level "u" in a shadow demand system
   4314       in which prices are fixed.
   4315    
   4316       Section B-2 calculates the utility elasticity of private consumption
   4317       expenditure, "ueprivev", within a shadow demand system for private
   4318       consumption, for use in section B-3.  B-3 calculates private consumption
   4319       expenditure "ypev" for use in B-2, and regional income "yev" for use in
   4320       B-4, within a shadow demand system for the regional household.  B-4
   4321       calculates "EV" and "WEV".
   4322   >!
   4323    
   4324    
   4325   !<
   4326       B-0. Appendix-Specific Variables and Coefficients
   4327       -------------------------------------------------
   4328    
   4329       only used in this Equivalent Variation section of the Summary Indices
   4330       appendix
   4331   >!
   4332    
   4333   Variable (all,r,REG)
   4334       uelasev(r)
   4335       # elasticity of cost of utility wrt utility, for EV calc. #;
   4336   Variable (all,r,REG)
   4337       ueprivev(r)
   4338       # utility elasticity of private consn expenditure, for EV calc. #;
   4339   Variable (all,r,REG)
   4340       ugev(r) # per capita utility from gov't expend., for EV calc. #;
   4341   Variable (all,r,REG)
   4342       upev(r) # per capita utility from private expend., for EV calc. #;
   4343   Variable (all,r,REG)
   4344       qsaveev(r) # total quantity of savings demanded, for EV calc. #;
   4345   Variable (all,r,REG)
   4346       yev(r) # regional household income in region r, for EV calc. #;
   4347   Variable (all,r,REG)
   4348       ypev(r)
   4349       # private consumption expenditure in region r, for EV calc. #;
   4350   Variable (all,r,REG)
   4351       ygev(r)
   4352       # government consumption expenditure in region r, for EV calc. #;
   4353    
   4354   Coefficient (all,r,REG)
   4355       INCOMEEV(r) # regional income, for EV calc. #;
   4356   Formula (initial) (all,r,REG)
   4357       INCOMEEV(r) = INCOME(r);
   4358   Update (all,r,REG)
   4359       INCOMEEV(r) = yev(r);
   4360    
   4361   Coefficient (all,r,REG)
   4362       UTILPRIVEV(r) # utility from private consumption, for EV calcs #;
   4363   Formula (initial) (all,r,REG)
   4364       UTILPRIVEV(r) = UTILPRIV(r);
   4365   Update (all,r,REG)
   4366       UTILPRIVEV(r) = upev(r);
   4367    
   4368   Coefficient (all,r,REG)
   4369       UTILGOVEV(r) # utility from private consumption, for EV calcs #;
   4370   Formula (initial) (all,r,REG)
   4371       UTILGOVEV(r) = UTILGOV(r);
   4372   Update (all,r,REG)
   4373       UTILGOVEV(r) = ugev(r);
   4374    
   4375   Coefficient (all,r,REG)
   4376       UTILSAVEEV(r) # utility from private consumption, for EV calcs #;
   4377   Formula (initial) (all,r,REG)
   4378       UTILSAVEEV(r) = UTILSAVE(r);
   4379   Update (change) (all,r,REG)
   4380       UTILSAVEEV(r) = [[qsaveev(r) - pop(r)] / 100] * UTILSAVEEV(r);
   4381    
   4382    
   4383   !<
   4384       B-1. Government Consumption Shadow Demand System
   4385       ------------------------------------------------
   4386   >!
   4387    
   4388   Equation GOVUSHD
   4389   # utility from government consumption in r #
   4390   (all,r,REG)
   4391       ygev(r) - pop(r) = ugev(r);
   4392    
   4393    
   4394   !<
   4395       B-2. Private Consumption Shadow Demand System
   4396       ---------------------------------------------
   4397   >!
   4398    
   4399   Variable (all,i,UP_COMM)(all,r,REG)
   4400       qpev(i,r)
   4401       # private hhld demand for commodity i in region r, for EV calc. #;
   4402    
   4403   Coefficient (all,i,UP_COMM)(all,r,REG)
   4404       VPAEV(i,r)
   4405       # private hhld expend. on i in r valued at agent's prices, for EV calc. #;
   4406   Formula (initial) (all,i,UP_COMM)(all,r,REG)
   4407       VPAEV(i,r) = VPA(i,r);
   4408   Update (all,i,UP_COMM)(all,r,REG)
   4409       VPAEV(i,r) = qpev(i,r);
   4410    
   4411   Coefficient (all,r,REG)
   4412       VPAREGEV(r) # private consumption expenditure in region r, for EV calc. #;
   4413   Formula (all,r,REG)
   4414       VPAREGEV(r) = sum(i,UP_COMM, VPAEV(i,r));
   4415   !<
   4416       VPAREGEV should agree with PRIVEXPEV.
   4417   >!
   4418    
   4419   Coefficient (all,i,UP_COMM)(all,r,REG)
   4420       CONSHREV(i,r)
   4421       # share of private hhld consn devoted to good i in r, for EV calc. #;
   4422   Formula (all,i,UP_COMM)(all,r,REG)
   4423       CONSHREV(i,r) = VPAEV(i,r) / VPAREGEV(r);
   4424   Coefficient (all,i,UP_COMM)(all,r,REG)
   4425       EYEV(i,r)
   4426       # expend. elast. of private hhld demand for i in r, for EV calc. #;
   4427   Formula (all,i,UP_COMM)(all,r,REG)
   4428       EYEV(i,r)
   4429           = [1.0 / sum(n,UP_COMM, CONSHREV(n,r) * INCPAR(n,r))]
   4430           * [INCPAR(i,r) * [1.0 - ALPHA(i,r)]
   4431               + sum(n,UP_COMM, CONSHREV(n,r) * INCPAR(n,r) * ALPHA(n,r))]
   4432           + ALPHA(i,r)
   4433           - sum(n,UP_COMM, CONSHREV(n,r) * ALPHA(n,r));
   4434    
   4435   Equation PRIVDMNDSEV
   4436       # private hhld demands for composite commodities, for EV calc. #
   4437       (all,i,UP_COMM)(all,r,REG)
   4438       qpev(i,r) - pop(r) = EYEV(i,r) * [ypev(r) - pop(r)];
   4439   !<
   4440       Prices are held constant for the EV calculation and so do not appear here.
   4441   >!
   4442    
   4443   Coefficient (all,r,REG)
   4444       UELASPRIVEV(r)
   4445       # elast. of cost wrt utility from private consn, for EV calc. #;
   4446   Formula (all,r,REG)
   4447       UELASPRIVEV(r) = sum(i,UP_COMM, CONSHREV(i,r) * INCPAR(i,r));
   4448    
   4449   Equation PRIVATEUEV
   4450   # computation of utility from private consumption in r (HT 45) #
   4451   (all,r,REG)
   4452       ypev(r) - pop(r) = UELASPRIVEV(r) * upev(r);
   4453    
   4454   Coefficient (all,i,UP_COMM)(all,r,REG)
   4455       XWCONSHREV(i,r)
   4456       # expansion-parameter-weighted consumption share, for EV calc. #;
   4457   Formula (all,i,UP_COMM)(all,r,REG)
   4458       XWCONSHREV(i,r) = CONSHREV(i,r) * INCPAR(i,r) / UELASPRIVEV(r);
   4459    
   4460   Equation UTILELASPRIVEV
   4461       # elasticity of cost wrt utility from private consn, for EV calc. #
   4462       (all,r,REG)
   4463       ueprivev(r)
   4464           = sum(i,UP_COMM, XWCONSHREV(i,r) * [qpev(i,r) - ypev(r)]);
   4465   !<
   4466       Prices are held constant for the EV calculation and so do not appear here.
   4467   >!
   4468    
   4469    
   4470   !<
   4471    
   4472       B-3. Regional Household Shadow Demand System
   4473       --------------------------------------------
   4474   >!
   4475    
   4476   Variable (all,r,REG)
   4477       ysaveev(r) # NET savings expenditure, for EV calc. #;
   4478    
   4479   Coefficient (all,r,REG)
   4480       PRIVEXPEV(r)
   4481       # private consumption expenditure in region r, for EV calc. #;
   4482   Formula (initial) (all,r,REG)
   4483       PRIVEXPEV(r) = PRIVEXP(r);
   4484   Update (all,r,REG)
   4485       PRIVEXPEV(r) = ypev(r);
   4486   !<
   4487       PRIVEXPEV should agree with VPAREGEV.
   4488   >!
   4489    
   4490   Coefficient (all,r,REG)
   4491       GOVEXPEV(r)
   4492       # government consumption expenditure in region r, for EV calc. #;
   4493   Formula (initial) (all,r,REG)
   4494       GOVEXPEV(r) = GOVEXP(r);
   4495   Update (all,r,REG)
   4496       GOVEXPEV(r) = ygev(r);
   4497    
   4498   Coefficient (all,r,REG)
   4499       SAVEEV(r)
   4500       # saving in region r, for EV calc. #;
   4501   Formula (initial) (all,r,REG)
   4502       SAVEEV(r) = SAVE(r);
   4503   Update (all,r,REG)
   4504       SAVEEV(r) = ysaveev(r);
   4505    
   4506   Coefficient (all,r,REG)
   4507       XSHRPRIVEV(r)
   4508       # private expenditure share in regional income, for EV calc. #;
   4509   Formula (all,r,REG)
   4510       XSHRPRIVEV(r) = PRIVEXPEV(r) / INCOMEEV(r);
   4511    
   4512   Coefficient (all,r,REG)
   4513       XSHRGOVEV(r)
   4514       # government expenditure share in regional income, for EV calc. #;
   4515   Formula (all,r,REG)
   4516       XSHRGOVEV(r) = GOVEXPEV(r) / INCOMEEV(r);
   4517    
   4518   Coefficient (all,r,REG)
   4519       XSHRSAVEEV(r) # saving share in regional income, for EV calc. #;
   4520   Formula (all,r,REG)
   4521       XSHRSAVEEV(r) = SAVEEV(r) / INCOMEEV(r);
   4522    
   4523   Variable (all,r,REG)
   4524       dpavev(r) # average distribution parameter shift, for EV calc. #;
   4525   Equation DPARAVEV
   4526   # average distribution parameter shift, for EV calc. #
   4527       (all,r,REG)
   4528       dpavev(r)
   4529           = XSHRPRIVEV(r) * dppriv(r)
   4530           + XSHRGOVEV(r) * dpgov(r)
   4531           + XSHRSAVEEV(r) * dpsave(r);
   4532    
   4533   Equation UTILITELASTICEV
   4534       # elasticity of cost of utility wrt utility, for EV calc. #
   4535       (all,r,REG)
   4536       uelasev(r) = XSHRPRIV(r) * ueprivev(r) - dpavev(r);
   4537    
   4538   Equation PCONSEXPEV
   4539   # private consumption expenditure, for EV calc. #
   4540       (all,r,REG)
   4541       ypev(r) - yev(r) = -[ueprivev(r) - uelasev(r)] + dppriv(r);
   4542    
   4543   Equation GOVCONSEXPEV
   4544   # government consumption expenditure #
   4545   (all,r,REG)
   4546       ygev(r) - yev(r) = uelasev(r) + dpgov(r);
   4547    
   4548   Equation SAVINGEV
   4549   # saving #
   4550   (all,r,REG)
   4551       ysaveev(r) - yev(r) = uelasev(r) + dpsave(r);
   4552    
   4553   Equation SAVEUEV
   4554   # saving #
   4555   (all,r,REG)
   4556       qsaveev(r) = ysaveev(r);
   4557    
   4558   !<
   4559       Note that because psave doesn't change, qsaveev moves with ysaveev.
   4560   >!
   4561    
   4562   Coefficient (all,r,REG)
   4563       UTILELASEV(r)
   4564       # elasticity of cost of utility wrt utility, for EV calc. #;
   4565   Formula (initial) (all,r,REG)
   4566       UTILELASEV(r) = UTILELAS(r);
   4567   Update (all,r,REG)
   4568       UTILELASEV(r) = uelasev(r);
   4569    
   4570   Equation INCOME_EQUIV
   4571   # equivalent income #
   4572   (all,r,REG)
   4573       u(r)
   4574           = au(r)
   4575           + DPARPRIV(r) * loge(UTILPRIVEV(r)) * dppriv(r)
   4576           + DPARGOV(r) * loge(UTILGOVEV(r)) * dpgov(r)
   4577           + DPARSAVE(r) * loge(UTILSAVEEV(r)) * dpsave(r)
   4578           + [1.0 / UTILELASEV(r)] * [yev(r) - pop(r)];
   4579    
   4580    
   4581   !<
   4582       B-4. Equivalent Variation
   4583       -------------------------
   4584   >!
   4585    
   4586   Variable (change)(all,r,REG)
