   4600   !<
   4601       ------------------------
   4602       C. Welfare Decomposition
   4603       ------------------------
   4604       See GTAP Technical Paper No. 5 for derivation and interpretation.
   4605   >!
   4606    
   4607   Coefficient (all,r,REG)
   4608       EVSCALFACT(r) # equivalent variation scaling factor #;
   4609   Formula (all,r,REG)
   4610       EVSCALFACT(r) = [UTILELASEV(r) / UTILELAS(r)] * [INCOMEEV(r) / INCOME(r)];
   4611    
   4612   Coefficient (all,m,MARG_COMM)(all,s,REG)
   4613       VTMD(m,s) # aggregate value of svce m in shipments to s #;
   4614   Formula (all,m,MARG_COMM)(all,s,REG)
   4615       VTMD(m,s) = sum(i,TRAD_COMM, sum(r,REG, VTMFSD(m,i,r,s)));
   4616    
   4617   Variable (linear,change)(all,r,REG)
   4618       EV_ALT(r) # regional EV computed in alternative way #;
   4619   Equation EV_DECOMPOSITION
   4620   # decomposition of Equivalent Variation #
   4621   (all,r,REG)
   4622       EV_ALT(r)
   4623           = -[0.01 * UTILELASEV(r) * INCOMEEV(r)]
   4624           * [DPARPRIV(r) * loge(UTILPRIVEV(r) / UTILPRIV(r)) * dppriv(r)
   4625               + DPARGOV(r) * loge(UTILGOVEV(r) / UTILGOV(r)) * dpgov(r)
   4626               + DPARSAVE(r) * loge(UTILSAVEEV(r) / UTILSAVE(r)) * dpsave(r)]
   4627           + [0.01 * EVSCALFACT(r)]
   4628           * [(100.0 * DVCO2TRA(r))
   4629               + sum(i,NSAV_COMM, PTAX(i,r) * [qo(i,r) - pop(r)])
   4630               + sum(i,ENDW_COMM, sum(j,PROD_COMM,
   4631                     ETAX(i,j,r) * [qfe(i,j,r) - pop(r)]))
   4632               + sum(j,PROD_COMM, sum(i,TRAD_COMM,
   4633                     [IFTAX(i,j,r) + IFCTAX(i,j,r)] * [qfm(i,j,r) - pop(r)]))
   4634               + sum(j,PROD_COMM, sum(i,TRAD_COMM,
   4635                     [DFTAX(i,j,r) + DFCTAX(i,j,r)] * [qfd(i,j,r) - pop(r)]))
   4636               + sum(i,TRAD_COMM,
   4637                   [IPTAX(i,r) + IPCTAX(i,r)] * [qpm(i,r) - pop(r)])
   4638               + sum(i,TRAD_COMM,
   4639                   [DPTAX(i,r) + DPCTAX(i,r)] * [qpd(i,r) - pop(r)])
   4640               + sum(i,TRAD_COMM,
   4641                   [IGTAX(i,r) + IGCTAX(i,r)] * [qgm(i,r) - pop(r)])
   4642               + sum(i,TRAD_COMM,
   4643                   [DGTAX(i,r) + DGCTAX(i,r)] * [qgd(i,r) - pop(r)])
   4644               + sum(i,TRAD_COMM, sum(s,REG, XTAXD(i,r,s) * [qxs(i,r,s) - pop(r)]))
   4645               + sum(i,TRAD_COMM, sum(s,REG, MTAX(i,s,r) * [qxs(i,s,r) - pop(r)]))
   4646               + sum(i,ENDW_COMM, VOA(i,r) * [qo(i,r) - pop(r)])
   4647               - VDEP(r) * [kb(r) - pop(r)]
   4648               + sum(i,PROD_COMM, VOA(i,r) * ao(i,r))
   4649               + sum(j,PROD_COMM, sum(i,FIRM_COMM, VFA(i,j,r) * af(i,j,r)))
   4650               + sum(m,MARG_COMM, sum(i,TRAD_COMM, sum(s,REG,
   4651                   VTMFSD(m,i,s,r) * atmfsd(m,i,s,r))))
   4652               + sum(i,TRAD_COMM, sum(s,REG, VIMS(i,s,r) * ams(i,s,r)))
   4653               + sum(i,TRAD_COMM, sum(s,REG, VXWD(i,r,s) * pfob(i,r,s)))
   4654               + sum(m,MARG_COMM, VST(m,r) * pm(m,r))
   4655               + NETINV(r) * pcgds(r)
   4656               - sum(i,TRAD_COMM, sum(s,REG, VXWD(i,s,r) * pfob(i,s,r)))
   4657               - sum(m,MARG_COMM, VTMD(m,r) * pt(m))
   4658               - SAVE(r) * psave(r)]
   4659           + 0.01 * INCOMEEV(r) * pop(r);
   4660    
   4661   Variable (linear,change)
   4662       WEV_ALT # expression for WEV computed in alternative way #;
   4663   Equation WORLDEV
   4664   # Equivalent Variation for the world #
   4665       WEV_ALT = sum(r,REG, EV_ALT(r));
