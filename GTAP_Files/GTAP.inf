   1044    
   1045   Coefficient (all,i,TRAD_COMM)(all,r,REG)
   1046       DGCTAX(i,r) # carbon tax on gov. cons. of domestic good i in region r #;
   1047   Formula (all,i,TRAD_COMM)(all,r,REG)
   1048       DGCTAX(i,r) = VDGA(i,r) - VDGANC(i,r);
   1049    
   1050   Coefficient (all,i,TRAD_COMM)(all,r,REG)
   1051       IGTAX(i,r) # ordinary tax on government consumption of imports #;
   1052   Formula (all,i,TRAD_COMM)(all,r,REG)
   1053       IGTAX(i,r) = VIGANC(i,r) - VIGM(i,r);
   1054    
   1055   Coefficient (all,i,TRAD_COMM)(all,r,REG)
   1056       IGCTAX(i,r) # carbon tax on government consumption of imports #;
   1057   Formula (all,i,TRAD_COMM)(all,r,REG)
   1058       IGCTAX(i,r) = VIGA(i,r) - VIGANC(i,r);
