   1855   !<
   1856       3-0. Module-Specific Variables
   1857       ------------------------------
   1858    
   1859       only used in this Firms Module or the Summary Indices or Welfare
   1860       appendices
   1861   >!
   1862    
   1863   Set
   1864       SUBF_COMM # subproducts in demand by firms #
   1865       (vaen, ken, eny, nely, ncoal);
   1866   Set
   1867       FIRM_COMM # commodities demanded by firms # = DEMD_COMM union SUBF_COMM;
   1868    
   1869   Variable (all,i,TRAD_COMM)(all,j,PROD_COMM)(all,r,REG)
   1870       pft(i,j,r) # firms' price for commodity i for use by j in r #;
   1871   Variable (all,i,TRAD_COMM)(all,j,PROD_COMM)(all,r,REG)
   1872       qft(i,j,r) # demand for commodity i for use by j in region r #;
   1873   Variable (all,i,FIRM_COMM)(all,j,PROD_COMM)(all,r,REG)
   1874       pf(i,j,r) # firms' price for commodity i for use by j in r #;
   1875   Variable (all,i,FIRM_COMM)(all,j,PROD_COMM)(all,r,REG)
   1876       qf(i,j,r) # demand for commodity i for use by j in r #;
   1877   Variable (all,i,FIRM_COMM)(all,j,PROD_COMM)(all,r,REG)
   1878       af(i,j,r) # factor i augmenting tech change in sector j of r #;
   1879   Variable (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   1880       ams(i,r,s) # import i from region r augmenting tech change in region s #;
   1881    
   1882   Coefficient (all,i,FIRM_COMM)(all,j,PROD_COMM)(all,r,REG)
   1883       VFA(i,j,r) # producer expend. on i by j in r valued at agent's prices #;
   1884    
   1885   Coefficient (all,i,SUBF_COMM)(all,j,PROD_COMM)(all,r,REG)
   1886       VFAS(i,j,r) # producer expend. on i by j in r valued at agent's prices #;
