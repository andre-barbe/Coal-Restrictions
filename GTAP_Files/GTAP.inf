   3036       ---------------------
   3037       8. Regional Household
   3038       ---------------------
   3039    
   3040       8-0. Module-Specific Coefficients
   3041       8-1. Supply of Sluggish Endowments by the Regional Household
   3042       8-2. Carbon Dioxide Emissions
   3043       8-3. Carbon Tax Rate
   3044       8-4. Computation of Regional Income
   3045       8-5. Regional Household Demand System
   3046       8-6. Aggregate Utility
   3047   >!
   3048    
   3049    
   3050   !<
   3051       8-0. Module-Specific Coefficients
   3052       ---------------------------------
   3053    
   3054       only used in this Regional Household module
   3055   >!
   3056    
   3057   Coefficient (all,r,REG)
   3058       XSHRPRIV(r) # private expenditure share in regional income #;
   3059   Formula (all,r,REG)
   3060       XSHRPRIV(r) = PRIVEXP(r) / INCOME(r);
   3061    
   3062   Coefficient (all,r,REG)
   3063       XSHRGOV(r) # government expenditure share in regional income #;
   3064   Formula (all,r,REG)
   3065       XSHRGOV(r) = GOVEXP(r) / INCOME(r);
   3066    
   3067   Coefficient (all,r,REG)
   3068       XSHRSAVE(r) # saving share in regional income #;
   3069   Formula (all,r,REG)
   3070       XSHRSAVE(r) = SAVE(r) / INCOME(r);
   3071    
   3072   !<
   3073       ====================
   3074       Code to show the absolute level of CO2 emissions by commodity and region
   3075       added by Andre Barbe April 1, 2015
   3076       ====================
   3077   >!
   3078    
   3079   Coefficient (all,r,REG)(all,i,CTAX_COMM)
   3080       CO2_COPY(r,i);
   3081   Formula (always) (all,r,REG)(all,i,CTAX_COMM)
   3082       CO2_COPY(r,i) = CO2(r,i);
   3083   Variable(orig_level=CO2_COPY) (all,r,REG)(all,i,CTAX_COMM)
   3084   !Variable (all,r,REG)(all,i,CTAX_COMM)! !Code commented out by Andre!
   3085   !End added code!
   3086       gco2(r,i) # carbon dioxide emissions #;
   3087   Variable (all,r,REG)
   3088       dppriv(r) # private consumption distribution parameter #;
   3089   Variable (all,r,REG)
   3090       dpgov(r) # government consumption distribution parameter #;
   3091   Variable (all,r,REG)
   3092       dpsave(r) # saving distribution parameter #;
   3093   Variable (all,r,REG)
   3094       uelas(r) # elasticity of cost of utility wrt utility #;
   3095    
   3096    
   3097   !<
   3098       8-1. Supply of Endowments by the Regional Household
   3099       ---------------------------------------------------
   3100   >!
   3101    
   3102   Equation FACTORINCPRICES
   3103   # eq'n links pre- and post-tax endowment supply prices (HT 15) #
   3104   (all,i,ENDW_COMM)(all,r,REG)
   3105       ps(i,r) = to(i,r) + pm(i,r);
   3106    
   3107   Coefficient (all,r,REG)
   3108       TINC(r) # income tax payments in r #;
   3109   Formula (all,r,REG)
   3110       TINC(r) = sum(i,ENDW_COMM, PTAX(i,r));
   3111    
   3112   Equation TINCRATIO
   3113   # change in ratio of income tax payments to regional income #
   3114   (all,r,REG)
   3115       100.0 * INCOME(r) * del_taxrinc(r) + TINC(r) * y(r)
   3116           = sum(i,ENDW_COMM,
   3117               VOA(i,r) * [-to(i,r)] + PTAX(i,r) * [pm(i,r) + qo(i,r)]);
   3118    
   3119   Coefficient (all,i,ENDW_COMM)(all,j,PROD_COMM)(all,r,REG)
   3120       REVSHR(i,j,r);
   3121   Formula (all,i,ENDW_COMM)(all,j,PROD_COMM)(all,r,REG)
   3122       REVSHR(i,j,r) = VFM(i,j,r) / sum(k,PROD_COMM, VFM(i,k,r));
   3123   Equation ENDW_PRICE
   3124   # eq'n generates the composite price for sluggish endowments (HT 50) #
   3125   (all,i,ENDWS_COMM)(all,r,REG)
   3126       pm(i,r) = sum(k,PROD_COMM, REVSHR(i,k,r) * pmes(i,k,r));
   3127    
   3128   Coefficient (parameter)(all,i,ENDW_COMM)
   3129       ETRAE(i)
   3130       # elst. of transformation for sluggish primary factor endowments #;
   3131   Read
   3132       ETRAE from file GTAPPARM header "ETRE";
   3133   !<
   3134       ETRAE is the elasticity of transformation for sluggish primary factor
   3135       endowments.  It is non-positive, by definition.
   3136   >!
   3137   Equation ENDW_SUPPLY
   3138   # eq'n distributes the sluggish endowments across sectors (HT 51) #
   3139   (all,i,ENDWS_COMM)(all,j,PROD_COMM)(all,r,REG)
   3140       qoes(i,j,r) = qo(i,r) - endwslack(i,r) + ETRAE(i) * [pm(i,r) - pmes(i,j,r)];
   3141    
   3142    
   3143   !<
   3144       8-2. Carbon Dioxide Emissions
   3145       -----------------------------
   3146   >!
   3147    
   3148   Equation PDEMISSIONS
   3149   # carbon dioxide emissions from private consumption of domestic product #
   3150   (all, i,TRAD_COMM)(all,r,REG)
   3151       gco2pd(i,r) = qpd(i,r);
   3152    
   3153   Equation PIEMISSIONS
   3154   # carbon dioxide emissions from private consumption of imports #
   3155   (all,i, TRAD_COMM)(all,r,REG)
   3156       gco2pm(i,r) = qpm(i,r);
   3157    
   3158   Equation GDEMISSIONS
   3159   # carbon dioxide emissions from government consumption of domestic product #
   3160   (all,i, TRAD_COMM)(all,r,REG)
   3161       gco2gd(i,r) = qgd(i,r);
   3162    
   3163   Equation GIEMISSIONS
   3164   # carbon dioxide emissions from government consumption of imports #
   3165   (all, i,TRAD_COMM)(all,r,REG)
   3166       gco2gm(i,r) = qgm(i,r);
   3167    
   3168   Equation FDEMISSIONS
   3169   # carbon dioxide emissions from firms' usage of domestic product #
   3170   (all,i,TRAD_COMM)(all,j,PROD_COMM)(all,r,REG)
   3171       gco2fd(i,j,r) = qfd(i,j,r);
   3172    
   3173   Equation FIEMISSIONS
   3174   # carbon dioxide emissions from firms' usage of imports #
   3175   (all,i,TRAD_COMM)(all,j,PROD_COMM)(all,r,REG)
   3176       gco2fm(i,j,r) = qfm(i,j,r);
   3177    
   3178   Equation CEMISSIONS # carbon dioxide emissions #
   3179   (all,r,REG)(all,i,CTAX_COMM)
   3180       CO2(r,i) * gco2(r,i) = sum(j,PROD_COMM, CO2IF(i,j,r) * gco2fm(i,j,r)
   3181           + CO2DF(i,j,r) * gco2fd(i,j,r))
   3182           + CO2DG(i,r) * gco2gd(i,r) + CO2IG(i,r) * gco2gm(i,r)
   3183           + CO2DP(i,r) * gco2pd(i,r) + CO2IP(i,r) * gco2pm(i,r);
   3184    
   3185   Equation TOTALEMISSIONS # carbon dioxide emissions #
   3186   (all,r,REG)
   3187       CO2T(r) * gco2t(r) = sum(i,CTAX_COMM, CO2(r,i) * gco2(r,i));
   3188    
   3189    
   3190   !<
   3191       ====================
   3192       Code to show the absolute level of CO2 emissions world wide
   3193       added by Andre Barbe March 17, 2015
   3194       ====================
   3195   >!
   3196    
   3197   Coefficient L_gco2tw;
   3198   Formula L_gco2tw = sum(r,REG, CO2T(r));
   3199    
   3200   Variable (orig_level=L_gco2tw)
   3201   !Variable!
   3202   !!
   3203       gco2tw # total world emissions #;
   3204   Equation WORLDEMISS # total world emissions #
   3205       sum(r,REG, CO2T(r)) * gco2tw = sum(r,REG, CO2T(r) * gco2t(r));
   3206    
   3207    
   3208   !<
   3209       8-3. Carbon Tax Rate
   3210       --------------------
   3211    
   3212       RCTAXB and NCTAXB represent changes in real and nominal carbon tax
   3213       rates.  The real rate is defined as the nominal rate deflated by the income
   3214       deflator.
   3215   >!
   3216    
   3217   Variable (change)(all,r,REG)
   3218       RCTAX(r) # real carbon tax rate (1997 USD per tonne of CO2) #;
   3219   Coefficient (all,r,REG)
   3220       PIND(r) # income deflator #;
   3221   Update (all,r,REG)
   3222       PIND(r) = p(r);
   3223   Formula (initial) (all,r,REG)
   3224       PIND(r) = 1.0;
   3225   Equation REALTAXCH
   3226   # real carbon tax rate #
   3227   (all,r,REG)
   3228       RCTAX(r) = [1.0 / PIND(r)]
   3229           * [NCTAXB(REGTOBLOC(r)) - 0.01 * NCTAXLEV(r) * p(r)];
   3230    
   3231   Variable (orig_level = 1.0)(all,b,BLOC)
   3232       pb(b) # income deflator #;
   3233    
   3234   Coefficient (all,b,BLOC)
   3235       INCOMEB(b) # Net income in bloc #;
   3236   Formula (all,b,BLOC)
   3237       INCOMEB(b) = sum(r,REG: REGTOBLOC(r) = b, INCOME(r));
   3238   Equation PIND_b # income deflator #
   3239   (all,b,BLOC)
   3240       INCOMEB(b) * pb(b) = sum(r,REG: REGTOBLOC(r) = b, INCOME(r) * p(r));
   3241    
   3242   Variable (change)(all,r,BLOC)
   3243       RCTAXB(r) # real carbon tax rate (1997 USD per tonne of CO2) #;
   3244   Coefficient (all,b,BLOC)
   3245       PINDB(b) # income deflator #;
   3246   Update (all,b,BLOC)
   3247       PINDB(b) = pb(b);
   3248   Formula (initial) (all,b,BLOC)
   3249       PINDB(b) = 1.0;
   3250   Coefficient (all,b,BLOC)
   3251       NCTAXLEVB(b) # nominal carbon tax rate (current USD per tonne of CO2) #;
   3252   Formula (all,b,BLOC)
   3253       NCTAXLEVB(b) = sum(r,REG: REGTOBLOC(r) = b, NCTAXLEV(r) * CO2T(r))
   3254           / CO2TB(b);
   3255   Equation REALTAXCHB
   3256   # real carbon tax rate #
   3257   (all,b,BLOC)
   3258       RCTAXB(b) = [1.0 / PINDB(b)]
   3259           * [NCTAXB(b) - 0.01 * NCTAXLEVB(b) * pb(b)];
   3260    
   3261   Variable (change) (all,r,REG)
   3262       del_ctgshr(r) # government share of carbon tax payments/permits revenue #;
   3263   Coefficient (all,r,REG)
   3264       CTGSHARE(r) # government share of carbon tax payments/permits revenue #;
   3265   Update (change) (all,r,REG)
   3266       CTGSHARE(r) = del_ctgshr(r);
   3267   Read
   3268       CTGSHARE from file GTAPDATA header "GSHR";
   3269    
   3270   Equation TCRATIO
   3271   # change in ratio of carbon tax payments/permit rent to regional income #
   3272   (all,r,REG)
   3273       100.0 * INCOME(r) * del_ctaxr(r)
   3274           + CTGSHARE(r) * NCTAXLEV(r) * CO2Q(r) * y(r)
   3275           = 100.0 * NCTAXLEV(r) * CO2Q(r) * del_ctgshr(r)
   3276               + 100.0 * CTGSHARE(r) * CO2Q(r) * NCTAXB(REGTOBLOC(r))
   3277               + CTGSHARE(r) * NCTAXLEV(r) * CO2Q(r) * gco2q(r);
   3278    
   3279    
   3280   !<
   3281       8-4. Computation of Regional Income
   3282       -----------------------------------
   3283   >!
   3284    
   3285   Coefficient (all,r,REG)
   3286       FY(r) # primary factor income in r net of depreciation #;
   3287   Formula (all,r,REG)
   3288       FY(r) = sum(i,ENDW_COMM, VOM(i,r)) - VDEP(r);
   3289   Variable (all,r,REG)
   3290       fincome(r) # factor income at market prices net of depreciation #;
   3291   Equation FACTORINCOME
   3292   # factor income at market prices net of depreciation #
   3293   (all,r,REG)
   3294       FY(r) * fincome(r)
   3295           = sum(i,ENDW_COMM, VOM(i,r) * [pm(i,r) + qo(i,r)])
   3296           - VDEP(r)  * [pcgds(r) + kb(r)];
   3297    
   3298   Variable (change)(all,r,REG)
   3299       del_indtaxr(r) # change in ratio of indirect taxes to INCOME in r #;
   3300   Equation DINDTAXRATIO
   3301   # change in ratio of indirect taxes to INCOME in r #
   3302   (all,r,REG)
   3303       del_indtaxr(r)
   3304           = del_taxrpc(r) + del_taxrgc(r) + del_taxriu(r) + del_taxrfu(r)
   3305           + del_taxrout(r) + del_taxrexp(r) + del_taxrimp(r);
   3306    
   3307   Variable (change)(all,r,REG)
   3308       del_tnctaxr(r) # change in ratio of carbon-exclusive taxes to INCOME in r #;
   3309   Equation DNCTAXRATIO
   3310   # change in ratio of carbon-exclusive taxes to INCOME in r #
   3311   (all,r,REG)
   3312       del_tnctaxr(r)
   3313           = del_taxrpc(r) + del_taxrgc(r) + del_taxriu(r) + del_taxrfu(r)
   3314           + del_taxrout(r) + del_taxrexp(r) + del_taxrimp(r) + del_taxrinc(r);
   3315    
   3316   Variable (change)(all,r,REG)
   3317       del_ttaxr(r) # change in ratio of total taxes to INCOME in r #;
   3318   Equation DTAXRATIO
   3319   # change in ratio of total taxes to INCOME in r #
   3320   (all,r,REG)
   3321       del_ttaxr(r) = del_tnctaxr(r) + del_ctaxr(r);
   3322    
   3323   !<
   3324      This variable can be swapped with the commodity- and source-generic
   3325      consumption tax shift, tp(r), in order to generate a tax replacement
   3326      scenario, whereby taxes remain a constant share of national income.
   3327   >!
   3328    
   3329   Variable (all,r,REG)
   3330       incomeslack(r) # slack variable in the expression for regional income #;
   3331   !<
   3332       This is exogenous, unless the user wishes to fix regional income.
   3333   >!
   3334    
   3335   Coefficient (all,r,REG)
   3336       INDTAX(r) # indirect tax receipts in r #;
   3337   Formula (all,r,REG)
   3338       INDTAX(r) = TPC(r) + TGC(r) + TIU(r) + TFU(r) + TOUT(r) + TEX(r) + TIM(r);
   3339    
   3340   Equation REGIONALINCOME
   3341   # regional income = sum of primary factor income and indirect tax receipts #
   3342   (all,r,REG)
   3343       INCOME(r) * y(r)
   3344           = FY(r) * fincome(r)
   3345           + 100.0 * INCOME(r) * del_indtaxr(r)
   3346           + INDTAX(r) * y(r)
   3347           + CO2Q(r) * NCTAXLEV(r) * gco2q(r)
   3348           + 100.0 * CO2Q(r) * NCTAXB(REGTOBLOC(r))
   3349           + INCOME(r) * incomeslack(r);
   3350   !<
   3351       This equation computes regional income as the sum of primary factor
   3352       payment and indirect tax receipts.  The first term computes the change in
   3353       endowment income, net of depreciation.  The second and third terms compute
   3354       the change in ordinary indirect tax revenue.  The fourth term computes the
   3355       revenues/expenditures from carbon emissions trading.  The fifth term
   3356       computes revenue from carbon taxes.
   3357   >!
   3358    
   3359    
   3360   !<
   3361       8-5. Regional Household Demand System
   3362       -------------------------------------
   3363   >!
   3364    
   3365   Variable (all,r,REG)
   3366       dpav(r) # average distribution parameter shift, for EV calc. #;
   3367   Equation DPARAV
   3368   # average distribution parameter shift #
   3369   (all,r,REG)
   3370       dpav(r)
   3371           = XSHRPRIV(r) * dppriv(r)
   3372           + XSHRGOV(r) * dpgov(r)
   3373           + XSHRSAVE(r) * dpsave(r);
   3374    
   3375   Equation UTILITELASTIC
   3376   # elasticity of cost of utility wrt utility #
   3377   (all,r,REG)
   3378       uelas(r) = XSHRPRIV(r) * uepriv(r) - dpav(r);
   3379    
   3380   Equation PRIVCONSEXP
   3381   # private consumption expenditure #
   3382   (all,r,REG)
   3383       yp(r) - y(r) = -[uepriv(r) - uelas(r)] + dppriv(r);
   3384    
   3385   Equation GOVCONSEXP
   3386   # government consumption expenditure #
   3387   (all,r,REG)
   3388       yg(r) - y(r) = uelas(r) + dpgov(r);
   3389    
   3390   Equation SAVING
   3391   # saving #
   3392   (all,r,REG)
   3393       psave(r) + qsave(r) - y(r) = uelas(r) + dpsave(r);
   3394    
   3395    
   3396   !<
   3397       8-6. Aggregate Utility
   3398       ----------------------
   3399   >!
   3400   Equation PRICEINDEXREG
   3401       # price index for disposition of income by regional household #
   3402       (all,r,REG)
   3403       p(r)
   3404           = XSHRPRIV(r) * ppriv(r)
   3405           + XSHRGOV(r) * pgov(r)
   3406           + XSHRSAVE(r) * psave(r);
   3407    
   3408   Variable (all,r,REG)
   3409       au(r) # input-neutral shift in utility function #;
   3410   Variable (all,r,REG)
   3411       dpsum(r) # sum of the distribution parameters #;
   3412    
   3413   Coefficient (all,r,REG)
   3414       DPARSUM(r) # sum of distribution parameters #;
   3415   Read
   3416       DPARSUM from file GTAPDATA header "DPSM";
   3417   Update (all,r,REG)
   3418       DPARSUM(r) = dpsum(r);
   3419    
   3420   Coefficient (all,r,REG)
   3421       UTILELAS(r) # elasticity of cost of utility wrt utility #;
   3422   Formula (all,r,REG)
   3423       UTILELAS(r)
   3424           = [UELASPRIV(r) * XSHRPRIV(r) + XSHRGOV(r) + XSHRSAVE(r)] / DPARSUM(r);
   3425    
   3426   Coefficient (all,r,REG)
   3427       DPARPRIV(r) # private consumption distribution parameter #;
   3428   Formula (all,r,REG)
   3429       DPARPRIV(r) = UELASPRIV(r) * XSHRPRIV(r) / UTILELAS(r);
   3430    
   3431   Coefficient (all,r,REG)
   3432       DPARGOV(r) # government consumption distribution parameter #;
   3433   Formula (all,r,REG)
   3434       DPARGOV(r) = XSHRGOV(r) / UTILELAS(r);
   3435    
   3436   Coefficient (all,r,REG)
   3437       DPARSAVE(r) # saving distribution parameter #;
   3438   Formula (all,r,REG)
   3439       DPARSAVE(r) = XSHRSAVE(r) / UTILELAS(r);
   3440    
   3441   Coefficient (all,r,REG)
   3442       UTILPRIV(r) # utility from private consumption #;
   3443   Formula (initial)(all,r,REG)
   3444       UTILPRIV(r) = 1.0;
   3445   Update (all,r,REG)
   3446       UTILPRIV(r) = up(r);
   3447    
   3448   Coefficient (all,r,REG)
   3449       UTILGOV(r) # utility from government consumption #;
   3450   Formula (initial)(all,r,REG)
   3451       UTILGOV(r) = 1.0;
   3452   Update (all,r,REG)
   3453       UTILGOV(r) = ug(r);
   3454    
   3455   Coefficient (all,r,REG)
   3456       UTILSAVE(r) # utility from saving #;
   3457   Formula (initial)(all,r,REG)
   3458       UTILSAVE(r) = 1.0;
   3459   Update (change) (all,r,REG)
   3460       UTILSAVE(r) = [[qsave(r) - pop(r)] / 100] * UTILSAVE(r);
   3461    
   3462   Variable (all,r,REG)
   3463       u(r) # per capita utility from aggregate hhld expend. in region r #;
   3464   Equation UTILITY
   3465   # regional household utility #
   3466   (all,r,REG)
   3467       u(r)
   3468           = au(r)
   3469           + DPARPRIV(r) * loge(UTILPRIV(r)) * dppriv(r)
   3470           + DPARGOV(r) * loge(UTILGOV(r)) * dpgov(r)
   3471           + DPARSAVE(r) * loge(UTILSAVE(r)) * dpsave(r)
   3472           + [1.0 / UTILELAS(r)] * [y(r) - pop(r) - p(r)];
   3473    
   3474   Equation DISTPARSUM
   3475   # sum of the distribution parameters #
   3476   (all,r,REG)
   3477       DPARSUM(r) * dpsum(r)
   3478           = DPARPRIV(r) * dppriv(r)
   3479           + DPARGOV(r) * dpgov(r)
   3480           + DPARSAVE(r) * dpsave(r);
   3481    
   3482    
   3483   !<
   3484       -------------------------
   3485       9. Equilibrium Conditions
   3486       -------------------------
   3487    
   3488       9-1. Market Clearing Conditions
   3489       9-2. Walras' Law
   3490   >!
   3491    
   3492    
   3493   !<
   3494       9-1. Market Clearing Conditions
   3495       -------------------------------
   3496   >!
   3497    
   3498   Coefficient (all,i,TRAD_COMM)(all,j,PROD_COMM)(all,r,REG)
   3499       SHRDFM(i,j,r) # share of dom. prod. i used by sector j in r at mkt prices #;
   3500   Formula (all,i,TRAD_COMM)(all,j,PROD_COMM)(all,r,REG)
   3501       SHRDFM(i,j,r) = VDFM(i,j,r) / VDM(i,r);
   3502    
   3503   Coefficient (all,i,TRAD_COMM)(all,r,REG)
   3504       SHRDPM(i,r) # share of domestic prod. of i used by private hhlds in r #;
   3505   Formula (all,i,TRAD_COMM)(all,r,REG)
   3506       SHRDPM(i,r) = VDPM(i,r) / VDM(i,r);
   3507    
   3508   Coefficient (all,i,TRAD_COMM)(all,r,REG)
   3509       SHRDGM(i,r) # share of imports of i used by gov't hhlds in r #;
   3510   Formula (all,i,TRAD_COMM)(all,r,REG)
   3511       SHRDGM(i,r) = VDGM(i,r) / VDM(i,r);
   3512    
   3513   Variable (orig_level=VDM)(all,i,TRAD_COMM)(all,r,REG)
   3514       qds(i,r) # domestic sales of commodity i in r #;
   3515   Equation MKTCLDOM
   3516   # eq'n assures market clearing for domestic sales (HT 3) #
   3517   (all,i,TRAD_COMM)(all,r,REG)
   3518       qds(i,r)
   3519           = sum(j,PROD_COMM, SHRDFM(i,j,r) * qfd(i,j,r))
   3520           + SHRDPM(i,r) * qpd(i,r)
   3521           + SHRDGM(i,r) * qgd(i,r);
   3522    
   3523   Coefficient (all,i,TRAD_COMM)(all,r,REG)
   3524       SHRDM(i,r) # share of domestic sales of i in r #;
   3525   Formula (all,i,TRAD_COMM)(all,r,REG)
   3526       SHRDM(i,r) = VDM(i,r) / VOM(i,r);
   3527    
   3528   Coefficient (all,m,MARG_COMM)(all,r,REG)
   3529       SHRST(m,r) # share of sales of m to global transport services in r #;
   3530   Formula (all,m,MARG_COMM)(all,r,REG)
   3531       SHRST(m,r) = VST(m,r) / VOM(m,r);
   3532    
   3533   Coefficient (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   3534       SHRXMD(i,r,s) # share of export sales of i to s in r #;
   3535   Formula (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   3536       SHRXMD(i,r,s) = VXMD(i,r,s) / VOM(i,r);
   3537    
   3538   Variable (all,i,TRAD_COMM)(all,r,REG)
   3539       tradslack(i,r) # slack variable in tradeables market clearing condition #;
   3540   !<
   3541       This is exogenous unless the user wishes to specify the price of
   3542       tradeables exogenously, in which case the analysis becomes partial
   3543       equilibrium and walraslack must be exogenized.
   3544   >!
   3545    
   3546   Equation MKTCLTRD_MARG
   3547   # eq'n assures market clearing for margins commodities (HT 1) #
   3548   (all,m,MARG_COMM)(all,r,REG)
   3549       qo(m,r)
   3550           = SHRDM(m,r) * qds(m,r)
   3551           + SHRST(m,r) * qst(m,r)
   3552           + sum(s,REG, SHRXMD(m,r,s) * qxs(m,r,s))
   3553           + tradslack(m,r);
   3554   Equation MKTCLTRD_NMRG
   3555   # eq'n assures market clearing for the non-margins commodities (HT 1) #
   3556   (all,i,NMRG_COMM)(all,r,REG)
   3557       qo(i,r)
   3558           = SHRDM(i,r) * qds(i,r)
   3559           + sum(s,REG, SHRXMD(i,r,s) * qxs(i,r,s))
   3560           + tradslack(i,r);
   3561    
   3562   Coefficient (all,i,TRAD_COMM)(all,r,REG)
   3563       VIM(i,r) # value of imports of commodity i in r at domestic market prices #;
   3564   Formula (all,i,TRAD_COMM)(all,r,REG)
   3565       VIM(i,r) = sum(j,PROD_COMM, VIFM(i,j,r)) + VIPM(i,r) + VIGM(i,r);
   3566    
   3567   Coefficient (all,i,TRAD_COMM)(all,j,PROD_COMM)(all,r,REG)
   3568       SHRIFM(i,j,r) # share of import i used by sector j in r  #;
   3569   Formula (all,i,TRAD_COMM)(all,j,PROD_COMM)(all,r,REG)
   3570       SHRIFM(i,j,r) = VIFM(i,j,r) / VIM(i,r);
   3571    
   3572   Coefficient (all,i,TRAD_COMM)(all,r,REG)
   3573       SHRIPM(i,r) # share of import i used by private hhlds in r #;
   3574   Formula (all,i,TRAD_COMM)(all,r,REG)
   3575       SHRIPM(i,r) = VIPM(i,r) / VIM(i,r);
   3576    
   3577   Coefficient (all,i,TRAD_COMM)(all,r,REG)
   3578       SHRIGM(i,r) # the share of import i used by gov't hhlds in r #;
   3579   Formula (all,i,TRAD_COMM)(all,r,REG)
   3580       SHRIGM(i,r) = VIGM(i,r) / VIM(i,r);
   3581    
   3582   Equation MKTCLIMP
   3583   # eq'n assures mkt clearing for imported goods entering each region (HT 2) #
   3584   (all,i,TRAD_COMM)(all,r,REG)
   3585       qim(i,r)
   3586           = sum(j,PROD_COMM, SHRIFM(i,j,r) * qfm(i,j,r))
   3587           + SHRIPM(i,r) * qpm(i,r)
   3588           + SHRIGM(i,r) * qgm(i,r);
   3589    
   3590   Coefficient (all,i,ENDWM_COMM)(all,j,PROD_COMM)(all,r,REG)
   3591       SHREM(i,j,r) # share of mobile endowment i used by sector j at mkt prices #;
   3592   Formula (all,i,ENDWM_COMM)(all,j,PROD_COMM)(all,r,REG)
   3593       SHREM(i,j,r) = VFM(i,j,r) / VOM(i,r);
   3594    
   3595   Equation MKTCLENDWM
   3596   # eq'n assures mkt clearing for perfectly mobile endowments in each r (HT 4) #
   3597   (all,i,ENDWM_COMM)(all,r,REG)
   3598       qo(i,r) = sum(j,PROD_COMM, SHREM(i,j,r) * qfe(i,j,r)) + endwslack(i,r);
   3599   !<
   3600       This equation assures market clearing for perfectly mobile endowments (HT 4)
   3601   >!
   3602    
   3603   Equation MKTCLENDWS
   3604   # eq'n assures mkt clearing for imperfectly mobile endowments in each r (HT 5)#
   3605   (all,i,ENDWS_COMM)(all,j,PROD_COMM)(all,r,REG)
   3606       qoes(i,j,r) = qfe(i,j,r);
   3607   !<
   3608       This equation assures market clearing for sluggish endowments (HT 5)
   3609   >!
   3610    
   3611    
   3612   !<
   3613       9-2. Walras' Law
   3614       ----------------
   3615   >!
   3616    
   3617   Variable
   3618       walras_sup # supply in omitted market--global supply of cgds composite #;
   3619   Equation WALRAS_S
   3620   # Extra eq'n computes change in supply in the omitted market. #
   3621       walras_sup = pcgdswld + globalcgds;
   3622   !<
   3623       This is an extra equation which simply computes change in supply in the
   3624       omitted market.  (modified from HT 12 to reflect the value as opposed to
   3625       quantity change, in light of the new treatment of psave(r))
   3626   >!
   3627    
   3628   Variable
   3629       walras_dem # demand in the omitted market--global demand for savings #;
   3630   Equation WALRAS_D
   3631   # Extra eq'n computes change in demand in the omitted market. #
   3632       GLOBINV * walras_dem = sum(r,REG, SAVE(r) * [psave(r) + qsave(r)]);
   3633   !<
   3634       This is an extra equation which simply computes change in demand in the
   3635       omitted market.  (modified from HT 13 to reflect the value as opposed to
   3636       quantity change)
   3637   >!
   3638    
   3639   Variable
   3640       walraslack # slack variable in the omitted market #;
   3641   !<
   3642       This is endogenous under normal, GE closure.  If the GE links are broken,
   3643       then this must be swapped with the numeraire, thereby forcing global
   3644       savings to explicitly equal global investment.
   3645   >!
   3646   Equation WALRAS
   3647   # Check Walras' Law.  Value of "walraslack" should be zero. (HT 14) #
   3648       walras_sup = walras_dem + walraslack;
   3649   !<
   3650       This equation checks Walras' Law.  The value of walraslack should be zero
   3651       in any GE simulation.  (HT 14)
   3652   >!
   3653    
