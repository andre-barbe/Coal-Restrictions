   3688       A-0. Appendix-Specific Variables and Coefficients
   3689       -------------------------------------------------
   3690    
   3691       only used in this Summary Indices appendix
   3692   >!
   3693    
   3694   Variable (all,i,TRAD_COMM)(all,s,REG)
   3695       vxwfob(i,s) # value of merchandise regional exports, by commodity, FOB #;
   3696   Variable (all,i,TRAD_COMM)(all,s,REG)
   3697       viwcif(i,s) # value of merchandise regional imports, by commodity, CIF #;
   3698   Variable (all,r,REG)
   3699       vxwreg(r) # value of merchandise exports, by region #;
   3700   Variable (all,r,REG)
   3701       viwreg(r) # value of merchandise imports, by region, at world prices #;
   3702    
   3703   Coefficient (all,i,TRAD_COMM)(all,r,REG)
   3704       VXW(i,r) # value of exports by comm. i and region r at FOB prices #;
   3705   Formula (all,m,MARG_COMM)(all,r,REG)
   3706       VXW(m,r) = sum(s,REG, VXWD(m,r,s)) + VST(m,r);
   3707   Formula (all,i,NMRG_COMM)(all,r,REG)
   3708       VXW(i,r) = sum(s,REG, VXWD(i,r,s));
   3709    
   3710   Coefficient (all,r,REG)
   3711       VXWREGION(r) # value of exports by region r at FOB prices #;
   3712   Formula (all,r,REG)
   3713       VXWREGION(r) = sum(i,TRAD_COMM, VXW(i,r));
   3714    
   3715   Coefficient (all,i,TRAD_COMM)(all,s,REG)
   3716       VIW(i,s) # value of commodity imports i into s at CIF prices #;
   3717   Formula (all,i,TRAD_COMM)(all,s,REG)
   3718       VIW(i,s) = sum(r,REG, VIWS(i,r,s));
   3719   Coefficient (all,r,REG)
   3720       VIWREGION(r) # value of commodity imports by region r at CIF prices #;
   3721   Formula (all,r,REG)
