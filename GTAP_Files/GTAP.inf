    918       -------------------------------
    919    
    920       Regional income is allocated between private consumption expenditure,
    921       government consumption expenditure, and saving.
    922    
    923    
    924       Expenditure of Regional Household
    925       .................................
    926    
    927                  Regional household(h.h.)
    928                   Expenditure = Income
    929                            _/|\_
    930                          _/  |  \_
    931                        _/    |    \_
    932                      _/      |      \_
    933                    _/        |        \_
    934          Private h.h.        |        Government h.h.
    935          ( PRIVEXP )         |          ( GOVEXP )
    936             _/\_             |              _/\_
    937           _/    \_           |            _/    \_
    938          /        \          |           /        \
    939       Domestic  Imported   Savings    Domestic    Imported
    940        Goods     Goods     ( SAVE )    Goods       Goods
    941       ( VDPA )  ( VIPA )              ( VDGA )    ( VIGA )
    942    
    943       Note: The coefficients at the ends of branches are
    944             Base Data, e.g., VDPA, SAVE.
    945   >!
    946    
    947   ! government consumption expenditure, GOVEXP !
    948   Coefficient (all,i,TRAD_COMM)(all,r,REG)
    949       VGAT(i,r) # government consn expenditure on i in r at agent's prices #;
    950   Formula (all,i,TRAD_COMM)(all,s,REG)
    951       VGAT(i,s) = VDGA(i,s) + VIGA(i,s);
    952   Coefficient (all,r,REG)
    953       GOVEXP(r) # government expenditure in region r #;
    954   Formula (all,r,REG)
    955       GOVEXP(r) = sum(i,TRAD_COMM, VGAT(i,r));
    956    
    957   ! private consumption expenditure, PRIVEXP !
    958   Coefficient (all,i,TRAD_COMM)(all,r,REG)
    959       VPAT(i,r) # private hhld expenditure on i in r valued at agent's prices #;
    960   Formula (all,i,TRAD_COMM)(all,s,REG)
    961       VPAT(i,s) = VDPA(i,s) + VIPA(i,s);
    962   Coefficient (all,r,REG)
    963       PRIVEXP(r) # private consumption expenditure in region r #;
    964   Formula (all,r,REG)
    965       PRIVEXP(r) = sum(i,TRAD_COMM, VPAT(i,r));
    966    
    967   ! aggregate expenditure + saving = aggregate income !
    968   Coefficient (all,r,REG)
    969       INCOME(r) # level of expenditure, which equals NET income in region r #;
    970   Formula (all,r,REG)
    971       INCOME(r) = PRIVEXP(r) + GOVEXP(r) + SAVE(r);
