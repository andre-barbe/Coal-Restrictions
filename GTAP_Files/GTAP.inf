   3806   !<
   3807       A-3. GDP Indices (Value, Price and Quantity)
   3808       --------------------------------------------
   3809   >!
   3810    
   3811   Coefficient (all,r,REG)
   3812       GDP(r) # Gross Domestic Product in region r #;
   3813   Formula (all,s,REG)
   3814       GDP(s)
   3815           = sum(i,TRAD_COMM, VPAT(i,s))
   3816           + sum(i,TRAD_COMM, VGAT(i,s))
   3817           + sum(k,CGDS_COMM, VOA(k,s))
   3818           + sum(i,TRAD_COMM, sum(r,REG, VXWD(i,s,r)))
   3819           + sum(m,MARG_COMM, VST(m,s))
   3820           - sum(i,TRAD_COMM, sum(r,REG, VIWS(i,r,s)));
   3821    
   3822   !<
   3823       Gross Domestic Product in region r.  Trade is valued at FOB and CIF
   3824       prices.
   3825   >!
   3826    
   3827   Variable (all,r,REG)
   3828       vgdp(r) # change in value of GDP #;
   3829   Equation VGDP_r
   3830   # change in value of GDP (HT 70) #
   3831   (all,r,REG)
   3832       GDP(r) * vgdp(r)
   3833           = sum(i,TRAD_COMM, VGAT(i,r) * [qgt(i,r) + pgt(i,r)])
   3834           + sum(i,TRAD_COMM, VPAT(i,r) * [qpt(i,r) + ppt(i,r)])
   3835           + REGINV(r) * [qcgds(r) + pcgds(r)]
   3836           + sum(i,TRAD_COMM, sum(s,REG, VXWD(i,r,s) * [qxs(i,r,s) + pfob(i,r,s)]))
   3837           + sum(m,MARG_COMM, VST(m,r) * [qst(m,r) + pm(m,r)])
   3838           - sum(i,TRAD_COMM, sum(s,REG,
   3839               VIWS(i,s,r) * [qxs(i,s,r) + pcif(i,s,r)]));
   3840    
   3841   Variable (orig_level=1.0)(all,r,REG)
   3842       pgdp(r) # GDP price index #;
   3843   Equation PGDP_r
   3844   # GDP price index (HT 71) #
   3845   (all,r,REG)
   3846       GDP(r) * pgdp(r)
   3847           = sum(i,TRAD_COMM, VGAT(i,r) * pgt(i,r))
   3848           + sum(i,TRAD_COMM, VPAT(i,r) * ppt(i,r))
   3849           + REGINV(r) * pcgds(r)
   3850           + sum(i,TRAD_COMM, sum(s,REG, VXWD(i,r,s) * pfob(i,r,s)))
   3851           + sum(m,MARG_COMM, VST(m,r) * pm(m,r))
   3852           - sum(i,TRAD_COMM, sum(s,REG, VIWS(i,s,r) * pcif(i,s,r)));
   3853    
   3854   Variable (orig_level=GDP)(all,r,REG)
   3855       qgdp(r) # GDP quantity index #;
   3856   Equation QGDP_r
   3857   # GDP quantity index #
   3858   (all,r,REG)
   3859       GDP(r) * qgdp(r)
   3860           = sum(i,TRAD_COMM, VGAT(i,r) * qgt(i,r))
   3861           + sum(i,TRAD_COMM, VPAT(i,r) * qpt(i,r))
   3862           + REGINV(r) * qcgds(r)
   3863           + sum(i,TRAD_COMM, sum(s,REG, VXWD(i,r,s) * qxs(i,r,s)))
   3864           + sum(m,MARG_COMM, VST(m,r) * qst(m,r))
   3865           - sum(i,TRAD_COMM, sum(s,REG, VIWS(i,s,r) * qxs(i,s,r)));
   3866   !<
   3867       modified from HT 72 for use with AnalyzeGE
   3868   >!
   3869    
   3870   Variable (all,i,PROD_COMM)(all,r,REG)
   3871       compvalad(i,r) # composition of value added for good i and region r #;
   3872   Equation COMPVALADEQ
   3873   # track change in composition of value added #
   3874   (all,i,PROD_COMM)(all,r,REG)
   3875       compvalad(i,r) = qo(i,r) - qgdp(r);
   3876    
