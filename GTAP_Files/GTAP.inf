   4955           = [0.01 * EVSCALFACT(r)]
   4956           * [sum(i,TRAD_COMM, sum(s,REG, VXWD(i,r,s) * [pfob(i,r,s) - pxwwld]))
   4957               + sum(m,MARG_COMM, VST(m,r) * [pm(m,r) - pxwwld])
   4958               - sum(i,TRAD_COMM, sum(s,REG, VXWD(i,s,r) * [pfob(i,s,r) - pxwwld]))
   4959               - sum(m,MARG_COMM, VTMD(m,r) * [pt(m) - pxwwld])];
   4960    
   4961   Variable (linear,change) (all,r,REG)
   4962       CNTcgdsr(r) # contribution to regional EV of changes in the price of cgds #;
   4963   Equation CNT_EV_cgdsr
   4964   (all,r,REG)
   4965       CNTcgdsr(r)
   4966           = [0.01 * EVSCALFACT(r)]
   4967           * [NETINV(r) * [pcgds(r) - pxwwld] - SAVE(r) * [psave(r) - pxwwld]];
   4968    
   4969   Variable (linear,change) (all,r,REG)
   4970       CNTendwr(r) # contribution to regional EV of changes in all ENDW_COMM #;
   4971   Equation CONT_EV_endwr
   4972   (all,r,REG)
   4973       CNTendwr(r)
   4974           = [0.01 * EVSCALFACT(r)]
   4975           * [sum(i,ENDW_COMM, VOA(i,r) * [qo(i,r) - pop(r)])
   4976               - VDEP(r) * [kb(r) - pop(r)]];
   4977    
   4978   Variable (linear,change) (all,i,ENDW_COMM)(all,r,REG)
   4979       CNTendwir(i,r) # contribution to regional EV of changes in ENDW_COMM i #;
   4980   Equation CONT_EV_endwir
   4981   (all,i,ENDW_COMM)(all,r,REG)
   4982       CNTendwir(i,r) = [0.01 * EVSCALFACT(r)] * [VOA(i,r) * [qo(i,r) - pop(r)]];
   4983    
   4984   Variable (linear,change) (all,r,REG)
   4985       CNTtechr(r) # contribution to regional EV of all technical change #;
   4986   Equation CONT_EV_techr
   4987   (all,r,REG)
   4988       CNTtechr(r)
   4989           = [0.01 * EVSCALFACT(r)]
   4990           * [sum(i,PROD_COMM, VOA(i,r) * ao(i,r))
   4991               + sum(j,PROD_COMM, sum(i,FIRM_COMM, VFA(i,j,r) * af(i,j,r)))
   4992               + sum(m,MARG_COMM, sum(i,TRAD_COMM, sum(s,REG,
   4993                   VTMFSD(m,i,s,r) * atmfsd(m,i,s,r))))
   4994               + sum(i,TRAD_COMM, sum(s,REG, VIMS(i,s,r) * ams(i,s,r)))];
   4995    
   4996   Variable (linear,change) (all,r,REG)
   4997       CNTtech_aor(r)
   4998       # contribution to regional EV of output augmenting technical change #;
   4999   Equation CONT_EV_tech_aor
   5000   (all,r,REG)
   5001       CNTtech_aor(r)
   5002           = [0.01 * EVSCALFACT(r)] * sum(i,PROD_COMM, VOA(i,r) * ao(i,r));
   5003    
   5004   Variable (linear,change) (all,i,PROD_COMM)(all,r,REG)
   5005       CNTtech_aoir(i,r)
   5006       # cont. to regional EV of output augmenting tech change in TRAD_COMM i #;
   5007   Equation CONT_EV_tech_aoir
   5008   (all,i,PROD_COMM)(all,r,REG)
   5009       CNTtech_aoir(i,r) = [0.01 * EVSCALFACT(r)] * VOA(i,r) * ao(i,r);
   5010    
   5011   Variable (linear,change) (all,r,REG)
   5012       CNTtech_afr(r)
   5013       # cont. to regional EV of input augmenting tech change #;
   5014   Equation CONT_EV_tech_afr
   5015   (all,r,REG)
   5016       CNTtech_afr(r)
   5017           = [0.01 * EVSCALFACT(r)]
   5018           * sum(j,PROD_COMM, sum(i,FIRM_COMM, VFA(i,j,r) * af(i,j,r)));
   5019    
   5020   Variable (linear,change) (all,i,FIRM_COMM)(all,j,PROD_COMM)(all,r,REG)
   5021       CNTtech_afijr(i,j,r)
   5022       # cont. to EV of input i input augmenting tech change in sector j #;
   5023   Equation CONT_EV_tech_afijr
   5024   (all,i,FIRM_COMM)(all,j,PROD_COMM)(all,r,REG)
   5025       CNTtech_afijr(i,j,r) = [0.01 * EVSCALFACT(r)] * VFA(i,j,r) * af(i,j,r);
   5026    
   5027   Variable (linear,change) (all,r,REG)
   5028       CNTtech_atrr(r)
   5029       # contribution to regional EV of technical change in transportation #;
   5030   Equation CONT_EV_tech_atrr
   5031   (all,r,REG)
   5032       CNTtech_atrr(r)
   5033           = [0.01 * EVSCALFACT(r)]
   5034           * sum(m,MARG_COMM, sum(i,TRAD_COMM, sum(s,REG,
   5035               VTMFSD(m,i,s,r) * atmfsd(m,i,s,r))));
   5036    
   5037   Variable (linear,change)(all,m,MARG_COMM)(all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   5038       CNTtech_afmfdsd(m,i,r,s)
   5039       # cont. to EV of tech change in transportation efficiency #;
   5040   Equation CONT_EV_tech_afmfdsd
   5041   (all,m,MARG_COMM)(all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   5042       CNTtech_afmfdsd(m,i,r,s)
   5043           = [0.01 * EVSCALFACT(s)] * VTMFSD(m,i,r,s) * atmfsd(m,i,r,s);
   5044    
   5045   Variable (linear,change)(all,r,REG)
   5046       CNTtech_amsr(r) # cont. to EV of bilateral import-augmenting tech change #;
   5047   Equation CONT_EV_tech_amsr
   5048   (all,r,REG)
   5049       CNTtech_amsr(r)
   5050           = [0.01 * EVSCALFACT(r)]
   5051           * sum(i,TRAD_COMM, sum(s,REG, VIMS(i,s,r) * ams(i,s,r)));
   5052    
   5053   Variable (linear,change) (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   5054       CNTtech_amsirs(i,r,s)
   5055    # cont. to EV of bilateral import augmenting tech change for TRAD_COMM i #;
   5056   Equation CONT_EV_tech_amsirs
   5057   (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   5058       CNTtech_amsirs(i,r,s) = [0.01 * EVSCALFACT(r)] * VIMS(i,r,s) * ams(i,r,s);
   5059    
   5060   Variable (linear,change) (all,r,REG)
   5061       CNTkbr(r)
   5062       # cont. to EV of changes to beg. period capital stock and depreciation #;
   5063   Equation CONT_EV_kbr
   5064   (all,r,REG)
   5065       CNTkbr(r) = - [0.01 * EVSCALFACT(r)] * VDEP(r) * [kb(r) - pop(r)];
   5066    
