    983    
    984   Coefficient (all,i,TRAD_COMM)(all,j,PROD_COMM)(all,r,REG)
    985       DFCTAX(i,j,r) # carbon tax on intermediate usage of domestic product #;
    986   Formula (all,i,TRAD_COMM)(all,j,PROD_COMM)(all,r,REG)
    987       DFCTAX(i,j,r) = VDFA(i,j,r) - VDFANC(i,j,r);
    988    
    989   Coefficient (all,i,TRAD_COMM)(all,j,PROD_COMM)(all,r,REG)
    990       IFTAX(i,j,r) # ordinary tax on intermediate usage of imports #;
    991   Formula (all,i,TRAD_COMM)(all,j,PROD_COMM)(all,r,REG)
    992       IFTAX(i,j,r) = VIFANC(i,j,r) - VIFM(i,j,r);
    993    
    994   Coefficient (all,i,TRAD_COMM)(all,j,PROD_COMM)(all,r,REG)
    995       IFCTAX(i,j,r) # carbon tax on intermediate usage of imports #;
    996   Formula (all,i,TRAD_COMM)(all,j,PROD_COMM)(all,r,REG)
    997       IFCTAX(i,j,r) = VIFA(i,j,r) - VIFANC(i,j,r);
    998    
    999   Coefficient (all,r,REG)
   1000       TIU(r) # ordinary tax on intermediate usage #;
   1001   Formula (all,r,REG)
   1002       TIU(r) = sum(j,PROD_COMM, sum(i,TRAD_COMM, DFTAX(i,j,r) + IFTAX(i,j,r)));
