   1465    
   1466   !<
   1467       2-0. Module-Specific Variables
   1468       ------------------------------
   1469    
   1470       only used in this Private Consumption module
   1471   >!
   1472    
   1473   Set
   1474       SUBP_COMM # subutilities in private consumption # (eny);
   1475   Set
   1476       PRIV_COMM # commodities demanded in private consumption #
   1477       = TRAD_COMM union SUBP_COMM;
   1478    
   1479   Variable (all,i,PRIV_COMM)(all,r,REG)
   1480       pp(i,r) # private consumption price for commodity i in region r #;
   1481   Variable (all,i,PRIV_COMM)(all,r,REG)
   1482       qp(i,r) # private hhld demand for commodity i in region r #;
   1483   Variable (all,i,TRAD_COMM)(all,r,REG)
   1484       ppt(i,r) # private consumption price for commodity i in region r #;
   1485   Variable (all,i,TRAD_COMM)(all,r,REG)
   1486       qpt(i,r) # private hhld demand for commodity i in region r #;
   1487    
   1488   Coefficient (all,i,PRIV_COMM)(all,r,REG)
   1489       VPA(i,r) # private hhld expend. on i in r valued at agent's prices #;
   1490   Coefficient (all,i,SUBP_COMM)(all,r,REG)
   1491       VPAS(i,r) # private hhld expend. on i in r valued at agent's prices #;
   1492    
   1493    
   1494   !<
   1495       2-1. Composite Tradeables
   1496       -------------------------
   1497   >!
   1498    
   1499   Variable (all,r,REG)
   1500       tp(r) # comm.-, source-gen. shift in tax on private cons. #;
   1501   !<
   1502       The variable tp(r) can be swapped with del_ttaxr(r) in order to generate a
   1503       tax replacement scenario, whereby taxes remain a constant share of
   1504       national income.
   1505   >!
   1506    
   1507   Variable (all,i,TRAD_COMM)(all,r,REG)
   1508       tpd(i,r) # comm.-, source-spec. shift in tax on private cons. of dom. #;
   1509   Equation TPDSHIFT
   1510   # permits uniform consumption tax change #
   1511   (all,i,TRAD_COMM)(all,r,REG)
   1512       atpd(i,r) = tpd(i,r) + tp(r);
   1513    
   1514   Zerodivide (zero_by_zero) default 1;
   1515   Coefficient (all,i,TRAD_COMM)(all,r,REG)
   1516       SHVDPANC(i,r)
   1517       # share of carbon-tax-excl. value of priv. consumption of dom. product #;
   1518   Formula (all,i,TRAD_COMM)(all,r,REG)
   1519       SHVDPANC(i,r) = VDPANC(i,r) / VDPA(i,r);
   1520   Zerodivide (zero_by_zero) off;
   1521    
   1522   Zerodivide (zero_by_zero) default 0;
   1523   Coefficient (all,i,TRAD_COMM)(all,r,REG)
   1524       CO2DPVDPA(i,r)
   1525       # emissions intensity of private consumption of domestic product #;
   1526   Formula (all,i,TRAD_COMM)(all,r,REG)
   1527       CO2DPVDPA(i,r) = CO2DP(i,r) / VDPA(i,r);
   1528   Zerodivide (zero_by_zero) off;
   1529    
   1530   Equation PHHDPRICE
   1531   # taxation of private consumption of domestic product #
   1532   (all,i,TRAD_COMM)(all,r,REG)
   1533       ppd(i,r) = SHVDPANC(i,r) * (pm(i,r) + atpd(i,r))
   1534           + 100.0 * CO2DPVDPA(i,r) * NCTAXB(REGTOBLOC(r));
   1535    
   1536   Variable (all,i,TRAD_COMM)(all,r,REG)
   1537       tpm(i,r) # comm.-, source-spec. shift in tax on private cons. of imp. #;
   1538   Equation TPMSHIFT
   1539   # permits uniform consumption tax change #
   1540   (all,i,TRAD_COMM)(all,r,REG)
   1541       atpm(i,r) = tpm(i,r) + tp(r);
   1542    
   1543   Zerodivide (zero_by_zero) default 1;
   1544   Coefficient (all,i,TRAD_COMM)(all,r,REG)
   1545       SHVIPANC(i,r)
   1546       # share of carbon-tax-excl. value of priv. consumption of imports #;
   1547   Formula (all,i,TRAD_COMM)(all,r,REG)
   1548       SHVIPANC(i,r) = VIPANC(i,r) / VIPA(i,r);
   1549   Zerodivide (zero_by_zero) off;
   1550    
   1551   Zerodivide (zero_by_zero) default 0;
   1552   Coefficient (all,i,TRAD_COMM)(all,r,REG)
   1553       CO2IPVIPA(i,r)
   1554       # emissions intensity of private consumption of imports #;
   1555   Formula (all,i,TRAD_COMM)(all,r,REG)
   1556       CO2IPVIPA(i,r) = CO2IP(i,r) / VIPA(i,r);
   1557   Zerodivide (zero_by_zero) off;
   1558    
   1559   Equation PHHIPRICES
   1560   # taxation of private consumption of imports #
   1561   (all,i,TRAD_COMM)(all,r,REG)
   1562       ppm(i,r) = SHVIPANC(i,r) * (pim(i,r) + atpm(i,r))
   1563           + 100.0 * CO2IPVIPA(i,r) * NCTAXB(REGTOBLOC(r));
   1564    
   1565   Equation TPCRATIO
   1566   # change in ratio of private consumption tax payments to regional income #
   1567   (all,r,REG)
   1568       100.0 * INCOME(r) * del_taxrpc(r) + TPC(r) * y(r)
   1569           = sum(i,TRAD_COMM,
   1570               VDPANC(i,r) * atpd(i,r) + DPTAX(i,r) * [pm(i,r) + qpd(i,r)])
   1571           + sum(i,TRAD_COMM,
   1572               VIPANC(i,r) * atpm(i,r) + IPTAX(i,r) * [pim(i,r) + qpm(i,r)]);
   1573    
   1574   Coefficient (all,i,TRAD_COMM)(all,s,REG)
   1575       PMSHR(i,s) # share of imports for priv hhld at agent's prices #;
   1576   Formula (all,i,TRAD_COMM)(all,s,REG)
   1577       PMSHR(i,s) = VIPA(i,s) / VPAT(i,s);
   1578    
   1579   Equation PCOMPRICE
   1580   # private consumption price for composite commodities (HT 47) #
   1581   (all,i,TRAD_COMM)(all,s,REG)
   1582       ppt(i,s) = PMSHR(i,s) * ppm(i,s) + [1 - PMSHR(i,s)] * ppd(i,s);
   1583    
   1584   Equation PHHLDDOM
   1585   # private consumption demand for domestic goods (HT 48) #
   1586   (all,i,TRAD_COMM)(all,s,REG)
   1587       qpd(i,s) = qpt(i,s) + ESUBD(i) * [ppt(i,s) - ppd(i,s)];
   1588    
   1589   Equation PHHLDAGRIMP
   1590   # private consumption demand for aggregate imports (HT 49) #
   1591   (all,i,TRAD_COMM)(all,s,REG)
   1592       qpm(i,s) = qpt(i,s) + ESUBD(i) * [ppt(i,s) - ppm(i,s)];
   1593    
   1594    
   1595   !<
   1596       2-2. Notational Links
   1597       ---------------------
   1598   >!
   1599    
   1600   Formula (all,i,TRAD_COMM)(all,r,REG)
   1601       VPA(i,r) = VPAT(i,r);
   1602    
   1603   Equation PPTEQ # prices into the private consumption system #
   1604   (all,i,TRAD_COMM)(all,r,REG)
   1605       ppt(i,r) = pp(i,r) ;
   1606    
   1607   Equation QPTEQ # quantities from the private consumption system #
   1608   (all,i,TRAD_COMM)(all,r,REG)
   1609       qpt(i,r) = qp(i,r) ;
   1610    
   1611    
   1612   !<
   1613       2-3. Energy Nest
   1614       ----------------
   1615   >!
   1616    
   1617   Set
   1618       ENY_PCOMM #inputs into private energy subutility#
   1619       (coal, oil, gas, oil_pcts, electricity);
   1620   Subset
   1621       ENY_PCOMM is subset of TRAD_COMM;
   1622    
   1623   Formula (all,r,REG)
   1624       VPAS("eny",r) = sum(i,ENY_PCOMM, VPA(i,r));
   1625   Formula (all,r,REG)
   1626       VPA("eny",r) = VPAS("eny",r);
   1627    
   1628   Coefficient
   1629       PSIZE_ENY # size of ENY_PCOMM set #;
   1630   Formula
   1631       PSIZE_ENY = sum(i,ENY_PCOMM, 1);
   1632    
   1633   Coefficient (all,i,ENY_PCOMM)(all,r,REG)
   1634       PSHENY(i,r) # share of i in cost of energy subutility #;
   1635   Formula (all,i,ENY_PCOMM)(all,r,REG: VPAS("eny",r) = 0)
   1636       PSHENY(i,r) = 1.0 / PSIZE_ENY;
   1637   Formula (all,i,ENY_PCOMM)(all,r,REG: VPAS("eny",r) > 0)
   1638       PSHENY(i,r) = VPA(i,r) / VPAS("eny",r);
   1639    
   1640   Equation ENYPPRICE # price of energy subutility #
   1641   (all,r,REG)
   1642       pp("eny",r) = sum(k,ENY_PCOMM, PSHENY(k,r) * pp(k,r));
   1643    
   1644   Coefficient (parameter) (all,r,REG)
   1645       ELPENY(r) # elasticity of substitution in energy subconsumption #;
   1646   Read
   1647       ELPENY from file GTAPPARM header "EPEN";
   1648    
   1649   Equation ENYPDEMAND # demand for inputs into energy subconsumption #
   1650   (all,i,ENY_PCOMM)(all,r,REG)
   1651       qp(i,r) = qp("eny",r) - ELPENY(r) * [pp(i,r) - pp("eny",r)];
   1652    
   1653    
   1654   !<
   1655       2-4. Top of the Private Consumption Structure
   1656       ---------------------------------------------
   1657   >!
   1658    
   1659   Set NENY_PCOMM
   1660       #inputs into private non-energy subutility# = TRAD_COMM - ENY_PCOMM;
   1661    
   1662   Set ENY # private energy composite # (eny);
   1663    
   1664   Set
   1665       UP_COMM
   1666       # inputs into top of private consumption structure # = ENY + NENY_PCOMM;
   1667    
   1668   Subset
   1669       UP_COMM is subset of PRIV_COMM ;
   1670    
   1671   Coefficient (parameter)(all,i,UP_COMM)(all,r,REG)
   1672       INCPAR(i,r)
   1673       # expansion parameter in the CDE minimum expenditure function #;
   1674   Read
   1675       INCPAR from file GTAPPARM header "INCP";
   1676    
   1677   Coefficient (parameter)(all,i,UP_COMM)(all,r,REG)
   1678       SUBPAR(i,r)
   1679       # substitution parameter in CDE minimum expenditure function #;
   1680   Read
   1681       SUBPAR from file GTAPPARM header "SUBP";
   1682    
   1683   Coefficient (all,i,UP_COMM)(all,r,REG)
   1684       PSHUP(i,r) # share of private hhld consumption devoted to good i in r #;
   1685   Formula (all,i,UP_COMM)(all,r,REG)
   1686       PSHUP(i,r) = VPA(i,r) / sum(k,UP_COMM, VPA(k,r));
   1687    
   1688   Formula (all,r,REG)
   1689       UELASPRIV(r) = sum(i,UP_COMM, PSHUP(i,r) * INCPAR(i,r));
   1690    
   1691   !<
   1692       (HT F1 - F3)
   1693   >!
   1694   Coefficient (all,i,UP_COMM)(all,r,REG)
   1695       ALPHA(i,r) # 1 - sub. parameter in the CDE minimum expenditure function #;
   1696   Formula (all,i,UP_COMM)(all,r,REG)
   1697       ALPHA(i,r) = 1 - SUBPAR(i,r);
   1698    
   1699   Coefficient (all,i,UP_COMM)(all,k,UP_COMM)(all,r,REG)
   1700       APE(i,k,r) # Allen partial elst. of sub. between composite i and k in r #;
   1701   Formula (all,i,UP_COMM)(all,k,UP_COMM)(all,r,REG)
   1702       APE(i,k,r)
   1703           = ALPHA(i,r) + ALPHA(k,r) - sum(n,UP_COMM, PSHUP(n,r) * ALPHA(n,r));
   1704   Formula (all,i,UP_COMM)(all,r,REG)
   1705       APE(i,i,r)
   1706           = 2.0 * ALPHA(i,r)
   1707           - sum(n,UP_COMM, PSHUP(n,r) * ALPHA(n,r))
   1708           - ALPHA(i,r) / PSHUP(i,r);
   1709    
   1710   Coefficient (all,i,UP_COMM)(all,r,REG)
   1711       EY(i,r) # income elast. of private hhld demand for i in r (HT F4) #;
   1712   Formula (all,i,UP_COMM)(all,r,REG)
   1713       EY(i,r)
   1714           =   [1.0 / sum(n,UP_COMM, PSHUP(n,r) * INCPAR(n,r))]
   1715               * [INCPAR(i,r) * [1.0 - ALPHA(i,r)]
   1716                   + sum(n,UP_COMM, PSHUP(n,r) * INCPAR(n,r) * ALPHA(n,r))]
   1717           + [ALPHA(i,r) - sum(n,UP_COMM, PSHUP(n,r) * ALPHA(n,r))];
   1718    
   1719   Coefficient (all,i,UP_COMM)(all,k,UP_COMM)(all,r,REG)
   1720       EP(i,k,r)
   1721       # uncomp. elast. of private hhld demand for i wrt price of k in r (HT F5) #;
   1722   Formula (all,i,UP_COMM)(all,k,UP_COMM)(all,r,REG)
   1723       EP(i,k,r) = 0;
   1724   Formula (all,i,UP_COMM)(all,k,UP_COMM)(all,r,REG)
   1725       EP(i,k,r) = [APE(i,k,r) - EY(i,r)] * PSHUP(k,r);
   1726    
   1727   Equation PRIVDMNDS
   1728   # private consumption demands for composite commodities (HT 46) #
   1729   (all,i,UP_COMM)(all,r,REG)
   1730       qp(i,r) - pop(r)
   1731           = sum(k,UP_COMM, EP(i,k,r) * pp(k,r)) + EY(i,r) * [yp(r) - pop(r)];
   1732   !<
   1733       Private consumption demands for composite commodities.  Demand system is
   1734       on a per capita basis.  Here, yp(r) - pop(r) is % change in per capita
   1735       income.  (HT 46)
   1736   >!
   1737    
   1738   Equation PHHLDINDEX
   1739   # price index for private consumption expenditure #
   1740   (all,r,REG)
   1741       ppriv(r) = sum(i,UP_COMM, PSHUP(i,r) * pp(i,r));
   1742    
   1743   Equation PRIVATEU
   1744   # computation of utility from private consumption in r (HT 45) #
   1745   (all,r,REG)
   1746       yp(r) - pop(r) = ppriv(r) + UELASPRIV(r) * up(r);
   1747   !<
   1748       This equation determines private consumption utility for a representative
   1749       household in region r, based on the per capita private expenditure
   1750       function.  (HT 45)
   1751   >!
   1752    
   1753   Coefficient (all,i,UP_COMM)(all,r,REG)
   1754       XWCONSHR(i,r)
   1755       # expansion-parameter-weighted consumption share #;
   1756   Formula (all,i,UP_COMM)(all,r,REG)
   1757       XWCONSHR(i,r) = PSHUP(i,r) * INCPAR(i,r) / UELASPRIV(r);
   1758    
   1759   Equation UTILELASPRIV
   1760   # elasticity of expenditure wrt utility from private consumption #
   1761   (all,r,REG)
   1762       uepriv(r)
   1763           = sum(i,UP_COMM, XWCONSHR(i,r) * [pp(i,r) + qp(i,r) - yp(r)]);
   1764    
