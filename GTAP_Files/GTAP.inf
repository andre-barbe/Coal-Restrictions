   3892   !<
   3893       Common Variables and Coefficients
   3894       .................................
   3895    
   3896       only used in this Aggregate Trade Indices section
   3897   >!
   3898    
   3899   Variable (orig_level=1.0)(all,i,TRAD_COMM)(all,r,REG)
   3900       pxw(i,r) # aggregate exports price index of i from region r #;
   3901    
   3902   Coefficient (all,i,TRAD_COMM)
   3903       VXWCOMMOD(i) # value of world exports by commodity i at FOB prices #;
   3904   Formula (all,i,TRAD_COMM)
   3905       VXWCOMMOD(i) = sum(r,REG, VXW(i,r));
   3906    
   3907   Coefficient
   3908       VXWLD # value of commodity exports, FOB, globally #;
   3909   Formula
   3910       VXWLD = sum(r,REG, VXWREGION(r));
   3911    
   3912   Coefficient (all,i,TRAD_COMM)
   3913       VIWCOMMOD(i) # global value of commodity imports, CIF, by commodity #;
   3914   Formula (all,i,TRAD_COMM)
   3915       VIWCOMMOD(i) = sum(r,REG, VIW(i,r));
   3916    
   3917   Coefficient (all,i,TRAD_COMM)(all,r,REG)
   3918       PW_PM(i,r) # ratio of world to domestic prices #;
   3919   Formula (all,i,TRAD_COMM)(all,r,REG)
   3920       PW_PM(i,r) = sum(s,REG, VXWD(i,r,s)) / sum(s,REG, VXMD(i,r,s));
   3921   Coefficient (all,i,TRAD_COMM)(all,r,REG)
   3922       VOW(i,r) # value of output in r at FOB including transportation services #;
   3923   Formula (all,m,MARG_COMM)(all,r,REG)
   3924       VOW(m,r) = VDM(m,r) * PW_PM(m,r) + sum(s,REG, VXWD(m,r,s)) + VST(m,r);
   3925   Formula (all,i,NMRG_COMM)(all,r,REG)
   3926       VOW(i,r) = VDM(i,r) * PW_PM(i,r) + sum(s,REG, VXWD(i,r,s));
   3927    
   3928   Coefficient (all,i,TRAD_COMM)
   3929       VWOW(i) # value of world supply at world prices for i #;
   3930   Formula (all,i,TRAD_COMM)
   3931       VWOW(i) = sum(r,REG, VOW(i,r));
   3932    
   3933   Coefficient (all,i,TRAD_COMM)
   3934       VWOU(i) # value of world output of i at user prices #;
   3935   Formula (all,i,TRAD_COMM)
   3936       VWOU(i) = sum(s,REG, VPAT(i,s) + VGAT(i,s)
   3937           + sum(j,PROD_COMM, VFAD(i,j,s)));
   3938    
   3939    
   3940   !<
   3941       Value Indices for Aggregate Trade
   3942       .................................
   3943   >!
   3944    
   3945   Equation VREGEX_ir_MARG
   3946   # the change in FOB value of exports of m from r #
   3947   (all,m,MARG_COMM)(all,r,REG)
   3948       VXW(m,r) * vxwfob(m,r)
   3949           = sum(s,REG, VXWD(m,r,s) * [qxs(m,r,s) + pfob(m,r,s)])
   3950           + VST(m,r) * [qst(m,r) + pm(m,r)];
   3951   Equation VREGEX_ir_NMRG
   3952   # the change in FOB value of exports of commodity i from r (HT 73) #
   3953   (all,i,NMRG_COMM)(all,r,REG)
   3954       VXW(i,r) * vxwfob(i,r)
   3955           = sum(s,REG, VXWD(i,r,s) * [qxs(i,r,s) + pfob(i,r,s)]);
   3956    
   3957   Equation VREGEX_r
   3958   # computes % change in value of merchandise exports, by region (HT 75) #
   3959   (all,r,REG)
   3960       VXWREGION(r) * vxwreg(r) = sum(i,TRAD_COMM, VXW(i,r) * vxwfob(i,r));
   3961    
   3962   Variable (all,i,TRAD_COMM)
   3963       vxwcom(i) # value of global merchandise exports by commodity #;
   3964   Equation VWLDEX_i
   3965   # computes % change in FOB value of global exports, by commodity (HT 77) #
   3966   (all,i,TRAD_COMM)
   3967       VXWCOMMOD(i) * vxwcom(i) = sum(r,REG, VXW(i,r) * vxwfob(i,r));
   3968    
   3969   Variable
   3970       vxwwld # value of world trade #;
   3971   Equation VWLDEX
   3972   # computes % change in value of global exports (HT 79) #
   3973       VXWLD * vxwwld = sum(r,REG, VXWREGION(r) * vxwreg(r));
   3974    
   3975   Equation VREGIM_is
   3976   # the change in CIF value of imports of commodity i into s (HT 74) #
   3977   (all,i,TRAD_COMM)(all,s,REG)
   3978       VIW(i,s) * viwcif(i,s)
   3979           = sum(r,REG, VIWS(i,r,s) * [pcif(i,r,s) + qxs(i,r,s)]);
   3980    
   3981   Equation VREGIM_s
   3982   # computes % change in value of imports, CIF basis, by region (HT 76) #
   3983   (all,s,REG)
   3984       VIWREGION(s) * viwreg(s) = sum(i,TRAD_COMM, VIW(i,s) * viwcif(i,s));
   3985    
   3986   Variable (all,i,TRAD_COMM)
   3987       viwcom(i) # value of global merchandise imports i, at world prices #;
   3988   Equation VWLDIM_i
   3989   # computes % change in value of global imports, by commodity (HT 78) #
   3990   (all,i,TRAD_COMM)
   3991       VIWCOMMOD(i) * viwcom(i) = sum(s,REG, VIW(i,s) * viwcif(i,s));
   3992    
   3993   Variable (all,i,TRAD_COMM)
   3994       valuew(i) # value of world supply of good i #;
   3995   Equation VWLDOUT
   3996   # change in value of world output of comm. i at FOB prices (HT 80) #
   3997   (all,i,TRAD_COMM)
   3998       VWOW(i) * valuew(i) = sum(r,REG, VOW(i,r) * [pxw(i,r) + qo(i,r)]);
   3999    
   4000   Variable (all,i,TRAD_COMM)
   4001       valuewu(i) # value of world supply of good i at user prices #;
   4002   Equation VWLDOUTUSE
   4003   # change in value of world output of commodity i at user prices #
   4004   (all,i,TRAD_COMM)
   4005       VWOU(i) * valuewu(i)
   4006           = sum(s,REG, VPAT(i,s) * [ppt(i,s) + qpt(i,s)]
   4007           + VGAT(i,s) * [pgt(i,s) + qgt(i,s)]
   4008           + sum(j,PROD_COMM, VFAD(i,j,s) * [pft(i,j,s) + qft(i,j,s)]));
   4009    
