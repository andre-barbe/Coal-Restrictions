   5068   !<
   5069       -------------------------------
   5070       D. Terms of Trade Decomposition
   5071       -------------------------------
   5072    
   5073       Computations for decomposition the terms-of-trade effect
   5074       Reference: Robert McDougall, SALTER No. 12 Working Paper
   5075   >!
   5076    
   5077   Coefficient (all,i,TRAD_COMM)(all,r,REG)
   5078       SX_IR(i,r) # share of good i in total exports from r #;
   5079   Formula (all,m,MARG_COMM)(all,r,REG)
   5080       SX_IR(m,r)
   5081           = [sum(s,REG, VXWD(m,r,s)) + VST(m,r)]
   5082           / [sum(k,TRAD_COMM, sum(s,REG, VXWD(k,r,s)))
   5083               + sum(l,MARG_COMM, VST(l,r))];
   5084   Formula (all,i,NMRG_COMM)(all,r,REG)
   5085       SX_IR(i,r)
   5086           = sum(s,REG, VXWD(i,r,s))
   5087           / [sum(k,TRAD_COMM, sum(s,REG, VXWD(k,r,s)))
   5088               + sum(l,MARG_COMM, VST(l,r))];
   5089    
   5090   Coefficient (all,m,MARG_COMM)(all,s,REG)
   5091       VTICOMM(m,s) # margin usage of m in getting imports to region s #;
   5092   Formula (all,m,MARG_COMM)(all,s,REG)
   5093       VTICOMM(m,s) = sum(i,TRAD_COMM, sum(r,REG, VTMFSD(m,i,r,s)));
   5094    
   5095   Coefficient (all,i,TRAD_COMM)(all,s,REG)
   5096       VIWDIRALL(i,s) # imports of i into s, with direct allocation of margins #;
   5097   Formula (all,m,MARG_COMM)(all,s,REG)
   5098       VIWDIRALL(m,s) = sum(r,REG, VXWD(m,r,s)) + VTICOMM(m,s);
   5099   Formula (all,i,NMRG_COMM)(all,s,REG)
   5100       VIWDIRALL(i,s) = sum(r,REG, VXWD(i,r,s));
   5101    
   5102   Coefficient (all,s,REG)
   5103       VIWDATOT(s) # total imports into s, calculated on direct allocation basis #;
   5104   Formula (all,s,REG)
   5105       VIWDATOT(s) = sum(i,TRAD_COMM, VIWDIRALL(i,s));
   5106    
   5107   Coefficient (all,i,TRAD_COMM)(all,r,REG)
   5108       SM_IR(i,r)  # share of good i in total imports into r #;
   5109   Formula (all,i,TRAD_COMM)(all,s,REG)
   5110       SM_IR(i,s) = VIWDIRALL(i,s) / VIWDATOT(s);
   5111    
   5112   Zerodivide (zero_by_zero) default 0;
   5113   Coefficient (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   5114       SX_IRS(i,r,s) # share of exports of good i from region r to s #;
   5115   Formula (all,m,MARG_COMM)(all,r,REG)(all,s,REG)
   5116       SX_IRS(m,r,s) = VXWD(m,r,s) / [sum(k,REG, VXWD(m,r,k)) + VST(m,r)];
   5117   Formula (all,i,NMRG_COMM)(all,r,REG)(all,s,REG)
   5118       SX_IRS(i,r,s) = VXWD(i,r,s) / sum(k,REG, VXWD(i,r,k));
   5119   Zerodivide (zero_by_zero) off;
   5120    
   5121   Coefficient (all,m,MARG_COMM)(all,r,REG)
   5122       SXT_IR(m,r) # share of margins in exports of good i from region r #;
   5123   Formula (all,m,MARG_COMM)(all,r,REG)
   5124       SXT_IR(m,r) = VST(m,r) / [sum(k,REG, VXWD(m,r,k)) + VST(m,r)];
   5125    
   5126   Variable (all,i,TRAD_COMM)(all,r,REG)
   5127       px_ir(i,r) # export price index for good i and region r #;
   5128   Equation EXPPRICE_MARG
   5129   # price index for total exports of m from r #
   5130   (all,m,MARG_COMM)(all,r,REG)
   5131       px_ir(m,r)
   5132           = sum(s,REG, SX_IRS(m,r,s) * pfob(m,r,s)) + SXT_IR(m,r) * pm(m,r);
   5133   Equation EXPPRICE_NMRG
   5134   # price index for total exports of i from r #
   5135   (all,i,NMRG_COMM)(all,r,REG)
   5136       px_ir(i,r) = sum(s,REG, SX_IRS(i,r,s) * pfob(i,r,s));
   5137    
   5138   Coefficient (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   5139       SM_IRS(i,r,s) # share of imports of good i into s from r at FOB prices #;
   5140   Formula (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
   5141       SM_IRS(i,r,s) = VXWD(i,r,s) / VIWDIRALL(i,s);
   5142    
   5143   Coefficient (all,m,MARG_COMM)(all,r,REG)
   5144       SMT_IR(m,r) # share of transport cost in imports of margin commodity #;
   5145   Formula (all,m,MARG_COMM)(all,s,REG)
   5146       SMT_IR(m,s) = VTICOMM(m,s) / VIWDIRALL(m,s);
   5147    
   5148   Variable (all,i,TRAD_COMM)(all,r,REG)
   5149       pm_ir(i,r) # imports price index for good i and region r #;
   5150   Equation IMPPRICE1_MARG
   5151   # price index for total imports of m in s -- margins commodities #
   5152   (all,m,MARG_COMM)(all,s,REG)
   5153       pm_ir(m,s)
   5154           = sum(r,REG, SM_IRS(m,r,s) * pfob(m,r,s)) + SMT_IR(m,s) * pt(m);
   5155   Equation IMPPRICE1_NMRG
   5156   # price index for total imports of i in s -- non-margins commodities #
   5157   (all,i,NMRG_COMM)(all,s,REG)
   5158       pm_ir(i,s) = sum(r,REG, SM_IRS(i,r,s) * pfob(i,r,s));
   5159    
   5160   Coefficient (all,i,TRAD_COMM)(all,r,REG)
   5161       SW_IR(i,r) # share of region r exports in world total for good i #;
   5162   Formula (all,m,MARG_COMM)(all,r,REG)
   5163       SW_IR(m,r)
   5164           = [sum(s,REG, VXWD(m,r,s)) + VST(m,r)]
   5165           / sum(k,REG, sum(s,REG, VXWD(m,k,s)) + VST(m,k));
   5166   Formula (all,i,NMRG_COMM)(all,r,REG)
   5167       SW_IR(i,r)
   5168           = sum(s,REG, VXWD(i,r,s)) / sum(k,REG, sum(s,REG, VXWD(i,k,s)));
   5169    
   5170   Variable (all,i,TRAD_COMM)
   5171       px_i(i) # world export price index for commodity i #;
   5172   Equation WRLDPRICEi
   5173   # world export price index for good i #
   5174   (all,i,TRAD_COMM)
   5175       px_i(i) = sum(r,REG, SW_IR(i,r) * px_ir(i,r));
   5176    
   5177   Coefficient (all,i,TRAD_COMM)
   5178       SW_I(i) # share of exports of i in world total #;
   5179   Formula (all,m,MARG_COMM)
   5180       SW_I(m)
   5181           = sum(r,REG, sum(s,REG, VXWD(m,r,s)) + VST(m,r))
   5182           / [sum(k,TRAD_COMM, sum(r,REG, sum(s,REG, VXWD(k,r,s))))
   5183               + sum(l,MARG_COMM, sum(r,REG, VST(l,r)))];
   5184   Formula (all,i,NMRG_COMM)
   5185       SW_I(i)
   5186           = sum(r,REG, sum(s,REG, VXWD(i,r,s)))
   5187           / [sum(k,TRAD_COMM, sum(r,REG, sum(s,REG, VXWD(k,r,s))))
   5188               + sum(l,MARG_COMM, sum(r,REG, VST(l,r)))];
   5189    
   5190   Variable
   5191       px_ # world export price index for all commodities #;
   5192   Equation WRLDPRICE
   5193   # world export price index for all goods #
   5194       px_ = sum(i,TRAD_COMM, SW_I(i) * px_i(i));
   5195    
   5196   Variable (all,i,TRAD_COMM)(all,r,REG)
   5197       c1_ir(i,r) # contribution of world price, px_i, to ToT #;
   5198   Equation c1_irEQ
   5199   # contribution of world export price index of good i to ToT for region r #
   5200   (all,i,TRAD_COMM)(all,r,REG)
   5201       c1_ir(i,r) = [SX_IR(i,r) - SM_IR(i,r)] * [px_i(i) - px_];
   5202    
   5203   Variable (all,i,TRAD_COMM)(all,r,REG)
   5204       c2_ir(i,r) # contribution of regional export price, px_ir, to ToT #;
   5205   Equation c2_irEQ
   5206   # contribution of regional export price of good i for region r #
   5207   (all,i,TRAD_COMM)(all,r,REG)
   5208       c2_ir(i,r) = SX_IR(i,r) * [px_ir(i,r) - px_i(i)];
   5209    
   5210   Variable (all,i,TRAD_COMM)(all,r,REG)
   5211       c3_ir(i,r) # contribution of regional import price, pm_ir, to ToT #;
   5212   Equation c3_irEQ
   5213   # contribution of imports price index of good i for region r #
   5214   (all,i,TRAD_COMM)(all,r,REG)
   5215       c3_ir(i,r) = SM_IR(i,r) * [pm_ir(i,r) - px_i(i)];
   5216    
   5217   Variable (all,r,REG)
   5218       c1_r(r) # contribution of world prices for all goods to ToT #;
   5219   Equation c1_rEQ
   5220   # contribution of world price indexes of all goods to ToT for r #
   5221   (all,r,REG)
   5222       c1_r(r) = sum(i,TRAD_COMM, c1_ir(i,r));
   5223    
   5224   Variable (all,r,REG)
   5225       c2_r(r) # contribution of regional export prices to ToT #;
   5226   Equation c2_rEQ
   5227   # contribution of regional exports prices to ToT for r #
   5228   (all,r,REG)
   5229       c2_r(r) = sum(i,TRAD_COMM, c2_ir(i,r));
   5230    
   5231   Variable (all,r,REG)
   5232       c3_r(r) # contribution of regional import prices to ToT #;
   5233   Equation c3_rEQ
   5234   # contribution of regional import prices to ToT for r #
   5235   (all,r,REG)
   5236       c3_r(r) = sum(i,TRAD_COMM, c3_ir(i,r));
   5237    
   5238   Variable (all,r,REG)
   5239       tot2(r) # trade terms for region r, computed from components #;
   5240   Equation TOT2eq
   5241   # trade terms for region r, computed from components #
   5242   (all,r,REG)
   5243       tot2(r) = c1_r(r) + c2_r(r) - c3_r(r);
   5244    
   5245   !<
   5246       ====================
   5247       Code to allow for a policy induced reduction in the usage of a particular
   5248       input by a particular industry, either in absolute quantity or relative intensity
   5249       added by Andre Barbe April 1, 2015
   5250       ====================
   5251   >!
   5252    
   5253   !The basic idea is to 1. calculate the shadow input price which would give the
   5254   specified reduction in quantity used of the restricted input (note that this
   5255   requires calculating the price of the output induced by these shadow input prices)
   5256   2. use those shadow prices to calculate the actual quantities used of each input.
   5257   3. Use those actual input quantities to calculate actual output prices!
   5258    
   5259   Variable (all,i,FIRM_COMM)(all,j,PROD_COMM)(all,r,REG)
   5260       pf_s(i,j,r) #shadow (after quantity restriction) price  for commodity i for use by j in r #;
   5261    
   5262   Variable (all,i,SUBF_COMM)(all,j,PROD_COMM)(all,r,REG)
   5263       pf_so(i,j,r) #shadow price  for ouptut commodity i for use by j in r whose inputs have a quantity restrition#;
   5264    
   5265   Variable (all,i,FIRM_COMM)(all,j,PROD_COMM)(all,r,REG)
   5266       pf_slack(i,j,r) # firm's slack variable for commodity i for use by j in r.#;
   5267       !ps_slack is non-zero if there is a quantity restriction for
   5268       this particular ijr.!
   5269    
   5270    
   5271   Equation pf_sBINDING
   5272   # relates the shadow and real price of commodities i for use by j in r #
   5273   (all,i,FIRM_COMM)(all,j,PROD_COMM)(all,r,REG)
   5274       pf_s(i,j,r)=pf(i,j,r) + pf_slack(i,j,r);
   5275       !note that while a slack variable technically exists for all FIRM_COMM,
   5276       it should only be endogenized for inputs of the 5 outputs shown below.
   5277       If other inputs are endogenized, nothing will happen, as those pf_s don't
   5278       show up in any other equations, only the pf do !
   5279    
   5280   !Useful sets!
   5281   !These sets exists to make defining the pf_slack closure more compact!
   5282   !because I need to explicitly list each set that is exogeneous!
   5283    
   5284   Set
   5285       NEP_COMM # non-energy producers # = PROD_COMM - EGY_COMM;
   5286   Subset
   5287       NEP_COMM is subset of PROD_COMM;
Declaration of 'NEP_COMM' as a SUBSET of 'PROD_COMM'.
%% Information only.
    This subset relation is a consequence of earlier subset declarations.
   5288   Set
   5289       NEF_COMM # non-energy firm commodities # = FIRM_COMM - EGY_COMM;
   5290   Subset
   5291       NEF_COMM is subset of FIRM_COMM;
Declaration of 'NEF_COMM' as a SUBSET of 'FIRM_COMM'.
%% Information only.
    This subset relation is a consequence of earlier subset declarations.
   5292    
   5293   Set
   5294       US_REG # Regions that are the US # (USA);
   5295   Subset
   5296       US_REG is subset of REG;
   5297   Set
   5298       NUS_REG # Regions that are not the US # = REG - US_REG;
   5299   Subset
   5300       NUS_REG is subset of REG;
Declaration of 'NUS_REG' as a SUBSET of 'REG'.
%% Information only.
    This subset relation is a consequence of earlier subset declarations.
   5301    
   5302   Set
   5303       INSUBF_COMM # set of all inputs used in producing the SUBF commodities # =
   5304           NCOAL_FCOMM + NELY_FCOMM + ENY_FCOMM + KEN_FCOMM + VAEN_FCOMM;
   5305   Subset
   5306       INSUBF_COMM is subset of FIRM_COMM;
   5307    
   5308    
   5309    
   5310   !Quantity Demand Equations!
   5311   !I commented out the old qf equations for the inputs and added the updated
   5312   ones below !
   5313   !They are updated by replacing the input prices with the input shadow prices
   5314   and the output prices with the output shadow prices!
   5315   Equation NCOALFDEMAND
   5316   # demand for inputs into non-coal energy subproduction #
   5317   (all,i,NCOAL_FCOMM)(all,j,PROD_COMM)(all,r,REG)
   5318       qf(i,j,r) = -af(i,j,r) + qf("ncoal",j,r)
   5319           - ELFNCOAL(j,r) * [pf_s(i,j,r) - af(i,j,r) - pf_so("ncoal",j,r)];
   5320    
   5321   Equation NELYFDEMAND
   5322   # demand for inputs into non-electricity energy subproduction #
   5323   (all,i,NELY_FCOMM)(all,j,PROD_COMM)(all,r,REG)
   5324       qf(i,j,r) = -af(i,j,r) + qf("nely",j,r)
   5325           - ELFNELY(j,r) * [pf_s(i,j,r) - af(i,j,r) - pf_so("nely",j,r)];
   5326    
   5327   Equation ENYFDEMAND # demand for inputs into energy subproduction #
   5328   (all,i,ENY_FCOMM)(all,j,PROD_COMM)(all,r,REG)
   5329       qf(i,j,r) = -af(i,j,r) + qf("eny",j,r)
   5330           - ELFENY(j,r) * [pf_s(i,j,r) - af(i,j,r) - pf_so("eny",j,r)];
   5331    
   5332   Equation KENFDEMAND # demand for inputs into capital-energy subproduction #
   5333   (all,i,KEN_FCOMM)(all,j,PROD_COMM)(all,r,REG)
