   1888    
   1889   !<
   1890       3-1. Factor Employment Taxation
   1891       -------------------------------
   1892   >!
   1893    
   1894   Variable (all,i,ENDW_COMM)(all,j,PROD_COMM)(all,r,REG)
   1895       tf(i,j,r) # tax on primary factor i used by j in region r #;
   1896   Equation MPFACTPRICE # eq'n links domestic and firm demand prices (HT 16) #
   1897   (all,i,ENDWM_COMM)(all,j,PROD_COMM)(all,r,REG)
   1898       pfe(i,j,r) = tf(i,j,r) + pm(i,r);
   1899    
   1900   Equation SPFACTPRICE # eq'n links domestic and firm demand prices (HT 17) #
   1901   (all,i,ENDWS_COMM)(all,j,PROD_COMM)(all,r,REG)
   1902       pfe(i,j,r) = tf(i,j,r) + pmes(i,j,r);
   1903    
   1904   Equation TFURATIO
   1905   # change in ratio of tax payments on factor usage to regional income #
   1906   (all,r,REG)
   1907       100.0 * INCOME(r) * del_taxrfu(r) + TFU(r) * y(r)
   1908           = sum(i,ENDWM_COMM, sum(j,PROD_COMM, VFAD(i,j,r) * tf(i,j,r)))
   1909           + sum(i,ENDWM_COMM, sum(j,PROD_COMM,
   1910               ETAX(i,j,r) * [pm(i,r) + qfe(i,j,r)]))
   1911           + sum(i,ENDWS_COMM, sum(j,PROD_COMM, VFAD(i,j,r) * tf(i,j,r)))
   1912           + sum(i,ENDWS_COMM, sum(j,PROD_COMM,
   1913               ETAX(i,j,r) * [pmes(i,j,r) + qfe(i,j,r)]));
   1914    
   1915    
   1916   !<
   1917       3-2. Composite Intermediates Nest
   1918       ---------------------------------
   1919   >!
   1920    
   1921   Zerodivide (zero_by_zero) default 1;
   1922   Coefficient (all,i,TRAD_COMM)(all,j,PROD_COMM)(all,r,REG)
   1923       SHVDFANC(i,j,r)
   1924       # share of carbon-tax-excl. value of domestic i for use by j in region r #;
   1925   Formula (all,i,TRAD_COMM)(all,j,PROD_COMM)(all,r,REG)
   1926       SHVDFANC(i,j,r) = VDFANC(i,j,r) / VDFA(i,j,r);
   1927   Zerodivide (zero_by_zero) off;
   1928    
   1929   Zerodivide (zero_by_zero) default 0;
   1930   Coefficient (all,i,TRAD_COMM)(all,j,PROD_COMM)(all,r,REG)
   1931       CO2DFVDFA(i,j,r)
   1932       # emissions intensity of domestic i for use by j in region r #;
   1933   Formula (all,i,TRAD_COMM)(all,j,PROD_COMM)(all,r,REG)
   1934       CO2DFVDFA(i,j,r) = CO2DF(i,j,r) / VDFA(i,j,r);
   1935   Zerodivide (zero_by_zero) off;
   1936    
   1937   Equation DMNDDPRICE
   1938   # taxation of intermediate usage of domestic product #
   1939   (all,i,TRAD_COMM)(all,j,PROD_COMM)(all,r,REG)
   1940       pfd(i,j,r) = SHVDFANC(i,j,r) * (pm(i,r) + tfd(i,j,r))
   1941           + 100.0 * CO2DFVDFA(i,j,r) * NCTAXB(REGTOBLOC(r));
   1942    
   1943   Zerodivide (zero_by_zero) default 1;
   1944   Coefficient (all,i,TRAD_COMM)(all,j,PROD_COMM)(all,r,REG)
   1945       SHVIFANC(i,j,r)
   1946       # share of carbon-tax-excl. value of imported i for use by j in region r #;
   1947   Formula (all,i,TRAD_COMM)(all,j,PROD_COMM)(all,r,REG)
   1948       SHVIFANC(i,j,r) = VIFANC(i,j,r) / VIFA(i,j,r);
   1949   Zerodivide (zero_by_zero) off;
   1950    
   1951   Zerodivide (zero_by_zero) default 0;
   1952   Coefficient (all,i,TRAD_COMM)(all,j,PROD_COMM)(all,r,REG)
   1953       CO2IFVIFA(i,j,r)
   1954       # emissions intensity of imported i for use by j in region r #;
   1955   Formula (all,i,TRAD_COMM)(all,j,PROD_COMM)(all,r,REG)
   1956       CO2IFVIFA(i,j,r) = CO2IF(i,j,r) / VIFA(i,j,r);
   1957   Zerodivide (zero_by_zero) off;
   1958    
   1959   Equation DMNDIPRICES
   1960   # taxation of intermediate usage of imports #
   1961   (all,i,TRAD_COMM)(all,j,PROD_COMM)(all,r,REG)
   1962       pfm(i,j,r) = SHVIFANC(i,j,r) * (pim(i,r) + tfm(i,j,r))
   1963           + 100.0 * CO2IFVIFA(i,j,r) * NCTAXB(REGTOBLOC(r));
   1964    
   1965   Equation TIURATIO
   1966   # change in ratio of tax payments on intermediate goods to regional income #
   1967   (all,r,REG)
   1968       100.0 * INCOME(r) * del_taxriu(r) + TIU(r) * y(r)
   1969           = sum(i,TRAD_COMM, sum(j,PROD_COMM,
   1970               VDFANC(i,j,r) * tfd(i,j,r)
   1971                   + DFTAX(i,j,r) * [pm(i,r) + qfd(i,j,r)]))
   1972           + sum(i,TRAD_COMM, sum(j,PROD_COMM,
   1973               VIFANC(i,j,r) * tfm(i,j,r)
   1974                   + IFTAX(i,j,r) * [pim(i,r) + qfm(i,j,r)]));
   1975    
   1976   Coefficient (all,i,TRAD_COMM)(all,j,PROD_COMM)(all,s,REG)
   1977       FMSHR(i,j,s) # share of firms' imports in dom. composite, agent's prices #;
   1978   Formula (all,i,TRAD_COMM)(all,j,PROD_COMM)(all,s,REG)
   1979       FMSHR(i,j,s) = VIFA(i,j,s) / VFAD(i,j,s);
   1980   Equation ICOMPRICE
   1981   # industry price for composite commodities (HT 30) #
   1982   (all,i,TRAD_COMM)(all,j,PROD_COMM)(all,r,REG)
   1983       pft(i,j,r) = FMSHR(i,j,r) * pfm(i,j,r) + [1 - FMSHR(i,j,r)] * pfd(i,j,r);
   1984    
   1985   Equation INDIMP
   1986   # industry j demands for composite import i (HT 31) #
   1987   (all,i,TRAD_COMM)(all,j,PROD_COMM)(all,s,REG)
   1988       qfm(i,j,s) = qft(i,j,s) - ESUBD(i) * [pfm(i,j,s) - pft(i,j,s)];
   1989    
   1990   Equation INDDOM
   1991   # industry j demands for domestic good i (HT 32) #
   1992   (all,i,TRAD_COMM)(all,j,PROD_COMM)(all,s,REG)
   1993       qfd(i,j,s) = qft(i,j,s) - ESUBD(i) * [pfd(i,j,s) - pft(i,j,s)];
   1994    
   1995    
   1996   !<
   1997       3-3. Notational Links
   1998       ---------------------
   1999   >!
   2000    
   2001   Formula (all,i,DEMD_COMM)(all,j,PROD_COMM)(all,r,REG)
   2002       VFA(i,j,r) = VFAD(i,j,r);
   2003    
   2004   Equation PFEEQ # Input endowment prices into the production system. #
   2005   (all,i,ENDW_COMM)(all,j,PROD_COMM)(all,r,REG)
   2006       pf(i,j,r) = pfe(i,j,r);
   2007    
   2008   Equation PFTEQ # Input tradables prices into the production system. #
   2009   (all,i,TRAD_COMM)(all,j,PROD_COMM)(all,r,REG)
   2010       pf(i,j,r) = pft(i,j,r);
   2011    
   2012   Equation QFEEQ # Output endowment quantities from the production system. #
   2013   (all,i,ENDW_COMM)(all,j,PROD_COMM)(all,r,REG)
   2014       qfe(i,j,r) = qf(i,j,r);
   2015    
   2016   Equation QFTEQ # Output tradables quantities from the production system. #
   2017   (all,i,TRAD_COMM)(all,j,PROD_COMM)(all,r,REG)
   2018       qft(i,j,r) = qf(i,j,r);
   2019    
