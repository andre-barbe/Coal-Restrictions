    884    
    885   Coefficient (all,i,DEMD_COMM)(all,j,PROD_COMM)(all,r,REG)
    886       VFAD(i,j,r) # producer expend. on i by j in r valued at agent's prices #;
    887   Formula (all,i,ENDW_COMM)(all,j,PROD_COMM)(all,r,REG)
    888       VFAD(i,j,r) = EVFA(i,j,r);
    889   Formula (all,i,TRAD_COMM)(all,j,PROD_COMM)(all,s,REG)
    890       VFAD(i,j,s) = VDFA(i,j,s) + VIFA(i,j,s);
    891    
    892   Coefficient (all,i,NSAV_COMM)(all,r,REG)
    893       VOA(i,r) # value of commodity i output in region r at agent's prices #;
    894   Formula (all,i,ENDW_COMM)(all,r,REG)
    895       VOA(i,r) = EVOA(i,r);
    896   Formula (all,i,PROD_COMM)(all,r,REG)
    897       VOA(i,r) = sum(j,DEMD_COMM, VFAD(j,i,r));
    898    
    899   Coefficient (all,i,TRAD_COMM)(all,r,REG)
    900       VDM(i,r) # domestic sales of i in r at mkt prices (tradeables only) #;
    901   Formula (all,i,TRAD_COMM)(all,r,REG)
    902       VDM(i,r) = VDPM(i,r) + VDGM(i,r) + sum(j,PROD_COMM, VDFM(i,j,r));
    903    
    904   Coefficient (all,i,NSAV_COMM)(all,r,REG)
    905       VOM(i,r) # value of commodity i output in region r at market prices #;
    906   Formula (all,i,ENDW_COMM)(all,r,REG)
    907       VOM(i,r) = sum(j,PROD_COMM, VFM(i,j,r));
    908   Formula (all,m,MARG_COMM)(all,r,REG)
    909       VOM(m,r) = VDM(m,r) + sum(s,REG, VXMD(m,r,s)) + VST(m,r);
    910   Formula (all,i,NMRG_COMM)(all,r,REG)
    911       VOM(i,r) = VDM(i,r) + sum(s,REG, VXMD(i,r,s));
    912   Formula (all,h,CGDS_COMM)(all,r,REG)
    913       VOM(h,r) = VOA(h,r);
