   1102   Coefficient (all,r,REG)
   1103       TIM(r) # import tax payments in r #;
   1104   Formula (all,r,REG)
   1105       TIM(r) = sum(i,TRAD_COMM, sum(s,REG, MTAX(i,s,r)));
