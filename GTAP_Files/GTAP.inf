   1006       Taxes on Private Consumption
   1007       - - - - - - - - - - - - - -
   1008   >!
   1009    
   1010   Coefficient (all,i,TRAD_COMM)(all,r,REG)
   1011       DPTAX(i,r) # ordinary tax on private consumption of domestic product #;
   1012   Formula (all,i,TRAD_COMM)(all,r,REG)
   1013       DPTAX(i,r) = VDPANC(i,r) - VDPM(i,r);
   1014    
   1015   Coefficient (all,i,TRAD_COMM)(all,r,REG)
   1016       DPCTAX(i,r) # carbon tax on private consumption of domestic product #;
   1017   Formula (all,i,TRAD_COMM)(all,r,REG)
   1018       DPCTAX(i,r) = VDPA(i,r) - VDPANC(i,r);
   1019    
   1020   Coefficient (all,i,TRAD_COMM)(all,r,REG)
   1021       IPTAX(i,r) # ordinary tax on private consumption of imports #;
   1022   Formula (all,i,TRAD_COMM)(all,r,REG)
   1023       IPTAX(i,r) = VIPANC(i,r) - VIPM(i,r);
   1024   Coefficient (all,i,TRAD_COMM)(all,r,REG)
   1025       IPCTAX(i,r) # carbon tax on private consumption of imports #;
   1026   Formula (all,i,TRAD_COMM)(all,r,REG)
   1027       IPCTAX(i,r) = VIPA(i,r) - VIPANC(i,r);
   1028    
   1029   Coefficient (all,r,REG)
   1030       TPC(r) # ordinary tax on private consumption #;
   1031   Formula (all,r,REG)
   1032       TPC(r) = sum(i,TRAD_COMM, DPTAX(i,r) + IPTAX(i,r));
