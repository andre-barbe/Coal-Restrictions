   3725   !<
   3726       A-1. Factor Price Indices
   3727       -------------------------
   3728   >!
   3729    
   3730   Variable (orig_level=1.0)(all,i,ENDW_COMM)(all,r,REG)
   3731       pfactreal(i,r) # ratio of return to primary factor i to CPI in r #;
   3732   Equation REALRETURN
   3733   # eq'n defines the real rate of return to primary factor i in region r #
   3734   (all,i,ENDW_COMM)(all,s,REG)
   3735       pfactreal(i,s) = pm(i,s) - ppriv(s);
   3736   !<
   3737       This equation defines the real rate of return to primary factor i in
   3738       region r (new).
   3739   >!
   3740    
   3741   Coefficient (all,r,REG)
   3742       VENDWREG(r) # value of primary factors, at mkt prices, by region #;
   3743   Formula (all,r,REG)
   3744       VENDWREG(r) = sum(i,ENDW_COMM, VOM(i,r));
   3745    
   3746   Variable (orig_level=1.0)(all,r,REG)
   3747       pfactor(r) # market price index of primary factors, by region #;
   3748   Equation PRIMFACTPR
   3749   # computes % change in price index of primary factors, by region #
   3750   (all,r,REG)
   3751       VENDWREG(r) * pfactor(r) = sum(i,ENDW_COMM, VOM(i,r) * pm(i,r));
   3752    
   3753   Coefficient
   3754       VENDWWLD # value of primary factors, at mkt prices, worldwide #;
   3755   Formula
   3756       VENDWWLD = sum(r,REG, VENDWREG(r));
   3757   Variable (orig_level=1.0)
   3758       pfactwld # world price index of primary factors #;
   3759   Equation PRIMFACTPRWLD
   3760   # computes % change in global price index of primary factors #
   3761       VENDWWLD * pfactwld = sum(r,REG, VENDWREG(r) * pfactor(r));
   3762    
