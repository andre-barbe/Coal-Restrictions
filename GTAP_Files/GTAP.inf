   4011   !<
   4012       Price Indices for Aggregate Trade
   4013       .................................
   4014   >!
   4015    
   4016   Equation PREGEX_ir_MARG
   4017   # change in FOB price index of exports of m from r #
   4018   (all,m,MARG_COMM)(all,r,REG)
   4019       VXW(m,r) * pxw(m,r)
   4020           = sum(s,REG, VXWD(m,r,s) * pfob(m,r,s)) + VST(m,r) * pm(m,r);
   4021    
   4022   Equation PREGEX_ir_NMRG
   4023   # change in FOB price index of exports of commodity i from r (HT 81) #
   4024   (all,i,NMRG_COMM)(all,r,REG)
   4025       VXW(i,r) * pxw(i,r) = sum(s,REG, VXWD(i,r,s) * pfob(i,r,s));
   4026    
   4027   Variable (orig_level=1.0)(all,r,REG)
   4028       pxwreg(r) # price index of merchandise exports, by region #;
   4029   Equation PREGEX_r
   4030   # computes % change in price index of exports, by region (HT 83) #
   4031   (all,r,REG)
   4032       VXWREGION(r) * pxwreg(r) = sum(i,TRAD_COMM, VXW(i,r) * pxw(i,r));
   4033    
   4034   Variable (orig_level=1.0)(all,i,TRAD_COMM)
   4035       pxwcom(i) # price index of global merchandise exports by commodity #;
   4036   Equation PWLDEX_i
   4037   # computes % change in price index of exports, by commodity (HT 85) #
   4038   (all,i,TRAD_COMM)
   4039       VXWCOMMOD(i) * pxwcom(i) = sum(r,REG, VXW(i,r) * pxw(i,r));
   4040    
   4041   Variable (orig_level=1.0)
   4042       pxwwld # price index of world trade #;
   4043   Equation PWLDEX
   4044   # computes % change in price index of global exports (HT 87) #
   4045       VXWLD * pxwwld = sum(r,REG, VXWREGION(r) * pxwreg(r));
   4046    
   4047   Variable (all,i,TRAD_COMM)(all,r,REG)
   4048       piw(i,r) # world price of composite import i in region r #;
   4049   Equation PREGIM_is
   4050   # change in CIF price index of imports of commodity i into s (HT 82) #
   4051   (all,i,TRAD_COMM)(all,s,REG)
   4052       VIW(i,s) * piw(i,s) = sum(r,REG, VIWS(i,r,s) * pcif(i,r,s));
   4053    
   4054   Variable (orig_level=1.0)(all,r,REG)
   4055       piwreg(r) # price index of merchandise imports, by region #;
   4056   Equation PREGIM_s
   4057   # computes % change in price index of imports, by region (HT 84) #
   4058   (all,s,REG)
   4059       VIWREGION(s) * piwreg(s) = sum(i,TRAD_COMM, VIW(i,s) * piw(i,s));
   4060    
   4061   Variable (orig_level=1.0)(all,i,TRAD_COMM)
   4062       piwcom(i) # price index of global merchandise imports by commodity #;
   4063   Equation PWLDIM_i
   4064   # computes % change in price index of imports, by commodity (HT 86) #
   4065   (all,i,TRAD_COMM)
   4066       VIWCOMMOD(i) * piwcom(i) = sum(s,REG, VIW(i,s) * piw(i,s));
   4067    
   4068   Variable (all,i,TRAD_COMM)
   4069       pw(i) # world price index for total good i supplies #;
   4070   Equation PWLDOUT
   4071   # change in index of world prices, FOB, for total production of i (HT 88) #
   4072   (all,i,TRAD_COMM)
   4073       VWOW(i) * pw(i) = sum(r,REG, VOW(i,r) * pxw(i,r));
   4074    
   4075   Variable (orig_level=1.0)(all,i,TRAD_COMM)
   4076       pwu(i) # world price index for total good i supplies at user prices #;
   4077   Equation PWLDUSE
   4078   # change in index of user prices for deflating world production of i #
   4079   (all,i,TRAD_COMM)
   4080       VWOU(i) * pwu(i)
   4081           = sum(s,REG, VPAT(i,s) * ppt(i,s)
   4082           + VGAT(i,s) * pgt(i,s)
   4083           + sum(j,PROD_COMM, VFAD(i,j,s) * pft(i,j,s)));
   4084    
