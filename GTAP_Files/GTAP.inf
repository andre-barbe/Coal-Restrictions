   3764   !<
   3765       A-2. Regional Terms of Trade
   3766       ----------------------------
   3767    
   3768       The next three equations correspond to Table 10 of Hertel and Tsigas on
   3769       Regional Terms of Trade.
   3770   >!
   3771    
   3772   Variable (orig_level=1.0)(all,r,REG)
   3773       psw(r) # index of prices received for tradeables produced in r #;
   3774   Equation REGSUPRICE
   3775   # estimate change in index of prices received for tradeables i produced in r #
   3776   (all,r,REG)
   3777       VXWREGION(r) * psw(r)
   3778           = sum(i,TRAD_COMM, sum(s,REG, VXWD(i,r,s) * pfob(i,r,s)))
   3779           + sum(m,MARG_COMM, VST(m,r) * pm(m,r));
   3780   !<
   3781       This equation estimates the change in the index of prices received for
   3782       tradeable products produced in r.  (modified from HT 64 to eliminate the
   3783       investment component)
   3784   >!
   3785    
   3786   Variable (orig_level=1.0)(all,r,REG)
   3787       pdw(r) # index of prices paid for tradeables used in region r #;
   3788   Equation REGDEMPRICE
   3789   # estimate change in index of prices paid for tradeable products used in r #
   3790   (all,r,REG)
   3791       VIWREGION(r) * pdw(r)
   3792           = sum(i,TRAD_COMM, sum(k,REG, VIWS(i,k,r) * pcif(i,k,r)));
   3793   !<
   3794       This equation estimates the change in the index of prices paid for
   3795       tradeable products used in r.  (modified from HT 65 to eliminate savings)
   3796   >!
   3797    
   3798   Variable (orig_level=1.0)(all,r,REG)
   3799       tot(r) # terms of trade for region r: tot(r) = psw(r) - pdw(r) #;
   3800   Equation TOTeq
   3801   # terms of trade equation computed as difference in psw and pdw (HT 66) #
   3802   (all,r,REG)
   3803       tot(r) = psw(r) - pdw(r);
   3804    
