   1211   !<
   1212       1-0. Module-Specific Variables
   1213       ------------------------------
   1214    
   1215       only used in this Government Consumption module
   1216   >!
   1217    
   1218   Set
   1219       SUBG_COMM # subutilities in government consumption #
   1220       (eny, neny);
   1221   Set
   1222       GOV_COMM # commodities demanded by government #
   1223       = TRAD_COMM union SUBG_COMM;
   1224    
   1225   Variable (all,i,GOV_COMM)(all,r,REG)
   1226       pg(i,r) # government consumption price for commodity i in region r #;
   1227   Variable (all,i,GOV_COMM)(all,r,REG)
   1228       qg(i,r) # government hhld demand for commodity i in region r #;
   1229   Variable (all,i,TRAD_COMM)(all,r,REG)
   1230       pgt(i,r) # government consumption price for commodity i in region r #;
   1231   Variable (all,i,TRAD_COMM)(all,r,REG)
   1232       qgt(i,r) # government hhld demand for commodity i in region r #;
   1233    
   1234   Coefficient (all,i,GOV_COMM)(all,r,REG)
   1235       VGA(i,r) # government expend. on i in r valued at agent's prices #;
   1236   Coefficient (all,i,SUBG_COMM)(all,r,REG)
   1237       VGAS(i,r) # government expend. on i in r valued at agent's prices #;
   1238    
   1239    
   1240   !<
   1241       1-1. Composite Tradeables
   1242       -------------------------
   1243   >!
   1244    
   1245   Zerodivide (zero_by_zero) default 1;
   1246   Coefficient (all,i,TRAD_COMM)(all,r,REG)
   1247       SHVDGANC(i,r)
   1248       # share of carbon-tax-excl. value of gov. consumption of domestic i in r #;
   1249   Formula (all,i,TRAD_COMM)(all,r,REG)
   1250       SHVDGANC(i,r) = VDGANC(i,r) / VDGA(i,r);
   1251   Zerodivide (zero_by_zero) off;
   1252    
   1253   Zerodivide (zero_by_zero) default 0;
   1254   Coefficient (all,i,TRAD_COMM)(all,r,REG)
   1255       CO2DGVDGA(i,r)
   1256       # emissions intensity of government consumption of domestic i in r #;
   1257   Formula (all,i,TRAD_COMM)(all,r,REG)
   1258       CO2DGVDGA(i,r) = CO2DG(i,r) / VDGA(i,r);
   1259   Zerodivide (zero_by_zero) off;
   1260    
   1261   Equation GHHDPRICE
   1262   # taxation of government consumption of domestic product #
   1263   (all,i,TRAD_COMM)(all,r,REG)
   1264       pgd(i,r)= SHVDGANC(i,r) * (pm(i,r) + tgd(i,r))
   1265           + 100.0 * CO2DGVDGA(i,r) * NCTAXB(REGTOBLOC(r));
   1266    
   1267   Zerodivide (zero_by_zero) default 1;
   1268   Coefficient (all,i,TRAD_COMM)(all,r,REG)
   1269       SHVIGANC(i,r)
   1270       # share of carbon-tax-excl. value of gov. consumption of imported i in r #;
   1271   Formula (all,i,TRAD_COMM)(all,r,REG)
   1272       SHVIGANC(i,r)=VIGANC(i,r) / VIGA(i,r);
   1273   Zerodivide (zero_by_zero) off;
   1274    
   1275   Zerodivide (zero_by_zero) default 0;
   1276   Coefficient (all,i,TRAD_COMM)(all,r,REG)
   1277       CO2IGVIGA(i,r)
   1278       # emissions intensity of government consumption of imported i in r #;
   1279   Formula (all,i,TRAD_COMM)(all,r,REG)
   1280       CO2IGVIGA(i,r) = CO2IG(i,r) / VIGA(i,r);
   1281   Zerodivide (zero_by_zero) off;
   1282    
   1283   Equation GHHIPRICES
   1284   # taxation of government consumption of imports #
   1285   (all,i,TRAD_COMM)(all,r,REG)
   1286       pgm(i,r) = SHVIGANC(i,r) * (pim(i,r) + tgm(i,r))
   1287           + 100.0 * CO2IGVIGA(i,r) * NCTAXB(REGTOBLOC(r));
   1288    
   1289   Coefficient (all,i,TRAD_COMM)(all,s,REG)
   1290       GMSHR(i,s) # share of imports for gov't hhld at agent's prices #;
   1291   Formula (all,i,TRAD_COMM)(all,s,REG)
   1292       GMSHR(i,s) = VIGA(i,s) / VGAT(i,s);
   1293    
   1294   Equation GCOMPRICE
   1295   # government consumption price for composite commodities (HT 42) #
   1296   (all,i,TRAD_COMM)(all,s,REG)
   1297       pgt(i,s) =  GMSHR(i,s) * pgm(i,s) + [1 - GMSHR(i,s)] * pgd(i,s);
   1298    
   1299   Equation GHHLDAGRIMP
   1300   # government consumption demand for aggregate imports (HT 43) #
   1301   (all,i,TRAD_COMM)(all,s,REG)
   1302       qgm(i,s) = qgt(i,s) + ESUBD(i) * [pgt(i,s) - pgm(i,s)];
   1303    
   1304   Equation GHHLDDOM
   1305   # government consumption demand for domestic goods (HT 44) #
   1306   (all,i,TRAD_COMM)(all,s,REG)
   1307       qgd(i,s) = qgt(i,s) + ESUBD(i) * [pgt(i,s) - pgd(i,s)];
   1308    
   1309   Equation TGCRATIO
   1310   # change in ratio of government consumption tax payments to regional income #
   1311   (all,r,REG)
   1312       100.0 * INCOME(r) * del_taxrgc(r) + TGC(r) * y(r)
   1313           = sum(i,TRAD_COMM,
   1314               VDGANC(i,r) * tgd(i,r) + DGTAX(i,r) * [pm(i,r) + qgd(i,r)])
   1315           + sum(i,TRAD_COMM,
   1316               VIGANC(i,r) * tgm(i,r) + IGTAX(i,r) * [pim(i,r) + qgm(i,r)]);
   1317    
   1318    
   1319   !<
   1320       1-2. Notational Links
   1321       ---------------------
   1322   >!
   1323    
   1324   Formula (all,i,TRAD_COMM)(all,r,REG)
   1325       VGA(i,r) = VGAT(i,r);
   1326    
   1327   Equation PGTEQ # prices into the government consumption system #
   1328   (all,i,TRAD_COMM)(all,r,REG)
   1329       pgt(i,r) = pg(i,r) ;
   1330    
   1331   Equation QGTEQ # quantities from the government consumption system #
   1332   (all,i,TRAD_COMM)(all,r,REG)
   1333       qgt(i,r) = qg(i,r) ;
   1334    
   1335    
   1336   !<
   1337       1-3. Energy Nest
   1338       ----------------
   1339   >!
   1340   Set
   1341       ENY_GCOMM # inputs into government energy subutility #
   1342       (coal, oil, gas, oil_pcts, electricity);
   1343   Subset
   1344       ENY_GCOMM is subset of TRAD_COMM;
   1345    
   1346   Formula (all,r,REG)
   1347       VGAS("eny",r) = sum(i,ENY_GCOMM, VGA(i,r));
   1348   Formula (all,r,REG)
   1349       VGA("eny",r) = VGAS("eny",r);
   1350    
   1351   Coefficient
   1352       GSIZE_ENY # size of ENY_GCOMM set #;
   1353   Formula
   1354       GSIZE_ENY = sum(i,ENY_GCOMM, 1);
   1355    
   1356   Coefficient (all,i,ENY_GCOMM)(all,r,REG)
   1357       GSHENY(i,r) # share of i in cost of energy subutility #;
   1358   Formula (all,i,ENY_GCOMM)(all,r,REG: VGAS("eny",r) = 0)
   1359       GSHENY(i,r) = 1.0 / GSIZE_ENY;
   1360   Formula (all,i,ENY_GCOMM)(all,r,REG: VGAS("eny",r) > 0)
   1361       GSHENY(i,r) = VGA(i,r) / VGAS("eny",r);
   1362    
   1363   Equation ENYGPRICE # price of energy subutility #
   1364   (all,r,REG)
   1365       pg("eny",r) = sum(k,ENY_GCOMM, GSHENY(k,r)* pg(k,r));
   1366    
   1367   Coefficient (parameter) (all,r,REG)
   1368       ELGENY(r) # elasticity of substitution in energy subconsumption #;
   1369   Read
   1370       ELGENY from file GTAPPARM header "EGEN";
   1371    
   1372   Equation ENYGDEMAND # demand for inputs into energy subconsumption #
   1373   (all,i,ENY_GCOMM)(all,r,REG)
   1374       qg(i,r) = qg("eny",r) - ELGENY(r) * [pg(i,r) - pg("eny",r)];
   1375    
   1376    
   1377   !<
   1378       1-4. Non-Energy Nest
   1379       --------------------
   1380   >!
   1381    
   1382   Set
   1383       NENY_GCOMM # non-energy subutility commodities # = TRAD_COMM - ENY_GCOMM;
   1384    
   1385   Formula (all,r,REG)
   1386       VGAS("neny",r) = sum(i,NENY_GCOMM, VGA(i,r));
   1387   Formula (all,r,REG)
   1388       VGA("neny",r) = VGAS("neny",r);
   1389    
   1390   Coefficient
   1391       GSIZE_NENY # size of NENY_GCOMM set #;
   1392   Formula
   1393       GSIZE_NENY = sum(i,NENY_GCOMM, 1);
   1394    
   1395   Coefficient (all,i,NENY_GCOMM)(all,r,REG)
   1396       GSHNENY(i,r)
   1397       # share of i in cost of non-energy subutility #;
   1398   Formula (all,i,NENY_GCOMM)(all,r,REG: VGAS("neny",r) = 0)
   1399       GSHNENY(i,r) = 1.0 / GSIZE_NENY;
   1400   Formula (all,i,NENY_GCOMM)(all,r,REG: VGAS("neny",r) > 0)
   1401       GSHNENY(i,r) = VGA(i,r) / VGAS("neny",r);
   1402    
   1403   Equation NENYGPRICE # price of non-energy subutility #
   1404   (all,r,REG)
   1405       pg("neny",r) = sum(k,NENY_GCOMM, GSHNENY(k,r)* pg(k,r));
   1406    
   1407   Coefficient (parameter) (all,r,REG)
   1408       ELGNENY(r)
   1409       # elasticity of substitution in non-energy subconsumption #;
   1410   Read
   1411       ELGNENY from file GTAPPARM header "EGNN";
   1412    
   1413   Equation NENYGDEMAND
   1414   # demand for inputs into non-energy subconsumption #
   1415   (all,i,NENY_GCOMM)(all,r,REG)
   1416       qg(i,r) = qg("neny",r) - ELGNENY(r) * [pg(i,r) - pg("neny",r)];
   1417    
   1418    
   1419   !<
   1420       1-5. Top of the Government Consumption Structure
   1421       ------------------------------------------------
   1422   >!
   1423    
   1424   Set
   1425       UG_COMM # inputs into top of government consumption structure #
   1426       (eny, neny);
   1427   Subset
   1428       UG_COMM is subset of GOV_COMM ;
   1429    
   1430   Equation GPRICEINDEX
   1431   # definition of price index for aggregate gov't purchases (HT 40) #
   1432   (all,r,REG)
   1433       pgov(r) = sum(i,UG_COMM, [VGA(i,r) / GOVEXP(r)] * pg(i,r));
   1434    
   1435   Coefficient (parameter) (all,r,REG)
   1436       ELGUG(r)
   1437       # elasticity of substitution in the top of government consumption nest #;
   1438   Read
   1439       ELGUG from file GTAPPARM header "EGUG";
   1440    
   1441   Equation GOVDMNDS
   1442   # government consumption demands for top nest commodities (HT 41) #
   1443   (all,i,UG_COMM)(all,r,REG)
   1444       qg(i,r) - pop(r) = ug(r) - ELGUG(r) * [pg(i,r) - pgov(r)];
   1445    
   1446   Equation GOVU
   1447   # utility from government consumption in r #
   1448   (all,r,REG)
   1449       yg(r) - pop(r) = pgov(r) + ug(r);
   1450    
