    263   Coefficient (all, r, REG)
    264       CO2Q(r) # CO2 emissions quota #;
    265    
    266   Read
    267       CO2Q from file GTAPDATA header "CO2Q";
    268    
    269   Coefficient (ge 0)(all,i,TRAD_COMM)(all,j,PROD_COMM) (all,r,REG)
    270       CO2DF(i,j,r)
    271       # carbon dioxide emissions from firms' usage of domestic product #;
    272   Read
    273       CO2DF from file GTAPDATA header "CODF";
    274    
    275   Coefficient (ge 0)(all,i,TRAD_COMM)(all,j,PROD_COMM)(all,r,REG)
    276       CO2IF(i,j,r)
    277       # carbon dioxide emissions from firms' usage of imports #;
    278   Read
    279       CO2IF from file GTAPDATA header "COIF";
    280    
    281   Coefficient (ge 0)(all,i,TRAD_COMM)(all,r,REG)
    282       CO2DG(i,r)
    283       # carbon dioxide emissions from government consumption of dom. product #;
    284   Read
    285       CO2DG from file GTAPDATA header "CODG";
    286    
    287   Coefficient (ge 0)(all,i,TRAD_COMM)(all,r,REG)
    288       CO2IG(i,r)
    289       # carbon dioxide emissions from government consumption of imports #;
    290   Read
    291       CO2IG from file GTAPDATA header "COIG";
    292    
    293   Coefficient (ge 0)(all,i,TRAD_COMM)(all,r,REG)
    294       CO2DP(i,r)
    295       # carbon dioxide emissions from private consumption of domestic product #;
    296   Read
    297       CO2DP from file GTAPDATA header "CODP";
    298    
    299   Coefficient (ge 0)(all,i,TRAD_COMM)(all,r,REG)
    300       CO2IP(i,r)
    301       # carbon dioxide emissions from private consumption of imports #;
    302   Read
    303       CO2IP from file GTAPDATA header "COIP";
    304    
    305   Coefficient(all,r,REG)(all,i,TRAD_COMM)
    306       CO2(r,i) # carbon dioxide emissions (millions of tonnes of carbon) #;
    307   Formula (all,r,REG)(all,i,TRAD_COMM)
    308       CO2(r,i) = sum(j,PROD_COMM, CO2DF(i,j,r)) + sum(j,PROD_COMM, CO2IF(i,j,r))
    309           + CO2DG(i,r) + CO2IG(i,r) + CO2DP(i,r) + CO2IP(i,r);
    310    
    311   Coefficient (all,r,REG)
    312       CO2T(r) # carbon dioxide emissions (millions of tonnes of carbon) #;
    313   Formula (all,r,REG)
    314       CO2T(r) = sum(i,TRAD_COMM, CO2(r,i));
    315    
    316   Coefficient (all,r,REG)
    317       NCTAXLEV(r) # nominal carbon tax rate (current USD per tonne of carbon) #;
    318   Read
    319       NCTAXLEV from file GTAPDATA header "NTAX";
