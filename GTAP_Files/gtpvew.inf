     64    
     65   Set
     66       REG # Regions in the model #
     67       maximum size 10 read elements from file GTAPSETS header "H1";
     68   Set
     69       TRAD_COMM # Traded commodities #
     70       maximum size 10 read elements from file GTAPSETS header "H2";
     71   Set
     72       MARG_COMM # Margin commodities #
     73       maximum size 10 read elements from file GTAPSETS header "MARG";
     74   Subset
     75       MARG_COMM is subset of TRAD_COMM;
     76   Set
     77       NMRG_COMM # Non-margin commodities # = TRAD_COMM - MARG_COMM;
     78   Set
     79       CGDS_COMM # Capital goods commodities #
     80       maximum size 1  read elements from file GTAPSETS header "H9";
     81   Set
     82       ENDW_COMM # Endowment commodities #
     83       maximum size 5  read elements from file GTAPSETS header "H6";
     84   Set
     85       ENDWC_COMM # Capital Endowment Commodity # (capital);
     86   Set
     87       PROD_COMM # PRODUCED COMMODITIES  # = TRAD_COMM union CGDS_COMM;
     88   Set
     89       DEMD_COMM # DEMANDED COMMODITIES # = ENDW_COMM union TRAD_COMM;
     90   Set
     91       NSAV_COMM # NON-SAVINGS COMMODITIES # = DEMD_COMM union CGDS_COMM;
     92   Subset
     93       PROD_COMM is subset of NSAV_COMM;
     94   Subset
     95       ENDWC_COMM is subset of NSAV_COMM;
