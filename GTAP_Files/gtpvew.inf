    322   !<
    323       ----------------------------
    324       DERIVATIVES OF THE BASE DATA
    325       ----------------------------
    326    
    327       1. Macro variables ( Reg )
    328       --------------------------
    329           GDPEXP
    330           GDPSRC
    331           CAPITALACCT
    332           CURRENTACCT
    333    
    334   >!
    335    
    336   Coefficient (all,i,TRAD_COMM)(all,r,REG)
    337       VPA(i,r) # Private household expenditure on i in r at agent's prices #;
    338   Formula (all,i,TRAD_COMM)(all,s,REG)
    339       VPA(i,s) = VDPA(i,s) + VIPA(i,s);
    340   Coefficient (all,r,REG)
    341       PRIVEXP(r) # Private consumption expenditure in region r #;
    342   Formula (all,r,REG)
    343       PRIVEXP(r) = sum{i,TRAD_COMM, VPA(i,r)};
    344    
    345   Coefficient (all,i,DEMD_COMM)(all,j,PROD_COMM)(all,r,REG)
    346       VFA(i,j,r) # Firm's expenditure on i by industry j #;
    347   Formula (all,i,ENDW_COMM)(all,j,PROD_COMM)(all,r,REG)
    348       VFA(i,j,r) = EVFA(i,j,r);
    349   Formula (all,i,TRAD_COMM)(all,j,PROD_COMM)(all,s,REG)
    350       VFA(i,j,s) = VDFA(i,j,s) + VIFA(i,j,s);
    351   Coefficient (all,i,NSAV_COMM)(all,r,REG)
    352       VOA(i,r) # Value of commodity i output in region r #;
    353   Formula (all,i,ENDW_COMM)(all,r,REG)
    354       VOA(i,r) = EVOA(i,r);
    355   Formula (all,i,PROD_COMM)(all,r,REG)
    356       VOA(i,r) = sum{j,DEMD_COMM, VFA(j,i,r)};
    357   Coefficient (all, r, REG)
    358       REGINV(r) # Regional gross investment in region r #;
    359   Formula (all, r, REG)
    360       REGINV(r) = sum{k,CGDS_COMM, VOA(k,r)};
    361    
    362   Coefficient (all,i,TRAD_COMM)(all,r,REG)
    363       VGA(i,r) # Government household expenditure on i in r at agent's prices #;
    364   Formula (all,i,TRAD_COMM)(all,s,REG)
    365       VGA(i,s) = VDGA(i,s) + VIGA(i,s);
    366   Coefficient (all,r,REG)
    367       GOVEXP(r) # Government expenditure in region r #;
    368   Formula (all,r,REG)
    369       GOVEXP(r) = sum{i,TRAD_COMM, VGA(i,r)};
    370    
    371   Coefficient (all,r,REG)
    372       VEXPORTS(r)
    373       # Value of merchandise and services sales to the world market from r #;
    374   Formula (all,r,REG)
    375       VEXPORTS(r)
    376           = sum{i,NMRG_COMM, sum{s,REG, VXWD(i,r,s)}}
    377           + sum{m,MARG_COMM, sum{s,REG, VXWD(m,r,s)} + VST(m,r)}
    378   ;
    379    
    380   Coefficient (all,s,REG)
    381       VIMPORTS(s)
    382       # Value of merchandise and services purchases from world market by s #;
    383   Formula (all,s,REG)
    384       VIMPORTS(s) = sum{i,TRAD_COMM, sum{r,REG, VIWS(i,r,s)}};
    385    
    386   Coefficient  (all,r,REG)(all,k,GDPEXPEND)
    387       GDPEXP(r,k) # Composition of GDP from the expenditure side #;
    388   Formula  (all,r,REG)
    389       GDPEXP(r,"cons") = PRIVEXP(r);
    390   Formula  (all,r,REG)
    391       GDPEXP(r,"inv") = REGINV(r);
    392   Formula  (all,r,REG)
    393       GDPEXP(r,"gov") = GOVEXP(r);
    394   Formula  (all,r,REG)
    395       GDPEXP(r,"export") = VEXPORTS(r);
    396   Formula  (all,r,REG)
    397       GDPEXP(r,"imp") = - VIMPORTS(r);
    398    
    399   Coefficient (all,r,REG)
    400       NETFACTINC(r) # Factor income, net of depreciation #;
    401   Formula (all,r,REG)
    402       NETFACTINC(r) = sum{i,ENDW_COMM, VOA(i,r)} - VDEP(r);
    403    
    404   Coefficient (all,i,TRAD_COMM)(all,r,REG)
    405       VDM(i,r) # Domestic sales of i in r at market prices (tradeables only) #;
    406   Formula (all,i,TRAD_COMM)(all,r,REG)
    407       VDM(i,r) = VDPM(i,r) + VDGM(i,r) + sum{j,PROD_COMM, VDFM(i,j,r)};
    408    
    409   Coefficient (all,i,NSAV_COMM)(all,r,REG)
    410       VOM(i,r) # Value of commodity i output in region r at market prices #;
    411   Formula (all,i,ENDW_COMM)(all,r,REG)
    412       VOM(i,r) = sum{j,PROD_COMM, VFM(i,j,r)};
    413   Formula (all,m,MARG_COMM)(all,r,REG)
    414       VOM(m,r) = VDM(m,r) + sum{s,REG, VXMD(m,r,s)} + VST(m,r);
    415   Formula (all,i,NMRG_COMM)(all,r,REG)
    416       VOM(i,r) = VDM(i,r) + sum{s,REG, VXMD(i,r,s)};
    417   Formula (all,h,CGDS_COMM)(all,r,REG)
    418       VOM(h,r) = VOA(h,r);
    419    
    420   Coefficient (all,r,REG)
    421       NETAXES(r) # Net tax revenues in r #;
    422   Formula (all,r,REG)
    423       NETAXES(r)
    424           = sum{i,NSAV_COMM, VOM(i,r) - VOA(i,r)}
    425           + sum{j,PROD_COMM, sum{i,ENDW_COMM, VFA(i,j,r) - VFM(i,j,r)}}
    426           + sum{i,TRAD_COMM, VIPA(i,r) - VIPM(i,r)}
    427           + sum{i,TRAD_COMM, VDPA(i,r) - VDPM(i,r)}
    428           + sum{i,TRAD_COMM, VIGA(i,r) - VIGM(i,r)}
    429           + sum{i,TRAD_COMM, VDGA(i,r) - VDGM(i,r)}
    430           + sum{j,PROD_COMM, sum{i,TRAD_COMM, VIFA(i,j,r) - VIFM(i,j,r)}}
    431           + sum{j,PROD_COMM, sum{i,TRAD_COMM, VDFA(i,j,r) - VDFM(i,j,r)}}
    432           + sum{i,TRAD_COMM, sum{s,REG, VXWD(i,r,s) - VXMD(i,r,s)}}
    433           + sum{i,TRAD_COMM, sum{s,REG, VIMS(i,s,r) - VIWS(i,s,r)}}
    434   ;
    435    
    436   Coefficient (all,r,REG)(all,k,GDPSOURCE)
    437       GDPSRC(r,k) # Composition of GDP from the sources side #;
    438   Formula (all,r,REG)
    439       GDPSRC(r,"fact") = NETFACTINC(r);
    440   Formula (all,r,REG)
    441       GDPSRC(r,"tax") = NETAXES(r);
    442   Formula (all,r,REG)
    443       GDPSRC(r,"depr") = VDEP(r);
    444    
    445   Coefficient (all, r, REG)
    446       NETINV(r) # Regional net investment in region r #;
    447   Formula (all, r, REG)
    448       NETINV(r) = REGINV(r) - VDEP(r);
    449    
    450   Coefficient (all,r,REG)(all,k,CAPACCT)
    451       CAPITALACCT(r,k) # S - I = X - M #;
    452   Formula (all,r,REG)
    453       CAPITALACCT(r,"save") = save(r);
    454   Formula (all,r,REG)
    455       CAPITALACCT(r,"inv") = - NETINV(r);
    456    
    457   Coefficient (all,r,REG)(all,k,CURACCT)(all,i,TRAD_COMM)
    458       CURRENTACCT(r,k,i) # X - M = S - I #;
    459   Formula (all,r,REG)(all,m,MARG_COMM)
    460       CURRENTACCT(r,"export",m) = sum{s,REG, VXWD(m,r,s)} + VST(m,r);
    461   Formula (all,r,REG)(all,i,NMRG_COMM)
    462       CURRENTACCT(r,"export",i) = sum{s,REG, VXWD(i,r,s)};
    463   Formula  (all,r,REG)(all,i,TRAD_COMM)
    464       CURRENTACCT(r,"imp",i) = - sum{s,REG, VIWS(i,s,r)};
    465   Formula  (all,r,REG)(all,i,TRAD_COMM)
    466       CURRENTACCT(r,"permits",i) = (CO2Q(r) - CO2T(r)) * NCTAXLEV(r);
