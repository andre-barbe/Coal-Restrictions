    469       2.Trade, Transport and Protection ( Reg x Reg )
    470       -----------------------------------------------
    471           VALEXPORTS
    472           VALIMPORTS
    473           CIFDECOMP
    474    
    475   >!
    476    
    477   Coefficient (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)(all,k,EXPVALUE)
    478       VALEXPORTS(i,r,s,k) # Value of exports at world prices #;
    479   Formula (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
    480       VALEXPORTS(i,r,s,"exprev") = VXMD(i,r,s);
    481   Formula (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
    482       VALEXPORTS(i,r,s,"xtax") = VXWD(i,r,s) - VXMD(i,r,s);
    483    
    484   Coefficient (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)(all,k,IMPVALUE)
    485       VALIMPORTS(i,r,s,k) # Value of imports at domestic prices #;
    486   Formula (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
    487       VALIMPORTS(i,r,s,"impcost") = VIWS(i,r,s);
    488   Formula (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
    489       VALIMPORTS(i,r,s,"mtax") = VIMS(i,r,s) - VIWS(i,r,s);
    490    
    491   Coefficient (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)(all,j,MKTVALUE)
    492       CIFDECOMP(i,r,s,j) # Value of tradables from r to s #;
    493   Formula (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
    494       CIFDECOMP(i,r,s,"fob") = VXWD(i,r,s);
    495   Formula (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
    496       CIFDECOMP(i,r,s,"trans") = VIWS(i,r,s) - VXWD(i,r,s);
    497    
    498   !<
    499       3. Commodities ( Comm x Reg )
    500       -----------------------------
    501           OUTDISP
    502           DOMSALESDISP
    503           IMPSALESDISP
    504           VALOUTPUT
    505           SUFFICIENCY
    506    
    507   >!
    508    
    509   Coefficient (all,i,TRAD_COMM)(all,j,OUT)(all,r,REG)
    510       OUTDISP(i,j,r)
    511       # Value of sales of tradables to different markets at dom market price #;
    512   Formula (all,i,TRAD_COMM)(all,j,OUT)(all,r,REG)
    513       OUTDISP(i,j,r) = 0.0;
    514   Formula (all,i,TRAD_COMM)(all,r,REG)
    515       OUTDISP(i,"dom",r) = VDM(i,r);
    516   Formula (all,m,MARG_COMM)(all,r,REG)
    517       OUTDISP(m,"trans",r) = VST(m,r);
    518   Formula (all,i,TRAD_COMM)(all,r,REG)
    519       OUTDISP(i,"export",r) = sum{s,REG, VXMD(i,r,s)};
    520    
    521   Coefficient (all,i,TRAD_COMM)(all,m,SALES)(all,r,REG)
    522       DOMSALESDISP(i,m,r)
    523       # Value of domestic sales of tradables at dom market price #;
    524   Formula (all,i,TRAD_COMM)(all,r,REG)
    525       DOMSALESDISP(i,"production",r) = sum{j,PROD_COMM, VDFM(i,j,r)};
    526   Formula  (all,i,TRAD_COMM)(all,r,REG)
    527       DOMSALESDISP(i,"cons",r) = VDPM(i,r);
    528   Formula  (all,i,TRAD_COMM)(all,r,REG)
    529       DOMSALESDISP(i,"gov",r) = VDGM(i,r);
    530    
    531   Coefficient (all,i,TRAD_COMM)(all,m,SALES)(all,r,REG)
    532       IMPSALESDISP(i,m,r)
    533       # Value of sales of tradables to different markets at dom market price #;
    534   Formula (all,i,TRAD_COMM)(all,r,REG)
    535       IMPSALESDISP(i,"production",r) = sum{j,PROD_COMM, VIFM(i,j,r)};
    536   Formula (all,i,TRAD_COMM)(all,r,REG)
    537       IMPSALESDISP(i,"cons",r) = VIPM(i,r);
    538   Formula (all,i,TRAD_COMM)(all,r,REG)
    539       IMPSALESDISP(i,"gov",r) = VIGM(i,r);
    540    
    541   !<
    542       Finally, calculate some policy instruments of interest.
    543       Note that this approach produces positive taxes,
    544       but GROSS tax rates.
    545   >!
    546   Coefficient (all,i,TRAD_COMM)(all,r,REG)(all,k,OUTVALUE)
    547       VALOUTPUT(i,r,k) # Value of output at agent's prices #;
    548   Formula (all,i,TRAD_COMM)(all,r,REG)
    549       VALOUTPUT(i,r,"prodrev") = VOA(i,r);
    550   Formula (all,i,TRAD_COMM)(all,r,REG)
    551       VALOUTPUT(i,r,"outtax") = VOM(i,r) - VOA(i,r);
    552    
    553   Zerodivide (zero_by_zero) default 0;
    554   Zerodivide (NONZERO_BY_ZERO) default 0;
    555    
    556   Coefficient (all,i, TRAD_COMM)(all,r, REG)
    557       SUFFICIENCY(i,r) # Region's r rate of self-sufficiency for good i #;
    558   Formula (all,i, TRAD_COMM)(all,r, REG)
    559       SUFFICIENCY(i,r) = VOM(i,r) / [VDM(i,r) + sum{s,REG, VIMS(i,s,r)}];
    560    
    561   Zerodivide (zero_by_zero) off;
    562   Zerodivide (NONZERO_BY_ZERO) off;
    563    
    564   !<
    565       4. Sectors ( Comm x Reg )
    566       -------------------------
    567           NVFA
    568           NVPA
    569           NVGA
    570    
    571   >!
    572    
    573   Coefficient (all,i,DEMD_COMM)(all,j,PROD_COMM)(all,r,REG)(all,d,DIR)
    574       (all,k,PURCHVALUE)
    575       NVFA(i,j,r,d,k) # Firm's expenditure on i by industry j #;
    576   Formula (all,i,ENDW_COMM)(all,j,PROD_COMM)(all,r,REG)
    577       NVFA(i,j,r,"domestic","mktexp") = VFM(i,j,r);
    578   Formula (all,i,ENDW_COMM)(all,j,PROD_COMM)(all,r,REG)
    579       NVFA(i,j,r,"imported","mktexp") = 0;
    580   Formula (all,i,TRAD_COMM)(all,j,PROD_COMM)(all,s,REG)
    581       NVFA(i,j,s,"domestic","mktexp") = VDFM(i,j,s);
    582   Formula (all,i,TRAD_COMM)(all,j,PROD_COMM)(all,s,REG)
    583       NVFA(i,j,s,"imported","mktexp") = VIFM(i,j,s);
    584    
    585   Formula (all,i,ENDW_COMM)(all,j,PROD_COMM)(all,r,REG)
    586       NVFA(i,j,r,"domestic","comtax") = EVFA(i,j,r) - VFM(i,j,r);
    587   Formula (all,i,ENDW_COMM)(all,j,PROD_COMM)(all,r,REG)
    588       NVFA(i,j,r,"imported","comtax") = 0;
    589   Formula (all,i,TRAD_COMM)(all,j,PROD_COMM)(all,s,REG)
    590       NVFA(i,j,s,"domestic","comtax") = VDFA(i,j,s) - VDFM(i,j,s);
    591   Formula (all,i,TRAD_COMM)(all,j,PROD_COMM)(all,s,REG)
    592       NVFA(i,j,s,"imported","comtax") = VIFA(i,j,s) - VIFM(i,j,s);
    593    
    594   Coefficient (all,i,TRAD_COMM)(all,s,REG)(all,d,DIR)(all,k,PURCHVALUE)
    595       NVPA(i,s,d,k) # Consumption expenditure on i by industry j #;
    596   Formula (all,i,TRAD_COMM)(all,s,REG)
    597       NVPA(i,s,"domestic","mktexp") = VDPM(i,s);
    598   Formula (all,i,TRAD_COMM)(all,s,REG)
    599       NVPA(i,s,"imported","mktexp") = VIPM(i,s);
    600    
    601   Formula (all,i,TRAD_COMM)(all,s,REG)
    602       NVPA(i,s,"domestic","comtax") = VDPA(i,s) - VDPM(i,s);
    603   Formula (all,i,TRAD_COMM)(all,s,REG)
    604       NVPA(i,s,"imported","comtax") = VIPA(i,s) - VIPM(i,s);
    605    
    606   Coefficient (all,i,TRAD_COMM)(all,s,REG)(all,d,DIR)(all,k,PURCHVALUE)
    607       NVGA(i,s,d,k) # Government expenditure on i by industry j #;
    608   Formula (all,i,TRAD_COMM)(all,s,REG)
    609       NVGA(i,s,"domestic","mktexp") = VDGM(i,s);
    610   Formula (all,i,TRAD_COMM)(all,s,REG)
    611       NVGA(i,s,"imported","mktexp") = VIGM(i,s);
    612    
    613   Formula (all,i,TRAD_COMM)(all,s,REG)
    614       NVGA(i,s,"domestic","comtax") = VDGA(i,s) - VDGM(i,s);
    615   Formula (all,i,TRAD_COMM)(all,s,REG)
    616       NVGA(i,s,"imported","comtax") = VIGA(i,s) - VIGM(i,s);
    617    
    618   !<
    619       -----------------------------------
    620       Write statements of the Derivatives
    621       -----------------------------------
    622    
    623       1. Macro variables ( Reg )
    624       --------------------------
    625   >!
    626   Write GDPEXP to file GTAPVIEW header "AG01" longname
    627       "GDPEXP = C + I + G + X - M, GDP from the expenditure side";
    628   Write GDPSRC to file GTAPVIEW header "AG02" longname
    629       "GDPSRC = NETFACTINC + NETAXES + VDEP, GDP from the sources side";
    630   Write EVFA to file GTAPVIEW header "AG03" longname
    631       "Sources of factor income (NETFACTINC + VDEP) by sector - EVFA(j,r)";
    632   Write CAPITALACCT to file GTAPVIEW header "AG04" longname
    633       "S - I = X - M: CAPITALACCT";
    634   Write CURRENTACCT to file GTAPVIEW header "AG05" longname
    635       "X - M = S - I: CURRENTACCT";
    636   Write VKB to file GTAPVIEW header "AG06" longname
    637       "Capital stock by region - VKB";
    638    
    639   !<
    640       2.Trade, Transport and Protection ( Reg x Reg )
    641       -----------------------------------------------
    642   >!
    643   Write VALEXPORTS to file GTAPVIEW header "BI01" longname
    644       "Decomposition of exports at world prices: VXWD=VXMD+XTAX";
    645   Write VALIMPORTS to file GTAPVIEW header "BI02" longname
    646       "Decomposition of imports at market prices: VIMS=VIWS+MTAX";
    647   Write CIFDECOMP to file GTAPVIEW header "BI03" longname
    648       "Decomposition of cif values - CIFDECOMP";
    649    
    650   !<
    651       3. Commodities ( Comm x Reg )
    652       -----------------------------
    653   >!
    654   Write OUTDISP to file GTAPVIEW header "CM01" longname
    655       "Disposition of output - OUTDISP";
    656   Write DOMSALESDISP to file GTAPVIEW header "CM02" longname
    657       "Disposition of domestic goods - DOMSALESDISP";
    658   Write IMPSALESDISP to file GTAPVIEW header "CM03" longname
    659       "Disposition of imported goods - IMPSALESDISP";
    660   Write VALOUTPUT to file GTAPVIEW header "CM04" longname
    661       "Value of output at market prices:  VOM = VOA + OUTTAX ";
    662   Write SUFFICIENCY to file GTAPVIEW header "CM05" longname
    663       "Self-sufficiency or domestic share in total use - SUFFICIENCY";
    664    
    665   !<
    666       4. Sectors ( Comm x Reg )
    667       -------------------------
    668   >!
    669    
    670   Write NVFA to file GTAPVIEW header "SF01" longname
    671       "Cost structure of firms - VFA";
    672   Write NVPA to file GTAPVIEW header "SF02" longname
    673       "Cost structure of consumption - VPA";
    674   Write NVGA to file GTAPVIEW header "SF03" longname
    675       "Cost structure of government - VGA";
    676    
    677   !<
    678       --------------------------------
    679       Computation of Initial Tax Rates
    680       --------------------------------
    681    
    682       Calculate initial distortions as percent ad valorem rates
    683   >!
    684    
    685   Coefficient
    686       (all,i,NSAV_COMM)(all,r,REG)                   rTO(i,r);
    687       (all,i,ENDW_COMM)(all,j,PROD_COMM)(all,r,REG)  rTF(i,j,r);
    688       (all,i,TRAD_COMM)(all,r,REG)                  rTPD(i,r);
    689       (all,i,TRAD_COMM)(all,r,REG)                  rTPI(i,r);
    690       (all,i,TRAD_COMM)(all,r,REG)                  rTGD(i,r);
    691       (all,i,TRAD_COMM)(all,r,REG)                  rTGI(i,r);
    692       (all,i,TRAD_COMM)(all,j,PROD_COMM)(all,r,REG) rTFD(i,j,r);
    693       (all,i,TRAD_COMM)(all,j,PROD_COMM)(all,r,REG) rTFI(i,j,r);
    694       (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)       rTXS(i,r,s);
    695       (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)       rTMS(i,r,s);
    696    
    697   Zerodivide default 1;
    698   Formula
    699       (all,i,NSAV_COMM)(all,r,REG)
    700       rTO(i,r)    = 100*{[VOA(i,r)/VOM(i,r)]-1};
    701    
    702       (all,i,ENDW_COMM)(all,j,PROD_COMM)(all,r,REG)
    703       rTF(i,j,r)  = 100*{[VFA(i,j,r)/VFM(i,j,r)]-1};
    704    
    705       (all,i,TRAD_COMM)(all,r,REG)
    706       rTPD(i,r)   =  100*{[VDPA(i,r)/VDPM(i,r)]-1};
    707    
    708       (all,i,TRAD_COMM)(all,r,REG)
    709       rTPI(i,r)   = 100*{[VIPA(i,r)/VIPM(i,r)]-1};
    710    
    711       (all,i,TRAD_COMM)(all,r,REG)
    712       rTGD(i,r)   = 100*{[VDGA(i,r)/VDGM(i,r)]-1};
    713    
    714       (all,i,TRAD_COMM)(all,r,REG)
    715       rTGI(i,r)   = 100*{[VIGA(i,r)/VIGM(i,r)]-1};
    716    
    717       (all,i,TRAD_COMM)(all,j,PROD_COMM)(all,r,REG)
    718       rTFD(i,j,r) = 100*{[VDFA(i,j,r)/VDFM(i,j,r)]-1};
    719    
    720       (all,i,TRAD_COMM)(all,j,PROD_COMM)(all,r,REG)
    721       rTFI(i,j,r) = 100*{[VIFA(i,j,r)/VIFM(i,j,r)]-1};
    722    
    723       (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
    724       rTXS(i,r,s) = 100*{[VXMD(i,r,s)/VXWD(i,r,s)]-1};
    725    
    726       (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
    727       rTMS(i,r,s) = 100*{[VIMS(i,r,s)/VIWS(i,r,s)]-1};
    728    
    729   Zerodivide off;
    730    
    731   Write
    732       rTO to file TAXRATES header "rTO " longname
    733       "% ad valorem rate, output (or income) subsidies in region r";
    734       rTF to file TAXRATES header "rTF " longname
    735       "% ad valorem rate, taxes on primary factors";
    736       rTPD to file TAXRATES header "rTPD" longname
    737       "% ad valorem rate, private domestic consumption taxes";
    738       rTPI to file TAXRATES header "rTPI" longname
    739       "% ad valorem rate, private import consumption taxes";
    740       rTGD to file TAXRATES header "rTGD" longname
    741       "% ad valorem rate, government domestic purchases taxes";
    742       rTGI to file TAXRATES header "rTGI" longname
    743       "% ad valorem rate, government import purchases taxes";
    744       rTFD to file TAXRATES header "rTFD" longname
    745       "% ad valorem rate, taxes on firms' domestic purchases";
    746       rTFI to file TAXRATES header "rTFI" longname
    747       "% ad valorem rate, taxes on firms' imports purchases";
    748       rTXS to file TAXRATES header "rTXS" longname
    749       "% ad valorem rate, export subsidies, by destination";
    750       rTMS to file TAXRATES header "rTMS" longname
    751       "% ad valorem rate, import taxes, by source";
    752    
    753   !<
    754       -------------------------------
    755       Checking the balance conditions
    756       -------------------------------
    757   >!
    758    
    759   Coefficient (all,i,TRAD_COMM)(all,s,REG)
    760       CHKMKTCLIMP(i,s)
    761       # Checking the accounting on traded commodities supply and demand #;
    762   Formula  (all,i,TRAD_COMM)(all,s,REG)
    763       CHKMKTCLIMP(i,s)
    764           = sum{r,REG, VIMS(i,r,s)}
    765           - sum{j,PROD_COMM, VIFM(i,j,s)}
    766           - VIPM(i,s) - VIGM(i,s);
    767    
    768   Coefficient (all,s,REG)
    769       SURPLUS(s)
    770       # Economic surplus in region s.  This should be close to zero. #;
    771   Formula (all,r,REG)
    772       SURPLUS(r)
    773           = sum{i,ENDW_COMM, VOA(i,r)} - VDEP(r)
    774           + sum{i,NSAV_COMM, VOM(i,r) - VOA(i,r)}
    775           + sum{j,PROD_COMM, sum{i,ENDW_COMM, VFA(i,j,r) - VFM(i,j,r)}}
    776           + sum{i,TRAD_COMM, VIPA(i,r) - VIPM(i,r)}
    777           + sum{i,TRAD_COMM, VDPA(i,r) - VDPM(i,r)}
    778           + sum{i,TRAD_COMM, VIGA(i,r) - VIGM(i,r)}
    779           + sum{i,TRAD_COMM, VDGA(i,r) - VDGM(i,r)}
    780           + sum{j,PROD_COMM, sum{i,TRAD_COMM, VIFA(i,j,r) - VIFM(i,j,r)}}
    781           + sum{j,PROD_COMM, sum{i,TRAD_COMM, VDFA(i,j,r) - VDFM(i,j,r)}}
    782           + sum{i,TRAD_COMM, sum{s,REG, VXWD(i,r,s) - VXMD(i,r,s)}}
    783           + sum{i,TRAD_COMM, sum{s,REG, VIMS(i,s,r) - VIWS(i,s,r)}}
    784           - sum{i,TRAD_COMM, VPA(i,r) + VGA(i,r)} - save(r)
    785           + [CO2Q(r) - CO2T(r)] * NCTAXLEV(r);
    786    
    787   Coefficient (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
    788       VTWR_M(i,r,s)
    789       # Value of transport services associated w/ the shipment of i from r to s #;
    790   Formula (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
    791       VTWR_M(i,r,s) = VIWS(i,r,s) - VXWD(i,r,s);
    792    
    793   Coefficient (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
    794       CHKTRANS_D(i,r,s)
    795       # Difference between global demand and margins, should be close to zero #;
    796   Formula (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
    797       CHKTRANS_D(i,r,s) = VTWR_M(i,r,s) - sum{m, MARG_COMM, VTWR(m,i,r,s)};
    798    
    799   Coefficient (all,i,TRAD_COMM)(all,r,REG)
    800       VSTT(i,r) #For check on tranport services and margines #;
    801   Formula (all,i,TRAD_COMM)(all,r,REG)
    802       VSTT(i,r) = 0;
    803   Formula (all,i,MARG_COMM)(all,r,REG)
    804       VSTT(i,r) = VST(i,r);
    805    
    806   Coefficient
    807       CHKTRANS_S
    808   #Diff btwn ttl supply of trnsprt srvcs & ttl margins, should be close to zero#;
    809   Formula
    810       CHKTRANS_S
    811           = sum{m,MARG_COMM,sum{i,TRAD_COMM,sum{r,REG,sum{s,REG, VTWR(m,i,r,s)}}}}
    812           - sum{i,TRAD_COMM,sum{r,REG, VSTT(i,r)}};
    813    
    814   !<
    815       ------------------------------------------
    816       Calculating Summary Measures for Imbalance
    817       ------------------------------------------
    818    
    819       For each GTAPDATA calculate a summary measure of imbalance, the sum of
    820       the absolute magnitudes of the imbalances in components.  Calculate an
    821       overall summary measure, the sum of the variable-specific summary
    822       measures.
    823   >!
    824    
    825   Coefficient
    826       SUMTRANS_S
    827       # Summary measure for transport services, should be close to zero #;
    828   Formula
    829       SUMTRANS_S = abs(CHKTRANS_S);
    830    
    831   Coefficient
    832       SUMTRANS_D
    833       # Summary measure for transport services, should be close to zero #;
    834   Formula
    835       SUMTRANS_D
    836           = sum{i, TRAD_COMM, sum{r, REG, sum{s, REG, abs(CHKTRANS_D(i,r,s))}}};
    837    
    838   Coefficient
    839       SUMSURPLUS
    840       # Summary measure for economic surplus, should be close to zero #;
    841   Formula
    842       SUMSURPLUS = sum{r, REG, abs(SURPLUS(r))};
    843    
    844   Coefficient
    845       SUMMKTCLIMP
    846       # Summary measure for excess importation, should be close to zero #;
    847   Formula
    848       SUMMKTCLIMP = sum{i,TRAD_COMM, sum{s,REG, abs(CHKMKTCLIMP(i,s))}};
    849    
    850   Coefficient
    851       SUMIMBALS # Summary measure of imbalances, should be close to zero #;
    852   Formula
    853       SUMIMBALS
    854           = SUMTRANS_S + SUMTRANS_D + SUMSURPLUS + SUMMKTCLIMP;
    855    
    856   !<
    857       -------------------------------------
    858       Writing Summary Measures of Imbalance
    859       -------------------------------------
    860   >!
    861    
    862   Write SUMIMBALS   to file GTAPVIEW header "0IMB";
    863   Write SUMTRANS_S  to file GTAPVIEW header "1TNS";
    864   Write SUMTRANS_D  to file GTAPVIEW header "1TND";
    865   Write SUMSURPLUS  to file GTAPVIEW header "1SRP";
    866   Write SUMMKTCLIMP to file GTAPVIEW header "1IMP";
    867   Write CHKTRANS_S  to file GTAPVIEW header "2TNS";
    868   Write CHKTRANS_D  to file GTAPVIEW header "2TND";
    869   Write SURPLUS     to file GTAPVIEW header "2SRP";
    870   Write CHKMKTCLIMP to file GTAPVIEW header "2IMP";
    871    
    872   !<
    873       ----------------------------
    874       Checking the sign conditions
    875       ----------------------------
    876   >!
    877    
    878   Coefficient ! work matrices for holding binary flags: re-used to save RAM !
    879   (all,i,ENDW_COMM)(all,j,PROD_COMM)(all,r,REG) SIGNEPR(i,j,r);
    880   (all,i,ENDW_COMM)(all,r,REG) SIGNER(i,r);
    881   (all,k,MARG_COMM)(all,r,REG) SIGNMR(k,r);
    882   (all,k,MARG_COMM)(all,i,TRAD_COMM)(all,r,REG)(all,s,REG) SIGNMTRR(k,i,r,s);
    883   (all,r,REG) SIGNR(r);
    884   (all,i,TRAD_COMM)(all,j,PROD_COMM)(all,r,REG) SIGNTPR(i,j,r);
    885   (all,i,TRAD_COMM)(all,r,REG) SIGNTR(i,r);
