!<
    -------------------------
    1. Government Consumption
    -------------------------

    1-0. Module-Specific Variables
    1-1. Composite Tradeables
    1-2. Notational Links
    1-3. Energy Nest
    1-4. Non-Energy Nest
    1-5. Top of the Government Consumption Structure


>!

!<
    1-0. Module-Specific Variables
    ------------------------------

    only used in this Government Consumption module
>!

Set
    SUBG_COMM # subutilities in government consumption #
    (eny, neny);
Set
    GOV_COMM # commodities demanded by government #
    = TRAD_COMM union SUBG_COMM;

Variable (all,i,GOV_COMM)(all,r,REG)
    pg(i,r) # government consumption price for commodity i in region r #;
Variable (all,i,GOV_COMM)(all,r,REG)
    qg(i,r) # government hhld demand for commodity i in region r #;
Variable (all,i,TRAD_COMM)(all,r,REG)
    pgt(i,r) # government consumption price for commodity i in region r #;
Variable (all,i,TRAD_COMM)(all,r,REG)
    qgt(i,r) # government hhld demand for commodity i in region r #;

Coefficient (all,i,GOV_COMM)(all,r,REG)
    VGA(i,r) # government expend. on i in r valued at agent's prices #;
Coefficient (all,i,SUBG_COMM)(all,r,REG)
    VGAS(i,r) # government expend. on i in r valued at agent's prices #;


!<
    1-1. Composite Tradeables
    -------------------------
>!

Zerodivide (zero_by_zero) default 1;
Coefficient (all,i,TRAD_COMM)(all,r,REG)
    SHVDGANC(i,r)
    # share of carbon-tax-excl. value of gov. consumption of domestic i in r #;
Formula (all,i,TRAD_COMM)(all,r,REG)
    SHVDGANC(i,r) = VDGANC(i,r) / VDGA(i,r);
Zerodivide (zero_by_zero) off;

Zerodivide (zero_by_zero) default 0;
Coefficient (all,i,TRAD_COMM)(all,r,REG)
    CO2DGVDGA(i,r)
    # emissions intensity of government consumption of domestic i in r #;
Formula (all,i,TRAD_COMM)(all,r,REG)
    CO2DGVDGA(i,r) = CO2DG(i,r) / VDGA(i,r);
Zerodivide (zero_by_zero) off;

Equation GHHDPRICE
# taxation of government consumption of domestic product #
(all,i,TRAD_COMM)(all,r,REG)
    pgd(i,r)= SHVDGANC(i,r) * (pm(i,r) + tgd(i,r))
        + 100.0 * CO2DGVDGA(i,r) * NCTAXB(REGTOBLOC(r));

Zerodivide (zero_by_zero) default 1;
Coefficient (all,i,TRAD_COMM)(all,r,REG)
    SHVIGANC(i,r)
    # share of carbon-tax-excl. value of gov. consumption of imported i in r #;
Formula (all,i,TRAD_COMM)(all,r,REG)
    SHVIGANC(i,r)=VIGANC(i,r) / VIGA(i,r);
Zerodivide (zero_by_zero) off;

Zerodivide (zero_by_zero) default 0;
Coefficient (all,i,TRAD_COMM)(all,r,REG)
    CO2IGVIGA(i,r)
    # emissions intensity of government consumption of imported i in r #;
Formula (all,i,TRAD_COMM)(all,r,REG)
    CO2IGVIGA(i,r) = CO2IG(i,r) / VIGA(i,r);
Zerodivide (zero_by_zero) off;

Equation GHHIPRICES
# taxation of government consumption of imports #
(all,i,TRAD_COMM)(all,r,REG)
    pgm(i,r) = SHVIGANC(i,r) * (pim(i,r) + tgm(i,r))
        + 100.0 * CO2IGVIGA(i,r) * NCTAXB(REGTOBLOC(r));

Coefficient (all,i,TRAD_COMM)(all,s,REG)
    GMSHR(i,s) # share of imports for gov't hhld at agent's prices #;
Formula (all,i,TRAD_COMM)(all,s,REG)
    GMSHR(i,s) = VIGA(i,s) / VGAT(i,s);

Equation GCOMPRICE
# government consumption price for composite commodities (HT 42) #
(all,i,TRAD_COMM)(all,s,REG)
    pgt(i,s) =  GMSHR(i,s) * pgm(i,s) + [1 - GMSHR(i,s)] * pgd(i,s);

Equation GHHLDAGRIMP
# government consumption demand for aggregate imports (HT 43) #
(all,i,TRAD_COMM)(all,s,REG)
    qgm(i,s) = qgt(i,s) + ESUBD(i) * [pgt(i,s) - pgm(i,s)];

Equation GHHLDDOM
# government consumption demand for domestic goods (HT 44) #
(all,i,TRAD_COMM)(all,s,REG)
    qgd(i,s) = qgt(i,s) + ESUBD(i) * [pgt(i,s) - pgd(i,s)];

Equation TGCRATIO
# change in ratio of government consumption tax payments to regional income #
(all,r,REG)
    100.0 * INCOME(r) * del_taxrgc(r) + TGC(r) * y(r)
        = sum(i,TRAD_COMM,
            VDGANC(i,r) * tgd(i,r) + DGTAX(i,r) * [pm(i,r) + qgd(i,r)])
        + sum(i,TRAD_COMM,
            VIGANC(i,r) * tgm(i,r) + IGTAX(i,r) * [pim(i,r) + qgm(i,r)]);


!<
    1-2. Notational Links
    ---------------------
>!

Formula (all,i,TRAD_COMM)(all,r,REG)
    VGA(i,r) = VGAT(i,r);

Equation PGTEQ # prices into the government consumption system #
(all,i,TRAD_COMM)(all,r,REG)
    pgt(i,r) = pg(i,r) ;

Equation QGTEQ # quantities from the government consumption system #
(all,i,TRAD_COMM)(all,r,REG)
    qgt(i,r) = qg(i,r) ;


!<
    1-3. Energy Nest
    ----------------
>!
Set
    ENY_GCOMM # inputs into government energy subutility #
    (coal, oil, gas, oil_pcts, electricity);
Subset
    ENY_GCOMM is subset of TRAD_COMM;

Formula (all,r,REG)
    VGAS("eny",r) = sum(i,ENY_GCOMM, VGA(i,r));
Formula (all,r,REG)
    VGA("eny",r) = VGAS("eny",r);

Coefficient
    GSIZE_ENY # size of ENY_GCOMM set #;
Formula
    GSIZE_ENY = sum(i,ENY_GCOMM, 1);

Coefficient (all,i,ENY_GCOMM)(all,r,REG)
    GSHENY(i,r) # share of i in cost of energy subutility #;
Formula (all,i,ENY_GCOMM)(all,r,REG: VGAS("eny",r) = 0)
    GSHENY(i,r) = 1.0 / GSIZE_ENY;
Formula (all,i,ENY_GCOMM)(all,r,REG: VGAS("eny",r) > 0)
    GSHENY(i,r) = VGA(i,r) / VGAS("eny",r);

Equation ENYGPRICE # price of energy subutility #
(all,r,REG)
    pg("eny",r) = sum(k,ENY_GCOMM, GSHENY(k,r)* pg(k,r));

Coefficient (parameter) (all,r,REG)
    ELGENY(r) # elasticity of substitution in energy subconsumption #;
Read
    ELGENY from file GTAPPARM header "EGEN";

Equation ENYGDEMAND # demand for inputs into energy subconsumption #
(all,i,ENY_GCOMM)(all,r,REG)
    qg(i,r) = qg("eny",r) - ELGENY(r) * [pg(i,r) - pg("eny",r)];


!<
    1-4. Non-Energy Nest
    --------------------
>!

Set
    NENY_GCOMM # non-energy subutility commodities # = TRAD_COMM - ENY_GCOMM;

Formula (all,r,REG)
    VGAS("neny",r) = sum(i,NENY_GCOMM, VGA(i,r));
Formula (all,r,REG)
    VGA("neny",r) = VGAS("neny",r);

Coefficient
    GSIZE_NENY # size of NENY_GCOMM set #;
Formula
    GSIZE_NENY = sum(i,NENY_GCOMM, 1);

Coefficient (all,i,NENY_GCOMM)(all,r,REG)
    GSHNENY(i,r)
    # share of i in cost of non-energy subutility #;
Formula (all,i,NENY_GCOMM)(all,r,REG: VGAS("neny",r) = 0)
    GSHNENY(i,r) = 1.0 / GSIZE_NENY;
Formula (all,i,NENY_GCOMM)(all,r,REG: VGAS("neny",r) > 0)
    GSHNENY(i,r) = VGA(i,r) / VGAS("neny",r);

Equation NENYGPRICE # price of non-energy subutility #
(all,r,REG)
    pg("neny",r) = sum(k,NENY_GCOMM, GSHNENY(k,r)* pg(k,r));

Coefficient (parameter) (all,r,REG)
    ELGNENY(r)
    # elasticity of substitution in non-energy subconsumption #;
Read
    ELGNENY from file GTAPPARM header "EGNN";

Equation NENYGDEMAND
# demand for inputs into non-energy subconsumption #
(all,i,NENY_GCOMM)(all,r,REG)
    qg(i,r) = qg("neny",r) - ELGNENY(r) * [pg(i,r) - pg("neny",r)];


!<
    1-5. Top of the Government Consumption Structure
    ------------------------------------------------
>!

Set
    UG_COMM # inputs into top of government consumption structure #
    (eny, neny);
Subset
    UG_COMM is subset of GOV_COMM ;

Equation GPRICEINDEX
# definition of price index for aggregate gov't purchases (HT 40) #
(all,r,REG)
    pgov(r) = sum(i,UG_COMM, [VGA(i,r) / GOVEXP(r)] * pg(i,r));

Coefficient (parameter) (all,r,REG)
    ELGUG(r)
    # elasticity of substitution in the top of government consumption nest #;
Read
    ELGUG from file GTAPPARM header "EGUG";

Equation GOVDMNDS
# government consumption demands for top nest commodities (HT 41) #
(all,i,UG_COMM)(all,r,REG)
    qg(i,r) - pop(r) = ug(r) - ELGUG(r) * [pg(i,r) - pgov(r)];

Equation GOVU
# utility from government consumption in r #
(all,r,REG)
    yg(r) - pop(r) = pgov(r) + ug(r);


!<
    -----------------------------
    2. Private Consumption Module
    -----------------------------

    2-0. Module-Specific Variables
    2-1. Composite Tradeables
    2-2. Notational Links
    2-3. Energy Nest
    2-4. Top of the Private Consumption Structure

>!


!<
    2-0. Module-Specific Variables
    ------------------------------

    only used in this Private Consumption module
>!

Set
    SUBP_COMM # subutilities in private consumption # (eny);
Set
    PRIV_COMM # commodities demanded in private consumption #
    = TRAD_COMM union SUBP_COMM;

Variable (all,i,PRIV_COMM)(all,r,REG)
    pp(i,r) # private consumption price for commodity i in region r #;
Variable (all,i,PRIV_COMM)(all,r,REG)
    qp(i,r) # private hhld demand for commodity i in region r #;
Variable (all,i,TRAD_COMM)(all,r,REG)
    ppt(i,r) # private consumption price for commodity i in region r #;
Variable (all,i,TRAD_COMM)(all,r,REG)
    qpt(i,r) # private hhld demand for commodity i in region r #;

Coefficient (all,i,PRIV_COMM)(all,r,REG)
    VPA(i,r) # private hhld expend. on i in r valued at agent's prices #;
Coefficient (all,i,SUBP_COMM)(all,r,REG)
    VPAS(i,r) # private hhld expend. on i in r valued at agent's prices #;


!<
    2-1. Composite Tradeables
    -------------------------
>!

Variable (all,r,REG)
    tp(r) # comm.-, source-gen. shift in tax on private cons. #;
!<
    The variable tp(r) can be swapped with del_ttaxr(r) in order to generate a
    tax replacement scenario, whereby taxes remain a constant share of
    national income.
>!

Variable (all,i,TRAD_COMM)(all,r,REG)
    tpd(i,r) # comm.-, source-spec. shift in tax on private cons. of dom. #;
Equation TPDSHIFT
# permits uniform consumption tax change #
(all,i,TRAD_COMM)(all,r,REG)
    atpd(i,r) = tpd(i,r) + tp(r);

Zerodivide (zero_by_zero) default 1;
Coefficient (all,i,TRAD_COMM)(all,r,REG)
    SHVDPANC(i,r)
    # share of carbon-tax-excl. value of priv. consumption of dom. product #;
Formula (all,i,TRAD_COMM)(all,r,REG)
    SHVDPANC(i,r) = VDPANC(i,r) / VDPA(i,r);
Zerodivide (zero_by_zero) off;

Zerodivide (zero_by_zero) default 0;
Coefficient (all,i,TRAD_COMM)(all,r,REG)
    CO2DPVDPA(i,r)
    # emissions intensity of private consumption of domestic product #;
Formula (all,i,TRAD_COMM)(all,r,REG)
    CO2DPVDPA(i,r) = CO2DP(i,r) / VDPA(i,r);
Zerodivide (zero_by_zero) off;

Equation PHHDPRICE
# taxation of private consumption of domestic product #
(all,i,TRAD_COMM)(all,r,REG)
    ppd(i,r) = SHVDPANC(i,r) * (pm(i,r) + atpd(i,r))
        + 100.0 * CO2DPVDPA(i,r) * NCTAXB(REGTOBLOC(r));

Variable (all,i,TRAD_COMM)(all,r,REG)
    tpm(i,r) # comm.-, source-spec. shift in tax on private cons. of imp. #;
Equation TPMSHIFT
# permits uniform consumption tax change #
(all,i,TRAD_COMM)(all,r,REG)
    atpm(i,r) = tpm(i,r) + tp(r);

Zerodivide (zero_by_zero) default 1;
Coefficient (all,i,TRAD_COMM)(all,r,REG)
    SHVIPANC(i,r)
    # share of carbon-tax-excl. value of priv. consumption of imports #;
Formula (all,i,TRAD_COMM)(all,r,REG)
    SHVIPANC(i,r) = VIPANC(i,r) / VIPA(i,r);
Zerodivide (zero_by_zero) off;

Zerodivide (zero_by_zero) default 0;
Coefficient (all,i,TRAD_COMM)(all,r,REG)
    CO2IPVIPA(i,r)
    # emissions intensity of private consumption of imports #;
Formula (all,i,TRAD_COMM)(all,r,REG)
    CO2IPVIPA(i,r) = CO2IP(i,r) / VIPA(i,r);
Zerodivide (zero_by_zero) off;

Equation PHHIPRICES
# taxation of private consumption of imports #
(all,i,TRAD_COMM)(all,r,REG)
    ppm(i,r) = SHVIPANC(i,r) * (pim(i,r) + atpm(i,r))
        + 100.0 * CO2IPVIPA(i,r) * NCTAXB(REGTOBLOC(r));

Equation TPCRATIO
# change in ratio of private consumption tax payments to regional income #
(all,r,REG)
    100.0 * INCOME(r) * del_taxrpc(r) + TPC(r) * y(r)
        = sum(i,TRAD_COMM,
            VDPANC(i,r) * atpd(i,r) + DPTAX(i,r) * [pm(i,r) + qpd(i,r)])
        + sum(i,TRAD_COMM,
            VIPANC(i,r) * atpm(i,r) + IPTAX(i,r) * [pim(i,r) + qpm(i,r)]);

Coefficient (all,i,TRAD_COMM)(all,s,REG)
    PMSHR(i,s) # share of imports for priv hhld at agent's prices #;
Formula (all,i,TRAD_COMM)(all,s,REG)
    PMSHR(i,s) = VIPA(i,s) / VPAT(i,s);

Equation PCOMPRICE
# private consumption price for composite commodities (HT 47) #
(all,i,TRAD_COMM)(all,s,REG)
    ppt(i,s) = PMSHR(i,s) * ppm(i,s) + [1 - PMSHR(i,s)] * ppd(i,s);

Equation PHHLDDOM
# private consumption demand for domestic goods (HT 48) #
(all,i,TRAD_COMM)(all,s,REG)
    qpd(i,s) = qpt(i,s) + ESUBD(i) * [ppt(i,s) - ppd(i,s)];

Equation PHHLDAGRIMP
# private consumption demand for aggregate imports (HT 49) #
(all,i,TRAD_COMM)(all,s,REG)
    qpm(i,s) = qpt(i,s) + ESUBD(i) * [ppt(i,s) - ppm(i,s)];


!<
    2-2. Notational Links
    ---------------------
>!

Formula (all,i,TRAD_COMM)(all,r,REG)
    VPA(i,r) = VPAT(i,r);

Equation PPTEQ # prices into the private consumption system #
(all,i,TRAD_COMM)(all,r,REG)
    ppt(i,r) = pp(i,r) ;

Equation QPTEQ # quantities from the private consumption system #
(all,i,TRAD_COMM)(all,r,REG)
    qpt(i,r) = qp(i,r) ;


!<
    2-3. Energy Nest
    ----------------
>!

Set
    ENY_PCOMM #inputs into private energy subutility#
    (coal, oil, gas, oil_pcts, electricity);
Subset
    ENY_PCOMM is subset of TRAD_COMM;

Formula (all,r,REG)
    VPAS("eny",r) = sum(i,ENY_PCOMM, VPA(i,r));
Formula (all,r,REG)
    VPA("eny",r) = VPAS("eny",r);

Coefficient
    PSIZE_ENY # size of ENY_PCOMM set #;
Formula
    PSIZE_ENY = sum(i,ENY_PCOMM, 1);

Coefficient (all,i,ENY_PCOMM)(all,r,REG)
    PSHENY(i,r) # share of i in cost of energy subutility #;
Formula (all,i,ENY_PCOMM)(all,r,REG: VPAS("eny",r) = 0)
    PSHENY(i,r) = 1.0 / PSIZE_ENY;
Formula (all,i,ENY_PCOMM)(all,r,REG: VPAS("eny",r) > 0)
    PSHENY(i,r) = VPA(i,r) / VPAS("eny",r);

Equation ENYPPRICE # price of energy subutility #
(all,r,REG)
    pp("eny",r) = sum(k,ENY_PCOMM, PSHENY(k,r) * pp(k,r));

Coefficient (parameter) (all,r,REG)
    ELPENY(r) # elasticity of substitution in energy subconsumption #;
Read
    ELPENY from file GTAPPARM header "EPEN";

Equation ENYPDEMAND # demand for inputs into energy subconsumption #
(all,i,ENY_PCOMM)(all,r,REG)
    qp(i,r) = qp("eny",r) - ELPENY(r) * [pp(i,r) - pp("eny",r)];


!<
    2-4. Top of the Private Consumption Structure
    ---------------------------------------------
>!

Set NENY_PCOMM 
    #inputs into private non-energy subutility# = TRAD_COMM - ENY_PCOMM;

Set ENY # private energy composite # (eny);

Set
    UP_COMM 
    # inputs into top of private consumption structure # = ENY + NENY_PCOMM;
    
Subset
    UP_COMM is subset of PRIV_COMM ;

Coefficient (parameter)(all,i,UP_COMM)(all,r,REG)
    INCPAR(i,r)
    # expansion parameter in the CDE minimum expenditure function #;
Read
    INCPAR from file GTAPPARM header "INCP";

Coefficient (parameter)(all,i,UP_COMM)(all,r,REG)
    SUBPAR(i,r)
    # substitution parameter in CDE minimum expenditure function #;
Read
    SUBPAR from file GTAPPARM header "SUBP";

Coefficient (all,i,UP_COMM)(all,r,REG)
    PSHUP(i,r) # share of private hhld consumption devoted to good i in r #;
Formula (all,i,UP_COMM)(all,r,REG)
    PSHUP(i,r) = VPA(i,r) / sum(k,UP_COMM, VPA(k,r));

Formula (all,r,REG)
    UELASPRIV(r) = sum(i,UP_COMM, PSHUP(i,r) * INCPAR(i,r));

!<
    (HT F1 - F3)
>!
Coefficient (all,i,UP_COMM)(all,r,REG)
    ALPHA(i,r) # 1 - sub. parameter in the CDE minimum expenditure function #;
Formula (all,i,UP_COMM)(all,r,REG)
    ALPHA(i,r) = 1 - SUBPAR(i,r);

Coefficient (all,i,UP_COMM)(all,k,UP_COMM)(all,r,REG)
    APE(i,k,r) # Allen partial elst. of sub. between composite i and k in r #;
Formula (all,i,UP_COMM)(all,k,UP_COMM)(all,r,REG)
    APE(i,k,r)
        = ALPHA(i,r) + ALPHA(k,r) - sum(n,UP_COMM, PSHUP(n,r) * ALPHA(n,r));
Formula (all,i,UP_COMM)(all,r,REG)
    APE(i,i,r)
        = 2.0 * ALPHA(i,r)
        - sum(n,UP_COMM, PSHUP(n,r) * ALPHA(n,r))
        - ALPHA(i,r) / PSHUP(i,r);

Coefficient (all,i,UP_COMM)(all,r,REG)
    EY(i,r) # income elast. of private hhld demand for i in r (HT F4) #;
Formula (all,i,UP_COMM)(all,r,REG)
    EY(i,r)
        =   [1.0 / sum(n,UP_COMM, PSHUP(n,r) * INCPAR(n,r))]
            * [INCPAR(i,r) * [1.0 - ALPHA(i,r)]
                + sum(n,UP_COMM, PSHUP(n,r) * INCPAR(n,r) * ALPHA(n,r))]
        + [ALPHA(i,r) - sum(n,UP_COMM, PSHUP(n,r) * ALPHA(n,r))];

Coefficient (all,i,UP_COMM)(all,k,UP_COMM)(all,r,REG)
    EP(i,k,r)
    # uncomp. elast. of private hhld demand for i wrt price of k in r (HT F5) #;
Formula (all,i,UP_COMM)(all,k,UP_COMM)(all,r,REG)
    EP(i,k,r) = 0;
Formula (all,i,UP_COMM)(all,k,UP_COMM)(all,r,REG)
    EP(i,k,r) = [APE(i,k,r) - EY(i,r)] * PSHUP(k,r);

Equation PRIVDMNDS
# private consumption demands for composite commodities (HT 46) #
(all,i,UP_COMM)(all,r,REG)
    qp(i,r) - pop(r)
        = sum(k,UP_COMM, EP(i,k,r) * pp(k,r)) + EY(i,r) * [yp(r) - pop(r)];
!<
    Private consumption demands for composite commodities.  Demand system is
    on a per capita basis.  Here, yp(r) - pop(r) is % change in per capita
    income.  (HT 46)
>!

Equation PHHLDINDEX
# price index for private consumption expenditure #
(all,r,REG)
    ppriv(r) = sum(i,UP_COMM, PSHUP(i,r) * pp(i,r));

Equation PRIVATEU
# computation of utility from private consumption in r (HT 45) #
(all,r,REG)
    yp(r) - pop(r) = ppriv(r) + UELASPRIV(r) * up(r);
!<
    This equation determines private consumption utility for a representative
    household in region r, based on the per capita private expenditure
    function.  (HT 45)
>!

Coefficient (all,i,UP_COMM)(all,r,REG)
    XWCONSHR(i,r)
    # expansion-parameter-weighted consumption share #;
Formula (all,i,UP_COMM)(all,r,REG)
    XWCONSHR(i,r) = PSHUP(i,r) * INCPAR(i,r) / UELASPRIV(r);

Equation UTILELASPRIV
# elasticity of expenditure wrt utility from private consumption #
(all,r,REG)
    uepriv(r)
        = sum(i,UP_COMM, XWCONSHR(i,r) * [pp(i,r) + qp(i,r) - yp(r)]);


!<
    --------
    3. Firms
    --------

    To implement the production system, we define a set "SUBF_COMM" of
    subproducts, corresponding to the various composites of TP-16, the
    value-added-energy composite, the capital-energy composite, and so on
    (we call them "subproducts" not "composites" to avoid confusion with the
    domestic-import and import-import composites).  We include subproducts
    with endowments and tradables in a set "FIRM_COMM" of commodities
    demanded by firms.


    Set structure
    -------------

    |--------------------------------------------------|
    |                    FIRM_COMM                     |
    |--------------------------------------------------|
    |              DEMD_COMM               | SUBF_COMM |
    |--------------------------------------|-----------|
    | ENDW_COMM |       TRAD_COMM          |
    |---------------------------------------


    Production structure
    --------------------

                                     qo(j,r)  [ao(j,r)]     TOTAL OUTPUT NEST
                                     /\
                                    /  \  <----- CES
                                   /    \
                                  /      \
                                 /        \
                    endowment-energy       non-energy inputs
Val.-add.-en. nest    /| \                                 /\
    CES ----->       / |  \                               /  \  <------ CES
                    /  |   \                             /    \
                   /   |    \                           /      \
                  /    |     \                         /        \
               Land  Labor  K-E subproduct         Domestic     Foreign
                              /\
                             /  \
                            /    \                  Capital-energy nest
                           /      \     <--------- CES (subst. el. ELFKEN)
                          /        \
                         /          \
                        /            \
                       /              \
                      K             energy subproduct
                                                /\         Energy nest
                                               /  \ <--- CES (subst. el. ELFENY)
                                              /    \
                                             /      \
                                            /        \
                                           /          \
                                      electricity  non-electr. energy
                                                        /\
    Non-electricity energy nest                        /  \
    CES (subst. el. ELFNELY)  ---------------->       /    \
                                                     /      \
                                                    /        \
                                           non-coal energy   coal
                                                /\
    Non-coal energy nest     ------------>     /  \
    CES (subst. el. ELFNCOAL)                 /    \
                                             /      \
                                            /        \
                                           /          \
                                 crude oil, gas, petr. products



