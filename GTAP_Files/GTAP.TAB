!<
    -------------------------------
    D. Terms of Trade Decomposition
    -------------------------------

    Computations for decomposition the terms-of-trade effect
    Reference: Robert McDougall, SALTER No. 12 Working Paper
>!

Coefficient (all,i,TRAD_COMM)(all,r,REG)
    SX_IR(i,r) # share of good i in total exports from r #;
Formula (all,m,MARG_COMM)(all,r,REG)
    SX_IR(m,r)
        = [sum(s,REG, VXWD(m,r,s)) + VST(m,r)]
        / [sum(k,TRAD_COMM, sum(s,REG, VXWD(k,r,s)))
            + sum(l,MARG_COMM, VST(l,r))];
Formula (all,i,NMRG_COMM)(all,r,REG)
    SX_IR(i,r)
        = sum(s,REG, VXWD(i,r,s))
        / [sum(k,TRAD_COMM, sum(s,REG, VXWD(k,r,s)))
            + sum(l,MARG_COMM, VST(l,r))];

Coefficient (all,m,MARG_COMM)(all,s,REG)
    VTICOMM(m,s) # margin usage of m in getting imports to region s #;
Formula (all,m,MARG_COMM)(all,s,REG)
    VTICOMM(m,s) = sum(i,TRAD_COMM, sum(r,REG, VTMFSD(m,i,r,s)));

Coefficient (all,i,TRAD_COMM)(all,s,REG)
    VIWDIRALL(i,s) # imports of i into s, with direct allocation of margins #;
Formula (all,m,MARG_COMM)(all,s,REG)
    VIWDIRALL(m,s) = sum(r,REG, VXWD(m,r,s)) + VTICOMM(m,s);
Formula (all,i,NMRG_COMM)(all,s,REG)
    VIWDIRALL(i,s) = sum(r,REG, VXWD(i,r,s));

Coefficient (all,s,REG)
    VIWDATOT(s) # total imports into s, calculated on direct allocation basis #;
Formula (all,s,REG)
    VIWDATOT(s) = sum(i,TRAD_COMM, VIWDIRALL(i,s));

Coefficient (all,i,TRAD_COMM)(all,r,REG)
    SM_IR(i,r)  # share of good i in total imports into r #;
Formula (all,i,TRAD_COMM)(all,s,REG)
    SM_IR(i,s) = VIWDIRALL(i,s) / VIWDATOT(s);

Zerodivide (zero_by_zero) default 0;
Coefficient (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
    SX_IRS(i,r,s) # share of exports of good i from region r to s #;
Formula (all,m,MARG_COMM)(all,r,REG)(all,s,REG)
    SX_IRS(m,r,s) = VXWD(m,r,s) / [sum(k,REG, VXWD(m,r,k)) + VST(m,r)];
Formula (all,i,NMRG_COMM)(all,r,REG)(all,s,REG)
    SX_IRS(i,r,s) = VXWD(i,r,s) / sum(k,REG, VXWD(i,r,k));
Zerodivide (zero_by_zero) off;

Coefficient (all,m,MARG_COMM)(all,r,REG)
    SXT_IR(m,r) # share of margins in exports of good i from region r #;
Formula (all,m,MARG_COMM)(all,r,REG)
    SXT_IR(m,r) = VST(m,r) / [sum(k,REG, VXWD(m,r,k)) + VST(m,r)];

Variable (all,i,TRAD_COMM)(all,r,REG)
    px_ir(i,r) # export price index for good i and region r #;
Equation EXPPRICE_MARG
# price index for total exports of m from r #
(all,m,MARG_COMM)(all,r,REG)
    px_ir(m,r)
        = sum(s,REG, SX_IRS(m,r,s) * pfob(m,r,s)) + SXT_IR(m,r) * pm(m,r);
Equation EXPPRICE_NMRG
# price index for total exports of i from r #
(all,i,NMRG_COMM)(all,r,REG)
    px_ir(i,r) = sum(s,REG, SX_IRS(i,r,s) * pfob(i,r,s));

Coefficient (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
    SM_IRS(i,r,s) # share of imports of good i into s from r at FOB prices #;
Formula (all,i,TRAD_COMM)(all,r,REG)(all,s,REG)
    SM_IRS(i,r,s) = VXWD(i,r,s) / VIWDIRALL(i,s);

Coefficient (all,m,MARG_COMM)(all,r,REG)
    SMT_IR(m,r) # share of transport cost in imports of margin commodity #;
Formula (all,m,MARG_COMM)(all,s,REG)
    SMT_IR(m,s) = VTICOMM(m,s) / VIWDIRALL(m,s);

Variable (all,i,TRAD_COMM)(all,r,REG)
    pm_ir(i,r) # imports price index for good i and region r #;
Equation IMPPRICE1_MARG
# price index for total imports of m in s -- margins commodities #
(all,m,MARG_COMM)(all,s,REG)
    pm_ir(m,s)
        = sum(r,REG, SM_IRS(m,r,s) * pfob(m,r,s)) + SMT_IR(m,s) * pt(m);
Equation IMPPRICE1_NMRG
# price index for total imports of i in s -- non-margins commodities #
(all,i,NMRG_COMM)(all,s,REG)
    pm_ir(i,s) = sum(r,REG, SM_IRS(i,r,s) * pfob(i,r,s));

Coefficient (all,i,TRAD_COMM)(all,r,REG)
    SW_IR(i,r) # share of region r exports in world total for good i #;
Formula (all,m,MARG_COMM)(all,r,REG)
    SW_IR(m,r)
        = [sum(s,REG, VXWD(m,r,s)) + VST(m,r)]
        / sum(k,REG, sum(s,REG, VXWD(m,k,s)) + VST(m,k));
Formula (all,i,NMRG_COMM)(all,r,REG)
    SW_IR(i,r)
        = sum(s,REG, VXWD(i,r,s)) / sum(k,REG, sum(s,REG, VXWD(i,k,s)));

Variable (all,i,TRAD_COMM)
    px_i(i) # world export price index for commodity i #;
Equation WRLDPRICEi
# world export price index for good i #
(all,i,TRAD_COMM)
    px_i(i) = sum(r,REG, SW_IR(i,r) * px_ir(i,r));

Coefficient (all,i,TRAD_COMM)
    SW_I(i) # share of exports of i in world total #;
Formula (all,m,MARG_COMM)
    SW_I(m)
        = sum(r,REG, sum(s,REG, VXWD(m,r,s)) + VST(m,r))
        / [sum(k,TRAD_COMM, sum(r,REG, sum(s,REG, VXWD(k,r,s))))
            + sum(l,MARG_COMM, sum(r,REG, VST(l,r)))];
Formula (all,i,NMRG_COMM)
    SW_I(i)
        = sum(r,REG, sum(s,REG, VXWD(i,r,s)))
        / [sum(k,TRAD_COMM, sum(r,REG, sum(s,REG, VXWD(k,r,s))))
            + sum(l,MARG_COMM, sum(r,REG, VST(l,r)))];

Variable
    px_ # world export price index for all commodities #;
Equation WRLDPRICE
# world export price index for all goods #
    px_ = sum(i,TRAD_COMM, SW_I(i) * px_i(i));

Variable (all,i,TRAD_COMM)(all,r,REG)
    c1_ir(i,r) # contribution of world price, px_i, to ToT #;
Equation c1_irEQ
# contribution of world export price index of good i to ToT for region r #
(all,i,TRAD_COMM)(all,r,REG)
    c1_ir(i,r) = [SX_IR(i,r) - SM_IR(i,r)] * [px_i(i) - px_];

Variable (all,i,TRAD_COMM)(all,r,REG)
    c2_ir(i,r) # contribution of regional export price, px_ir, to ToT #;
Equation c2_irEQ
# contribution of regional export price of good i for region r #
(all,i,TRAD_COMM)(all,r,REG)
    c2_ir(i,r) = SX_IR(i,r) * [px_ir(i,r) - px_i(i)];

Variable (all,i,TRAD_COMM)(all,r,REG)
    c3_ir(i,r) # contribution of regional import price, pm_ir, to ToT #;
Equation c3_irEQ
# contribution of imports price index of good i for region r #
(all,i,TRAD_COMM)(all,r,REG)
    c3_ir(i,r) = SM_IR(i,r) * [pm_ir(i,r) - px_i(i)];

Variable (all,r,REG)
    c1_r(r) # contribution of world prices for all goods to ToT #;
Equation c1_rEQ
# contribution of world price indexes of all goods to ToT for r #
(all,r,REG)
    c1_r(r) = sum(i,TRAD_COMM, c1_ir(i,r));

Variable (all,r,REG)
    c2_r(r) # contribution of regional export prices to ToT #;
Equation c2_rEQ
# contribution of regional exports prices to ToT for r #
(all,r,REG)
    c2_r(r) = sum(i,TRAD_COMM, c2_ir(i,r));

Variable (all,r,REG)
    c3_r(r) # contribution of regional import prices to ToT #;
Equation c3_rEQ
# contribution of regional import prices to ToT for r #
(all,r,REG)
    c3_r(r) = sum(i,TRAD_COMM, c3_ir(i,r));

Variable (all,r,REG)
    tot2(r) # trade terms for region r, computed from components #;
Equation TOT2eq
# trade terms for region r, computed from components #
(all,r,REG)
    tot2(r) = c1_r(r) + c2_r(r) - c3_r(r);

!<
    ====================
    Code to allow for a policy induced reduction in the usage of a particular
    input by a particular industry, either in absolute quantity or relative intensity
    added by Andre Barbe April 1, 2015
    ====================
>!

!The basic idea is to 1. calculate the shadow input price which would give the
specified reduction in quantity used of the restricted input (note that this
requires calculating the price of the output induced by these shadow input prices)
2. use those shadow prices to calculate the actual quantities used of each input.
3. Use those actual input quantities to calculate actual output prices!

Variable (all,i,FIRM_COMM)(all,j,PROD_COMM)(all,r,REG)
    pf_s(i,j,r) #shadow (after quantity restriction) price  for commodity i for use by j in r #;

Variable (all,i,SUBF_COMM)(all,j,PROD_COMM)(all,r,REG)
    pf_so(i,j,r) #shadow price  for ouptut commodity i for use by j in r whose inputs have a quantity restrition#;

Variable (all,i,FIRM_COMM)(all,j,PROD_COMM)(all,r,REG)
    pf_slack(i,j,r) # firm's slack variable for commodity i for use by j in r.#;
    !ps_slack is non-zero if there is a quantity restriction for
    this particular ijr.!


Equation pf_sBINDING
# relates the shadow and real price of commodities i for use by j in r #
(all,i,FIRM_COMM)(all,j,PROD_COMM)(all,r,REG)
    pf_s(i,j,r)=pf(i,j,r) + pf_slack(i,j,r);
    !note that while a slack variable technically exists for all FIRM_COMM,
    it should only be endogenized for inputs of the 5 outputs shown below.
    If other inputs are endogenized, nothing will happen, as those pf_s don't
    show up in any other equations, only the pf do !

!Useful sets!
!These sets exists to make defining the pf_slack closure more compact!
!because I need to explicitly list each set that is exogeneous!

Set
    NEP_COMM # non-energy producers # = PROD_COMM - EGY_COMM;
Subset
    NEP_COMM is subset of PROD_COMM;
Set
    NEF_COMM # non-energy firm commodities # = FIRM_COMM - EGY_COMM;
Subset
    NEF_COMM is subset of FIRM_COMM;

Set
    US_REG # Regions that are the US # (USA);
Subset
    US_REG is subset of REG;
Set
    NUS_REG # Regions that are not the US # = REG - US_REG;
Subset
    NUS_REG is subset of REG;

Set
    INSUBF_COMM # set of all inputs used in producing the SUBF commodities # = 
        NCOAL_FCOMM + NELY_FCOMM + ENY_FCOMM + KEN_FCOMM + VAEN_FCOMM;
Subset
    INSUBF_COMM is subset of FIRM_COMM;



!Quantity Demand Equations!
!I commented out the old qf equations for the inputs and added the updated
ones below !
!They are updated by replacing the input prices with the input shadow prices
and the output prices with the output shadow prices!
Equation NCOALFDEMAND
# demand for inputs into non-coal energy subproduction #
(all,i,NCOAL_FCOMM)(all,j,PROD_COMM)(all,r,REG)
    qf(i,j,r) = -af(i,j,r) + qf("ncoal",j,r)
        - ELFNCOAL(j,r) * [pf_s(i,j,r) - af(i,j,r) - pf_so("ncoal",j,r)];

Equation NELYFDEMAND
# demand for inputs into non-electricity energy subproduction #
(all,i,NELY_FCOMM)(all,j,PROD_COMM)(all,r,REG)
    qf(i,j,r) = -af(i,j,r) + qf("nely",j,r)
        - ELFNELY(j,r) * [pf_s(i,j,r) - af(i,j,r) - pf_so("nely",j,r)];

Equation ENYFDEMAND # demand for inputs into energy subproduction #
(all,i,ENY_FCOMM)(all,j,PROD_COMM)(all,r,REG)
    qf(i,j,r) = -af(i,j,r) + qf("eny",j,r)
        - ELFENY(j,r) * [pf_s(i,j,r) - af(i,j,r) - pf_so("eny",j,r)];

Equation KENFDEMAND # demand for inputs into capital-energy subproduction #
(all,i,KEN_FCOMM)(all,j,PROD_COMM)(all,r,REG)
    qf(i,j,r)= -af(i,j,r) + qf("ken",j,r)
        - ELFKEN(j,r) * [pf_s(i,j,r) - af(i,j,r) - pf_so("ken",j,r)];

Equation VAENFDEMAND
# demand for inputs into value-added-energy subproduction #
(all,i,VAEN_FCOMM)(all,j,PROD_COMM)(all,r,REG)
    qf(i,j,r) = -af(i,j,r) + qf("vaen",j,r)
        - ELFVAEN(j,r) * [pf_s(i,j,r) - af(i,j,r) - pf_so("vaen",j,r)];
!Replacement of commented out equations ends here!

!Intensity or Relative Quantity Demand Equations!

Variable (all,i,INSUBF_COMM)(all,j,PROD_COMM)(all,r,REG)
    intf(i,j,r) # intensity of usage of input commodity i for use by j in r.#;

Equation NCOALFINTENS
# demand for inputs into non-coal energy subproduction divided by output#
(all,i,NCOAL_FCOMM)(all,j,PROD_COMM)(all,r,REG)
    intf(i,j,r) = qf(i,j,r) - qf("ncoal",j,r);

Equation NELYFINTENS
# demand for inputs into non-electricity energy subproduction divided by output#
(all,i,NELY_FCOMM)(all,j,PROD_COMM)(all,r,REG)
    intf(i,j,r) = qf(i,j,r) - qf("nely",j,r);

Equation ENYFINTENS # demand for inputs into energy subproduction divided by output#
(all,i,ENY_FCOMM)(all,j,PROD_COMM)(all,r,REG)
    intf(i,j,r) = qf(i,j,r) - qf("eny",j,r);

Equation KENFINTENS # demand for inputs into capital-energy subproduction divided by output#
(all,i,KEN_FCOMM)(all,j,PROD_COMM)(all,r,REG)
    intf(i,j,r) = qf(i,j,r) - qf("ken",j,r);

Equation VAENFINTENS
# demand for inputs into value-added-energy subproduction divided by output#
(all,i,VAEN_FCOMM)(all,j,PROD_COMM)(all,r,REG)
    intf(i,j,r) = qf(i,j,r) - qf("vaen",j,r);
!Intensity of Demand Related equations ends here!

!Shadow Cost Share Related Equations!
Coefficient (all,i,FIRM_COMM)(all,j,PROD_COMM)(all,r,REG)
    VFA_S(i,j,r) # producer expend. on input i by j in r valued at agent's shadow prices #;

Formula (initial) (all,i,FIRM_COMM)(all,j,PROD_COMM)(all,r,REG)
    VFA_S(i,j,r) = VFA(i,j,r);
!might have to edit this formula!
!I think this forumla is correct as long as there is no levels variable for 
PF_SLACK. If there is AND it is set such that there is a quantity restriction 
extant in the initial state (such that initial PF_S is not equal to initial PF),
this formula would not be correct. But since I'm not doing that, this formula
should be fine!

Update (all,i,FIRM_COMM)(all,j,PROD_COMM)(all,r,REG)
    VFA_S(i,j,r) = pf_s(i,j,r) * qf(i,j,r);

Coefficient (all,i,SUBF_COMM)(all,j,PROD_COMM)(all,r,REG)
    VFAS_S(i,j,r) # producer expend. on output i by j in r due to agent's shadow prices of inputs #;



!Shadow cost shares of each input!
!Output NCOAL Share!
Formula (all,j,PROD_COMM)(all,r,REG)
    VFAS_S("ncoal",j,r) = sum(i,NCOAL_FCOMM, VFA_S(i,j,r));
Coefficient (all,i,NCOAL_FCOMM)(all,j,PROD_COMM)(all,r,REG)
    FSHNCOAL_S(i,j,r)
    # shadow share of i in cost to j of non-coal energy subproduct #;
Formula (all,i,NCOAL_FCOMM)(all,j,PROD_COMM)(all,r,REG: VFAS_S("ncoal",j,r) = 0)
    FSHNCOAL_S(i,j,r) = 1.0 / FSIZE_NCOAL;
Formula (all,i,NCOAL_FCOMM)(all,j,PROD_COMM)(all,r,REG: VFAS_S("ncoal",j,r) > 0)
    FSHNCOAL_S(i,j,r) = VFA_S(i,j,r) / VFAS_S("ncoal",j,r);


!Output NELY Share!
Formula (all,j,PROD_COMM)(all,r,REG)
    VFAS_S("nely",j,r) = sum(i,NELY_FCOMM, VFA_S(i,j,r));
Coefficient (all,i,NELY_FCOMM)(all,j,PROD_COMM)(all,r,REG)
    FSHNELY_S(i,j,r)
    # share of i in cost to j of non-electricity energy subproduct #;
Formula (all,i,NELY_FCOMM)(all,j,PROD_COMM)(all,r,REG: VFAS_S("nely",j,r) = 0)
    FSHNELY_S(i,j,r) = 1.0 / FSIZE_NELY;
Formula (all,i,NELY_FCOMM)(all,j,PROD_COMM)(all,r,REG: VFAS_S("nely",j,r) > 0)
    FSHNELY_S(i,j,r) = VFA_S(i,j,r) / VFAS_S("nely",j,r);


!Output ENY Share!
Formula (all,j,PROD_COMM)(all,r,REG)
    VFAS_S("eny",j,r) = sum(i,ENY_FCOMM, VFA_S(i,j,r));
Coefficient (all,i,ENY_FCOMM)(all,j,PROD_COMM)(all,r,REG)
    FSHENY_S(i,j,r) # share of i in cost to j of energy subproduct #;
Formula (all,i,ENY_FCOMM)(all,j,PROD_COMM)(all,r,REG: VFAS_S("eny",j,r) = 0)
    FSHENY_S(i,j,r) = 1.0 / FSIZE_ENY;
Formula (all,i,ENY_FCOMM)(all,j,PROD_COMM)(all,r,REG: VFAS_S("eny",j,r) > 0)
    FSHENY_S(i,j,r) = VFA_S(i,j,r) / VFAS_S("eny",j,r);

!Output KEN Share!
Formula (all,j,PROD_COMM)(all,r,REG)
    VFAS_S("ken",j,r) = sum(i,KEN_FCOMM, VFA_S(i,j,r));
Coefficient (all,i,KEN_FCOMM)(all,j,PROD_COMM)(all,r,REG)
    FSHKEN_S(i,j,r) # share of i in cost to j of capital-energy subproduct #;
Formula (all,i,KEN_FCOMM)(all,j,PROD_COMM)(all,r,REG: VFAS_S("ken",j,r) = 0)
    FSHKEN_S(i,j,r) = 1.0 / FSIZE_KEN;
Formula (all,i,KEN_FCOMM)(all,j,PROD_COMM)(all,r,REG: VFAS_S("ken",j,r) > 0)
    FSHKEN_S(i,j,r) = VFA_S(i,j,r) / VFAS_S("ken",j,r);


!Output VAEN Share!
Formula (all,j,PROD_COMM)(all,r,REG)
    VFAS_S("vaen",j,r) = sum(i,VAEN_FCOMM, VFA_S(i,j,r));
Coefficient (all,i,VAEN_FCOMM)(all,j,PROD_COMM)(all,r,REG)
    FSHVAEN_S(i,j,r)
    # share of i in cost to j of value-added-energy subproduct #;
Formula (all,i,VAEN_FCOMM)(all,j,PROD_COMM)(all,r,REG: VFAS_S("vaen",j,r) = 0)
    FSHVAEN_S(i,j,r) = 1.0 / FSIZE_VAEN;
Formula (all,i,VAEN_FCOMM)(all,j,PROD_COMM)(all,r,REG: VFAS_S("vaen",j,r) > 0)
    FSHVAEN_S(i,j,r) = VFA_S(i,j,r) / VFAS_S("vaen",j,r);

!Output shadow price equations!
!Note that the "output" shadow price is different from the "input" shadow price!
!The input shadow price is the shadow price of a restricted input, the output
shadow price (or cost) is the price the input restriction induces in the output!
!This distinction is important if there is are multiple quantity restrictions,
e.g. a good that has restrictions on both the inputs used to make it and its use
as an input to make other goods. The different restrictions at different levels
of the production chain won't create the same shadow prices for a good when it
is an input and facing one set of restrictions as when it is an output and
is affected by by another set of restrictions!
Equation NCOALFPRICE_S # output shadow price of non-coal energy output from input restrictions #
(all,j,PROD_COMM)(all,r,REG)
    pf_so("ncoal",j,r)
        = sum(k,NCOAL_FCOMM, FSHNCOAL_S(k,j,r) * [pf_s(k,j,r) - af(k,j,r)]);

Equation NELYFPRICE_S # output shadow price of non-electricity energy subproduct output from input restrictions #
(all,j,PROD_COMM)(all,r,REG)
    pf_so("nely",j,r) =
        sum(k,NELY_FCOMM, FSHNELY_S(k,j,r) * [pf_s(k,j,r) - af(k,j,r)]);

Equation ENYFPRICE_S # output shadow price of energy subproduct output from input restrictions #
(all,j,PROD_COMM)(all,r,REG)
    pf_so("eny",j,r) = sum(k,ENY_FCOMM, FSHENY_S(k,j,r) * [pf_s(k,j,r) - af(k,j,r)]);

Equation KENFPRICE_S # output shadow price of capital-energy subproduct output from input restrictions #
(all,j,PROD_COMM)(all,r,REG)
    pf_so("ken",j,r)
        = sum(k,KEN_FCOMM, FSHKEN_S(k,j,r) * [pf_s(k,j,r) - af(k,j,r)]);

Equation VAENFPRICE_S # output shadow price of value-added-energy subproduct output from input restrictions #
(all,j,PROD_COMM)(all,r,REG)
    pf_so("vaen",j,r)
        = sum(k,VAEN_FCOMM, FSHVAEN_S(k,j,r) * [pf_s(k,j,r) - af(k,j,r)]);



!Output Price Equations!
!I commented out the old pf equations for the outputs and added the updated
ones below !
!They are updated to allow for changes in output price due to non-optimal input
bundles. The exact formula is based on my own derivation!

Equation NCOALFPRICE # price of non-coal energy #
(all,j,PROD_COMM)(all,r,REG)
    pf("ncoal",j,r)
        = sum(k,NCOAL_FCOMM, FSHNCOAL(k,j,r) * [qf(k,j,r) + pf(k,j,r)])
        - qf("ncoal",j,r);

!I'm not sure about the term "ELFNCOAL(j,r) * af(i,j,r)" because af(i,j,r) is in the original equation but I don't quite understand why!
!Or rather, I understand why af(i,j,r) is in the pf equation but not why its multiplied by the elasticity in the qf equation!

Equation NELYFPRICE # price of non-electricity energy subproduct #
(all,j,PROD_COMM)(all,r,REG)
    pf("nely",j,r) =
        sum(k,NELY_FCOMM, FSHNELY(k,j,r) * [qf(k,j,r) + pf(k,j,r)])
        - qf("nely",j,r);

Equation ENYFPRICE # price of energy subproduct #
(all,j,PROD_COMM)(all,r,REG)
    pf("eny",j,r) = sum(k,ENY_FCOMM, FSHENY(k,j,r) * [qf(k,j,r) + pf(k,j,r)])
    - qf("eny",j,r);

Equation KENFPRICE # price of capital-energy subproduct #
(all,j,PROD_COMM)(all,r,REG)
    pf("ken",j,r)
        = sum(k,KEN_FCOMM, FSHKEN(k,j,r) * [qf(k,j,r) + pf(k,j,r)])
        - qf("ken",j,r);

Equation VAENFPRICE # price of value-added-energy subproduct #
(all,j,PROD_COMM)(all,r,REG)
    pf("vaen",j,r)
        = sum(k,VAEN_FCOMM, FSHVAEN(k,j,r) * [qf(k,j,r) + pf(k,j,r)])
        - qf("vaen",j,r);
!Replacement of commented out equations ends here!


